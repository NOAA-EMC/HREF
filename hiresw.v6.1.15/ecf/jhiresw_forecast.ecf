%include <head.h>
%include <envir-xc40.h>

set -x

export job=hiresw_%MODEL%_%DOMAIN%_forecast_%CYC%
export cyc=%CYC%
export NEST="%DOMAIN%"
export MODEL="%MODEL%"
export MEMBER="ctl"

export MP_EUILIB=us
export OMP_NUM_THREADS=1
export MP_MPILIB=mpich2
export NTASK=%NTASK%
export PTILE=%PTILE%

VERSION_FILE=$NWROOT/versions/hiresw.ver
if [ -f $VERSION_FILE ]; then
  . $VERSION_FILE
else
  ecflow_client --msg="***JOB ${ECFNAME} ERROR: Version File $VERSION_FILE does not exist"
  ecflow_client --abort
  exit
fi

$NWROOT/hiresw.${hiresw_ver}/jobs/JHIRESW_FORECAST

