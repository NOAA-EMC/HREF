set -x

#!/bin/sh

make clean
cp parmeta.emfull parmeta
make
mv hiresw_post hiresw_em_post_full

make clean
cp parmeta.emsmall parmeta
make
mv hiresw_post hiresw_em_post_small

make clean
cp parmeta.nmmfull parmeta
make
mv hiresw_post hiresw_nmm_post_full

make clean
cp parmeta.nmmsmall parmeta
make
mv hiresw_post hiresw_nmm_post_small

