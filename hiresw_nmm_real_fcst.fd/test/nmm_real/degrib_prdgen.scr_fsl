#! /bin/ksh
# @ step_name = wrfsi
# @ output = /emc2/wx20py/test.out
# @ error = /emc2/wx20py/test.out
# @ notification = never
# @ group=devonprod
# @ arguments = fslrun_native.grd
# @ wall_clock_limit = 00:25:00
# @ job_type = serial
# @ class = dev
# @ queue

. ~/.profile

outfile=$1


hrs="00 06 12 18 24 30 36 42 48"

indir=/gpfstmp/wx20py/output_from_fsl

workdir=/ptmp/wx20py/

cd $workdir

cp /emc2/wx20py/wrf_nmm/WRFV1/test/nmm_real/grdnav.tbl .

rm $outfile

for hr in $hrs

do


# rm WRFPRS${hr}.tm00
# cp $indir/meso.AWPREG${hr}.tm00  WRFPRS${hr}.tm00

cp $indir/WRFPRS${hr}.tm00 .

nagrib << endgrib
 GBFILE   = WRFPRS${hr}.tm00
 INDXFL   =
 GDOUTF   = $outfile
 PROJ     =
 GRDAREA  =
 KXKY     =
 MAXGRD   = 4999
 CPYFIL   = #255
 GAREA    = dset
 OUTPUT   = T
 GBTBLS   = 
 GBDIAG   =
 PDSEXT   = NO

r

ex

endgrib

done

gpend
