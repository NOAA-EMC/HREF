hrs="00 03 06 09" 

cd /emc2/wx20py/wrf_nmm/WRFV1/test/nmm_real/

workdir=/gpfstmp/wx20py/wrfpost_nmmtest_centnmm_test

cp grdnav.tbl $workdir

cd $workdir

rm $outfile

for hr in $hrs

do

nagrib << endgrib
 GBFILE   = WRFPRS${hr}.tm00
 INDXFL   =
 GDOUTF   = $outfile
 PROJ     =
 GRDAREA  =
 KXKY     =
 MAXGRD   = 4999
 CPYFIL   = #255
 GAREA    = dset
 OUTPUT   = T
 GBTBLS   =
 GBDIAG   =
 PDSEXT   = NO

r

ex

endgrib

done

gpend

mv $1 /ptmp/wx20py/
