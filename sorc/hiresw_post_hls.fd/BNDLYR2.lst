        114 |C     START BNDLYR HERE
        115 |C
        116 |      ALLOCATE (PBINT(IM,JSTA_2L:JEND_2U,NBND+1))
        117 |      ALLOCATE (QSBND(IM,JSTA_2L:JEND_2U,NBND))
        118 |      ALLOCATE (PSUM(IM,JSTA_2L:JEND_2U,NBND))
        119 |      ALLOCATE (PVSUM(IM,JSTA_2L:JEND_2U,NBND))
        120 |      ALLOCATE (NSUM(IM,JSTA_2L:JEND_2U,NBND))
        121 |C
        122 |        CLIMIT =1.0E-20
        123 |C
        124 |C     LOOP OVER HORIZONTAL GRID.  AT EACH MASS POINT COMPUTE
        125 |C     PRESSURE AT THE INTERFACE OF EACH BOUNDARY LAYER.
        126 |C
        127 |!$omp  parallel do
        128 |      DO J=JSTA,JEND
        129 |      DO I=1,IM
        130 |        PBINT(I,J,1)=PDTOP+PD(I,J)+PT
        131 |C       PBINT(I,J,1)=PD(I,J)+PT
        132 |      ENDDO
        133 |      ENDDO
        134 |C
        135 |      DO LBND=2,NBND+1
        136 |!$omp  parallel do
        137 |        DO J=JSTA,JEND
        138 |        DO I=1,IM
        139 |          PBINT(I,J,LBND)=PBINT(I,J,LBND-1)-DPBND
        140 |        ENDDO
        141 |        ENDDO
        142 |      ENDDO
        143 |C
        144 |C     LOOP OVER HORIZONTAL.  AT EACH MASS POINT COMPUTE
        145 |C     MASS WEIGHTED LAYER MEAN P, T, Q, U, V, OMEGA,
        146 |C     WAND PRECIPITABLE WATER IN EACH BOUNDARY LAYER FROM THE SURFACE UP.
        147 |C
        148 |!$omp  parallel do
        149 |!$omp& private(ai,bi,dp,iwl,iwm1,pm,qi,qint,qsat,qw,tmt0,tmt15)
        150 |      DO LBND=1,NBND
        151 |        DO J=JSTA,JEND
        152 |        DO I=1,IM
        153 |          PBND(I,J,LBND)   = D00
        154 |          TBND(I,J,LBND)   = D00
        155 |          QBND(I,J,LBND)   = D00
        156 |          QSBND(I,J,LBND)  = D00
        157 |          RHBND(I,J,LBND)  = D00
        158 |          UBND(I,J,LBND)   = D00
        159 |          VBND(I,J,LBND)   = D00
        160 |          OMGBND(I,J,LBND) = D00
        161 |          LVLBND(I,J,LBND) = 0
        162 |          NSUM(I,J,LBND)   = 0
        163 |          PSUM(I,J,LBND)   = D00
        164 |          PVSUM(I,J,LBND)  = D00
        165 |          PWTBND(I,J,LBND) = D00
        166 |        ENDDO
        167 |        ENDDO
        168 |C
        169 |        IWL=0.
        170 |        DO L=1,LM
        171 |          DO J=JSTA,JEND
        172 |          DO I=1,IM
        173 |C
        174 |C   COMPUTE IW
        175 |C
        176 |           IF (L.LE.LMH(I,J)) THEN
        177 |            IWM1=IWL(I,J)
        178 |            IF(CWM(I,J,L).GT.CLIMIT) THEN
        179 |             IF(T(I,J,L).LT.258.15)THEN
        180 |               IWL(I,J)=1.
        181 |             ELSEIF(T(I,J,L).GE.273.15)THEN
        182 |               IWL(I,J)=0.
        183 |             ELSE
        184 |               IF(IWM1.EQ.1.0)IWL(I,J)=1.
        185 |             ENDIF
        186 |            ELSE
        187 |             IWL(I,J)=0.
        188 |            ENDIF
        189 |           ENDIF
        190 |C
        191 |            PM=D50*(PINT(I,J,L)+PINT(I,J,L+1))
        192 |            IF((PBINT(I,J,LBND).GE.PM).AND.
        193 |     1         (PBINT(I,J,LBND+1).LE.PM)) THEN
        194 |              DP     = PINT(I,J,L+1)-PINT(I,J,L)
        195 |              PSUM(I,J,LBND)  =PSUM(I,J,LBND)+DP
        196 |              NSUM(I,J,LBND)  =NSUM(I,J,LBND)+1
        197 |              LVLBND(I,J,LBND)=LVLBND(I,J,LBND)+L
        198 |              TBND(I,J,LBND)  =TBND(I,J,LBND)+T(I,J,L)*DP
        199 |              QBND(I,J,LBND)  =QBND(I,J,LBND)+Q(I,J,L)*DP
        200 |              OMGBND(I,J,LBND)=OMGBND(I,J,LBND)+OMGA(I,J,L)*DP
        201 |              PWTBND(I,J,LBND)=PWTBND(I,J,LBND)
        202 |     1                        +(Q(I,J,L)+CWM(I,J,L))*DP*GI
        203 |C
        204 |              TMT0=T(I,J,L)-273.16
        205 |              TMT15=AMIN1(TMT0,-15.)
        206 |              AI=0.008855
        207 |              BI=1.
        208 |              IF(TMT0.LT.-20.)THEN
        209 |                AI=0.007225
        210 |                BI=0.9674
        211 |              ENDIF
        212 |              QW=PQ0/PM
        213 |     1          *EXP(A2*(T(I,J,L)-A3)/(T(I,J,L)-A4))
        214 |              QI=QW*(BI+AI*AMIN1(TMT0,0.))
        215 |              QINT=QW*(1.-0.00032*TMT15*(TMT15+15.))
        216 |              IF(TMT0.LT.-15.)THEN
        217 |                  QSAT=QI
        218 |              ELSEIF(TMT0.GE.0.)THEN
        219 |                  QSAT=QINT
        220 |              ELSE
        221 |                IF(IWL(I,J).GT.0.0) THEN
        222 |                  QSAT=QI
        223 |                ELSE
        224 |                  QSAT=QINT
        225 |                ENDIF
        226 |              ENDIF
        227 |CMEB 12/22/98 SWITCH TO RH VS WATER NO MATTER WHAT
        228 |C             DELETE THIS LINE TO SWITCH BACK TO RH VS ICE
        229 |              QSAT=QW
        230 |CMEB 12/22/98 SWITCH TO RH VS WATER NO MATTER WHAT
        231 |              QSBND(I,J,LBND)=QSBND(I,J,LBND)+QSAT*DP
        232 |            ENDIF
        233 |          ENDDO
        234 |          ENDDO
        235 |        ENDDO
        236 |C
        237 |C         NEED TO MAKE SURE THAT PINT HALOS EXIST
        238 |C
        239 |        DO L = 1, LP1
        240 |           CALL EXCH_P(PINT(1,jsta_2l,L))
        241 |        end do
        242 |C
        243 |        DO L=1,LM
        244 |C
        245 |          DO J=JSTA_M,JEND_M
        246 |          DO I=2,IM-1
        247 |            IE=I+IVE(J)
        248 |            IW=I+IVW(J)
        249 |            PV1=0.25*(PINT(IW,J,L)+PINT(IE,J,L)
        250 |     1               +PINT(I,J+1,L)+PINT(I,J-1,L))
        251 |            PV2=0.25*(PINT(IW,J,L+1)+PINT(IE,J,L+1)
        252 |     1               +PINT(I,J+1,L+1)+PINT(I,J-1,L+1))
        253 |            DP=PV2-PV1
        254 |            PMV=0.5*(PV1+PV2)
        255 |            IF((PBINT(IW,J,LBND).GE.PMV).AND.
        256 |     X         (PBINT(IW,J,LBND+1).LE.PMV)) THEN
        257 |              PVSUM(I,J,LBND)=PVSUM(I,J,LBND)+DP
        258 |              UBND(I,J,LBND)=UBND(I,J,LBND)+U(I,J,L)*DP
        259 |              VBND(I,J,LBND)=VBND(I,J,LBND)+V(I,J,L)*DP
        260 |            ENDIF
        261 |C
        262 |          ENDDO
        263 |          ENDDO
        264 |        ENDDO
        265 |      ENDDO
        266 |C
        267 |!$omp  parallel do
        268 |!$omp& private(rpsum)
        269 |      DO LBND=1,NBND
        270 |        DO J=JSTA,JEND
        271 |        DO I=1,IM
        272 |          IF(PSUM(I,J,LBND).NE.0.)THEN
        273 |            RPSUM           = 1./PSUM(I,J,LBND)
        274 |            LVLBND(I,J,LBND)= LVLBND(I,J,LBND)/NSUM(I,J,LBND)
        275 |            PBND(I,J,LBND)  = (PBINT(I,J,LBND)+PBINT(I,J,LBND+1))*0.5
        276 |            TBND(I,J,LBND)  = TBND(I,J,LBND)*RPSUM
        277 |            QBND(I,J,LBND)  = QBND(I,J,LBND)*RPSUM
        278 |            QSBND(I,J,LBND) = QSBND(I,J,LBND)*RPSUM
        279 |            OMGBND(I,J,LBND)= OMGBND(I,J,LBND)*RPSUM
        280 |          ENDIF
        281 |        ENDDO
        282 |        ENDDO
        283 |C
        284 |        DO J=JSTA_M,JEND_M
        285 |        DO I=2,IM-1
        286 |          IF(PVSUM(I,J,LBND).NE.0.)THEN
        287 |            RPVSUM      =1./PVSUM(I,J,LBND)
        288 |            UBND(I,J,LBND)=UBND(I,J,LBND)*RPVSUM
        289 |            VBND(I,J,LBND)=VBND(I,J,LBND)*RPVSUM
        290 |          ENDIF
        291 |        ENDDO
        292 |        ENDDO
        293 |      ENDDO
        294 |C
        295 |C  IF NO ETA MID LAYER PRESSURES FELL WITHIN A BND LYR,
        296 |C   FIND THE CLOSEST LAYER TO THE BND LYR AND ASSIGN THE VALUES THERE
        297 |C
        298 |!$omp  parallel do
        299 |!$omp& private(ai,bi,delp,delpv,dp,ie,iw,l,lv,pm,pmin,pminv,pmv,
        300 |!$omp&         qi,qint,qsat,qw,tmt0,tmt15)
        301 |      DO LBND=1,NBND
        302 |        DO J=JSTA,JEND
        303 |        DO I=1,IM
        304 |          IF(PSUM(I,J,LBND).EQ.0.)THEN
        305 |            L=LM
        306 |            PMIN=9999999.
        307 |            PBND(I,J,LBND)=(PBINT(I,J,LBND)+PBINT(I,J,LBND+1))*0.5
        308 |C
        309 |            DO LL=1,LM
        310 |              PM=D50*(PINT(I,J,LL)+PINT(I,J,LL+1))
        311 |              DELP=ABS(PM-PBND(I,J,LBND))
        312 |              IF(DELP.LT.PMIN)THEN
        313 |                PMIN=DELP
        314 |                L=LL
        315 |              ENDIF
        316 |            ENDDO
        317 |C
        318 |            DP=PINT(I,J,L+1)-PINT(I,J,L)
        319 |            PM=D50*(PINT(I,J,L)+PINT(I,J,L+1))
        320 |            LVLBND(I,J,LBND)=L
        321 |            TBND(I,J,LBND)=T(I,J,L)
        322 |            QBND(I,J,LBND)=Q(I,J,L)
        323 |C
        324 |            TMT0=T(I,J,L)-273.16
        325 |            TMT15=AMIN1(TMT0,-15.)
        326 |            AI=0.008855
        327 |            BI=1.
        328 |            IF(TMT0.LT.-20.)THEN
        329 |              AI=0.007225
        330 |              BI=0.9674
        331 |            ENDIF
        332 |            QW=PQ0/PM
        333 |     1          *EXP(A2*(T(I,J,L)-A3)/(T(I,J,L)-A4))
        334 |            QI=QW*(BI+AI*AMIN1(TMT0,0.))
        335 |            QINT=QW*(1.-0.00032*TMT15*(TMT15+15.))
        336 |            IF(TMT0.LT.-15.)THEN
        337 |                QSAT=QI
        338 |            ELSEIF(TMT0.GE.0.)THEN
        339 |                QSAT=QINT
        340 |            ELSE
        341 |              IF(IWL(I,J).GT.0.0) THEN
        342 |                QSAT=QI
        343 |              ELSE
        344 |                QSAT=QINT
        345 |              ENDIF
        346 |            ENDIF
        347 |CMEB 12/22/98 SWITCH TO RH VS WATER NO MATTER WHAT
        348 |C             DELETE THIS LINE TO SWITCH BACK TO RH VS ICE
        349 |              QSAT=QW
        350 |CMEB 12/22/98 SWITCH TO RH VS WATER NO MATTER WHAT
        351 |            QSBND(I,J,LBND)=QSAT
        352 |            OMGBND(I,J,LBND)=OMGA(I,J,L)
        353 |            PWTBND(I,J,LBND)=(Q(I,J,L)+CWM(I,J,L))*DP*GI
        354 |          ENDIF
        355 |C
        356 |C   RH, BOUNDS CHECK
        357 |C
        358 |          RHBND(I,J,LBND)=QBND(I,J,LBND)/QSBND(I,J,LBND)
        359 |          IF (RHBND(I,J,LBND).GT.1.0) THEN
        360 |            RHBND(I,J,LBND)=1.0
        361 |            QBND(I,J,LBND)=RHBND(I,J,LBND)*QSBND(I,J,LBND)
        362 |          ENDIF
        363 |          IF (RHBND(I,J,LBND).LT.0.01) THEN
        364 |            RHBND(I,J,LBND)=0.01
        365 |            QBND(I,J,LBND)=RHBND(I,J,LBND)*QSBND(I,J,LBND)
        366 |          ENDIF
        367 |        ENDDO
        368 |        ENDDO
        369 |C
        370 |        DO J=JSTA_M,JEND_M
        371 |        DO I=2,IM-1
        372 |          IF(PVSUM(I,J,LBND).EQ.0.)THEN
        373 |            LV=LM
        374 |            PMINV=9999999.
        375 |            IE=I+IVE(J)
        376 |            IW=I+IVW(J)
        377 |C
        378 |C           PINT HALOS UPDATED ALREADY
        379 |C
        380 |            DO LL=1,LM
        381 |              PMV=0.125*(PINT(IW,J,LL)+PINT(IE,J,LL)+
        382 |     1              PINT(I,J+1,LL)+PINT(I,J-1,LL)+
        383 |     2              PINT(IW,J,LL+1)+PINT(IE,J,LL+1)+
        384 |     3              PINT(I,J+1,LL+1)+PINT(I,J-1,LL+1))
        385 |              DELPV=ABS(PMV-PBND(I,J,LBND))
        386 |              IF(DELPV.LT.PMINV)THEN
        387 |                PMINV=DELPV
        388 |                LV=LL
        389 |              ENDIF
        390 |            ENDDO
        391 |C
        392 |            UBND(I,J,LBND)=U(I,J,LV)
        393 |            VBND(I,J,LBND)=V(I,J,LV)
        394 |          ENDIF
        395 |        ENDDO
        396 |        ENDDO
        397 |      ENDDO
        398 |C
        399 |      DEALLOCATE (PBINT)
        400 |      DEALLOCATE (QSBND)
        401 |      DEALLOCATE (PSUM)
        402 |      DEALLOCATE (PVSUM)
        403 |      DEALLOCATE (NSUM)
        404 |C
        405 |C     END OF ROUTINE
        406 |C
