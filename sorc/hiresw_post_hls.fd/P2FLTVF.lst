          1 |      SUBROUTINE P2FLTVF(MKOUNT,VBM2,FIELD)
          2 |CFPP$ NOCONCUR R
          3 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          4 |C                .      .    .
          5 |C SUBPROGRAM:    P2FLTVF     4-TH ORDER SMOOTHER for vel pts
          6 |C   PRGRMMR: TREADON         ORG: W/NP2      DATE: 93-03-02
          7 |C
          8 |C ABSTRACT:
          9 |C     THIS ROUTINE WILL APPLY A FOURTH OTHER FILTER (SMOOTHER)
         10 |C     OF A CONTINOUS FIELD AT VELOCITY POINTS ON THE STAGGERED
         11 |C     E-GRID.  MULTIPLE APPLICATIONS OF THIS FILTER MAY BE
         12 |C     MADE.  DOES FULL FIELD
         13 |C   .
         14 |C
         15 |C PROGRAM HISTORY LOG:
         16 |C   93-03-02  RUSS TREADON - MODIFIED SUBROUTINE P2FILT.
         17 |C   98-06-04  BLACK - CONVERSION TO 2-D
         18 |C
         19 |C USAGE:    CALL P2FLTV(MKOUNT,VBM2,FIELD)
         20 |C   INPUT ARGUMENT LIST:
         21 |C     MKOUNT   - NUMBER OF APPLICATIONS OF THE SMOOTHER.
         22 |C     HBM2     - VELOCITY POINT BOUNDARY MASK.
         23 |C     FIELD    - VELOCITY POINT FIELD TO BE SMOOTHED.
         24 |C
         25 |C   OUTPUT ARGUMENT LIST:
         26 |C     FIELD    - SMOOTHED VELOCITY POINT FIELD.
         27 |C
         28 |C   OUTPUT FILES:
         29 |C     NONE
         30 |C
         31 |C   SUBPROGRAMS CALLED:
         32 |C     UTILITIES:
         33 |C       NONE
         34 |C     LIBRARY:
         35 |C       NONE
         36 |C
         37 |C   ATTRIBUTES:
         38 |C     LANGUAGE: FORTRAN
         39 |C     MACHINE : CRAY Y-MP
         40 |C$$$
         41 |C
         42 |C
         43 |C     INCLUDE GLOBAL PARAMETERS.  SET DEPENDENT VARIABLES.
         44 |C
         45 |C----------------------------------------------------------------------
         46 |      INCLUDE "parmeta"
  1       1 |C-------------------------------------------------------------
  1       2 |C***  SET PRIMARY GRID DIMENSIONS AND PRESSURE OUTPUT LEVELS
  1       3 |      PARAMETER (IM=89,JM=143,LM=60,LSM=39)
  1       4 |C-------------------------------------------------------------
  1       5 |C***  SET THE NUMBER OF PEs IN THE I-DIRECTION AND J-DIRECTION
  1       6 |      PARAMETER(INPES=5,JNPES=6)
  1       7 |C-------------------------------------------------------------
  1       8 |C
  1       9 |      PARAMETER(IGSTL=-5,IGSTR=5)
  1      10 |      PARAMETER(JGSTL=-5,JGSTR=5)
  1      11 |      PARAMETER(ITAIL=IM-(INPES*(IM/INPES)))
  1      12 |      PARAMETER(JTAIL=JM-(JNPES*(JM/JNPES)))
  1      13 |C
  1      14 |      PARAMETER(IDIM1=IGSTL,IDIM2=IM/INPES+IGSTR+1)
  1      15 |      PARAMETER(JDIM1=JGSTL,JDIM2=JM/JNPES+JGSTR+1)
  1      16 |C
  1      17 |C-------------------------------------------------------------
         47 |C
         48 |      INCLUDE "INDX.comm"
  2       1 |C-----------------------------------------------------------------------
  2       2 |                        C O M M O N /INDX/
  2       3 |     & IHE(JM),IHW(JM),IVE(JM),IVW(JM)
  2       4 |C-----------------------------------------------------------------------
         49 |C----------------------------------------------------------------------
         50 |                            D I M E N S I O N
         51 |     & VBM2(IM,JM),FIELD(IM,JM),WFIELD(IM,JM),FNE(IM,JM),FSE(IM,JM)
         52 |C
         53 |C***********************************************************************
         54 |C     START P2FLTV HERE.
         55 |C
         56 |C     IF NUMBER OF PASSES IS LESS THAN 1, EXIT ROUTINE.
         57 |C
         58 |      IF(MKOUNT.LT.1)        RETURN
         59 |C
         60 |C     OTHERWISE, APPLY SMOOTHER MKOUND TIMES.
         61 |C
         62 |C-----------------------------------------------------------------------
         63 |      DO 400 KOUNT=1,MKOUNT
         64 |C-----------------------------------------------------------------------
         65 |C
         66 |C        FILTERING STARTS
         67 |C
         68 |C-----------------------------------------------------------------------
         69 |!$omp  parallel do
         70 |      DO J=1,JM
         71 |      DO I=1,IM
         72 |        FNE   (I,J)=0.0
         73 |        FSE   (I,J)=0.0
         74 |        WFIELD(I,J)=0.0
         75 |      ENDDO
         76 |      ENDDO
         77 |C
         78 |C        2-ND ORDER DIAGONAL CONTRIBUTIONS
         79 |C
         80 |!$omp  parallel do
         81 |      DO J=1,JM-1
         82 |      DO I=1,IM-1
         83 |        FNE(I,J)=FIELD(I+IVE(J),J+1)-FIELD(I,J)
         84 |      ENDDO
         85 |      ENDDO
         86 |C
         87 |!$omp  parallel do
         88 |      DO J=2,JM
         89 |      DO I=1,IM-1
         90 |        FSE(I,J)=FIELD(I+IVE(J),J-1)-FIELD(I,J)
         91 |      ENDDO
         92 |      ENDDO
         93 |C
         94 |!$omp  parallel do
         95 |      DO J=3,JM-2
         96 |      DO I=2,IM-1
         97 |        WFIELD(I,J)=(FNE(I,J)-FNE(I+IVW(J),J-1)
         98 |     1              +FSE(I,J)-FSE(I+IVW(J),J+1))
         99 |     2              *VBM2(I,J)*0.125
        100 |      ENDDO
        101 |      ENDDO
        102 |C
        103 |C        4-TH ORDER DIAGONAL CONTRIBUTIONS
        104 |C
        105 |!$omp  parallel do
        106 |      DO J=1,JM-1
        107 |      DO I=1,IM-1
        108 |        FNE(I,J)=WFIELD(I+IHE(J),J+1)-WFIELD(I,J)
        109 |      ENDDO
        110 |      ENDDO
        111 |C
        112 |!$omp  parallel do
        113 |      DO J=2,JM
        114 |      DO I=1,IM-1
        115 |        FSE(I,J)=WFIELD(I+IVE(J),J-1)-WFIELD(I,J)
        116 |      ENDDO
        117 |      ENDDO
        118 |C
        119 |!$omp  parallel do
        120 |      DO J=3,JM-2
        121 |      DO I=2,IM-1
        122 |        FIELD(I,J)=FIELD(I,J)-(FNE(I,J)-FNE(I+IVW(J),J-1)
        123 |     1                        +FSE(I,J)-FSE(I+IVW(J),J+1))
        124 |     2                        *VBM2(I,J)*0.125
        125 |      ENDDO
        126 |      ENDDO
        127 |C
        128 | 400  CONTINUE
        129 |C
        130 |C     END OF ROUTINE
        131 |      RETURN
        132 |      END
 
