         56 |C
         57 |C**********************************************************************
         58 |C     START EFILL HERE
         59 |C
         60 |C     SEE IF THERE ARE ANY MISSING VALUES IN THE ARRAY.  IF NOT,
         61 |C     RETURN TO CALLING PROGRAM.
         62 |C
         63 |      CALL MINMAX(EGRID,IM,JM,FMIN,FMAX)
         64 |      CALL MPI_ALLREDUCE(FMAX,TEMP,1,MPI_REAL,MPI_MAX,
         65 |     *                MPI_COMM_COMP,IERR)
         66 |      FMAX = TEMP
         67 |      IF (ABS(FMAX-SPVAL).GT.SMALL) RETURN
         68 |C
         69 |C     COMPUTE FIELD MEAN, EXCLUDING MISSING POINTS.
         70 |C
         71 |      NPTS=0
         72 |      SUM =0.
         73 |C
         74 |      DO J=JSTA,JEND
         75 |      DO I=1,IM
         76 |        IF(ABS(EGRID(I,J)-SPVAL).GE.SMALL)THEN
         77 |          NPTS=NPTS+1
         78 |          SUM =SUM+EGRID(I,J)
         79 |        ENDIF
         80 |      ENDDO
         81 |      ENDDO
         82 |      CALL MPI_ALLREDUCE(SUM,SUMT,1,MPI_REAL8,MPI_SUM,
         83 |     *                MPI_COMM_COMP,IERR)
         84 |      SUM = SUMT
         85 |      CALL MPI_ALLREDUCE(NPTS,ITEMP,1,MPI_INTEGER,MPI_SUM,
         86 |     *                MPI_COMM_COMP,IERR)
         87 |      NPTS = ITEMP
         88 |      IF(NPTS.EQ.0)THEN
         89 |        WRITE(STDOUT,*)'EFILL:  ALL DATA ON EGRID EQUAL SPVAL'
         90 |        RETURN
         91 |      ENDIF
         92 |      AVG=SUM/NPTS
         93 |c     if ( me .eq. 0 ) print *, ' avg, sum, npts = ', avg, sum, npts
         94 |C
         95 |C     REPLACE MISSING VALUES WITH FIELD MEAN.
         96 |C
         97 |!$omp  parallel do
         98 |      DO J=JSTA,JEND
         99 |      DO I=1,IM
        100 |        IF(ABS(EGRID(I,J)-SPVAL).LT.SMALL)EGRID(I,J)=AVG
        101 |      ENDDO
        102 |      ENDDO
        103 |CX      CALL MINMAX(EGRID,IM,JM,FMIN,FMAX)
        104 |CX      WRITE(STDOUT,*)'EFILL:  EGRID MIN,MAX,AVG:  ',FMIN,FMAX,AVG
        105 |C
        106 |C     END OF ROUTINE.
        107 |C
        108 |      RETURN
        109 |      END
 
