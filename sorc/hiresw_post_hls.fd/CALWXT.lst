          1 |       SUBROUTINE CALWXT(T,Q,RES,PD,HTM,LMH,PREC,PT,PDTOP
          2 |     +  ,SGML1,SGML2,SG1,SG2,IWX)
          3 |C
          4 |C     FILE: CALWXT.f
          5 |C     WRITTEN: 11 NOVEMBER 1993, MICHAEL BALDWIN
          6 |C     REVISIONS:
          7 |C               30 SEPT 1994-SETUP NEW DECISION TREE (M BALDWIN)
          8 |C               12 JUNE 1998-CONVERSION TO 2-D (T BLACK)
          9 |C     01-10-25  H CHUANG - MODIFIED TO PROCESS HYBRID MODEL OUTPUT
         10 |C
         11 |C
         12 |C     ROUTINE TO COMPUTE PRECIPITATION TYPE USING A DECISION TREE
         13 |C     APPROACH THAT USES VARIABLES SUCH AS INTEGRATED WET BULB TEMP
         14 |C     BELOW FREEZING AND LOWEST LAYER TEMPERATURE
         15 |C
         16 |C     SEE BALDWIN AND CONTORNO PREPRINT FROM 13TH WEATHER ANALYSIS
         17 |C     AND FORECASTING CONFERENCE FOR MORE DETAILS
         18 |C     (OR BALDWIN ET AL, 10TH NWP CONFERENCE PREPRINT)
         19 |C
         20 |      INCLUDE "parmeta"
  1       1 |C-------------------------------------------------------------
  1       2 |C***  SET PRIMARY GRID DIMENSIONS AND PRESSURE OUTPUT LEVELS
  1       3 |      PARAMETER (IM=89,JM=143,LM=60,LSM=39)
  1       4 |C-------------------------------------------------------------
  1       5 |C***  SET THE NUMBER OF PEs IN THE I-DIRECTION AND J-DIRECTION
  1       6 |      PARAMETER(INPES=5,JNPES=6)
  1       7 |C-------------------------------------------------------------
  1       8 |C
  1       9 |      PARAMETER(IGSTL=-5,IGSTR=5)
  1      10 |      PARAMETER(JGSTL=-5,JGSTR=5)
  1      11 |      PARAMETER(ITAIL=IM-(INPES*(IM/INPES)))
  1      12 |      PARAMETER(JTAIL=JM-(JNPES*(JM/JNPES)))
  1      13 |C
  1      14 |      PARAMETER(IDIM1=IGSTL,IDIM2=IM/INPES+IGSTR+1)
  1      15 |      PARAMETER(JDIM1=JGSTL,JDIM2=JM/JNPES+JGSTR+1)
  1      16 |C
  1      17 |C-------------------------------------------------------------
         21 |C
         22 |      PARAMETER (LP1=LM+1)
         23 |      PARAMETER (H1M12=1.E-12)
         24 |      PARAMETER (PTHRESH=0.000004)
         25 |C
         26 |C  LIST OF VARIABLES NEEDED
         27 |C    PARAMETERS:
         28 |C      D608,ROG,H1,D00
         29 |       PARAMETER(D608=0.608,ROG=287.04/9.806,H1=1.0,D00=0.0)
         30 |C
         31 |C    INPUT:
         32 |C      T,Q,RES,PD,HTM,LMH,PREC,PT,AETA,ETA
         33 |      DIMENSION T(IM,jsta_2l:jend_2u,LM),Q(IM,jsta_2l:jend_2u,LM)
         34 |     &,         RES(IM,jsta_2l:jend_2u),PD(IM,jsta_2l:jend_2u)
         35 |     &,         HTM(IM,jsta_2l:jend_2u,LM)
         36 |      DIMENSION LMH(IM,JM),PREC(IM,JM),SG1(LP1),SG2(LP1)
         37 |     & ,SGML1(LM),SGML2(LM)
         38 |C    OUTPUT:
         39 |C      IWX - INSTANTANEOUS WEATHER TYPE.
         40 |C        ACTS LIKE A 4 BIT BINARY
         41 |C          1111 = RAIN/FREEZING RAIN/ICE PELLETS/SNOW
         42 |C          WHERE THE ONE'S DIGIT IS FOR SNOW
         43 |C                THE TWO'S DIGIT IS FOR ICE PELLETS
         44 |C                THE FOUR'S DIGIT IS FOR FREEZING RAIN
         45 |C            AND THE EIGHT'S DIGIT IS FOR RAIN
         46 |      DIMENSION IWX(IM,JM)
         47 |C    INTERNAL:
         48 |C      PDSL
         49 |C
         50 |      REAL, ALLOCATABLE :: TWET(:,:,:)
         51 |      DIMENSION PDSL(IM,JM)
         52 |      DIMENSION KARR(IM,JM)
         53 |      DIMENSION LICEE(IM,JM),TCOLD(IM,JM),TWARM(IM,JM)
         54 |C    SUBROUTINES CALLED:
         55 |C     WETBULB
         56 |C
         57 |C
         58 |C     INITIALIZE WEATHER TYPE ARRAY TO ZERO (IE, OFF).
         59 |C     WE DO THIS SINCE WE WANT IWX TO REPRESENT THE
         60 |C     INSTANTANEOUS WEATHER TYPE ON RETURN.
         61 |C
         62 |      INCLUDE "CTLBLK.comm"
  2       1 |C-----------------------------------------------------------------------
  2       2 |      COMMON /CTLBLK/
  2       3 |     & SIGMA,RUN,IDAT(3),IHRST,FIRST,RESTRT,NFCST,NBC,LIST,IOUT
  2       4 |     &,DT,NTSD,NTSTM,NDDAMP,NPREC,IDTAD,NBOCO,NSHDE,NCP
  2       5 |     &,NPHS,NRADS,NRADL
  2       6 |      INTEGER NUM_PROCS, ME, JSTA, JEND,
  2       7 |     &        JSTA_M, JSTA_M2, JEND_M, JEND_M2, IUP, IDN, ICNT, IDSP,
  2       8 |     &        JSTA_2L, JEND_2U
  2       9 |      COMMON /PARALLEL/NUM_PROCS, ME, JSTA, JEND, JSTA_M, JEND_M,
  2      10 |     &                 JSTA_M2, JEND_M2, IUP, IDN,
  2      11 |     &                 ICNT(0:1023), IDSP(0:1023),
  2      12 |     &                 JSTA_2L, JEND_2U, NUM_SERVERS, MPI_COMM_INTER,
  2      13 |     &                 MPI_COMM_COMP
  2      14 |C-----------------------------------------------------------------------
         63 |C
         64 |C     ALLOCATE LOCAL STORAGE
         65 |C
         66 |      ALLOCATE ( TWET(IM,JSTA_2L:JEND_2U,LM) )
         67 |C
         68 |!$omp  parallel do
         69 |      DO J=JSTA,JEND
         70 |      DO I=1,IM
         71 |        IWX(I,J) = 0
         72 |        PDSL(I,J)=RES(I,J)*PD(I,J)
         73 |      ENDDO
         74 |      ENDDO
         75 |C
         76 |!$omp  parallel do
         77 |!$omp& private(a,lmhk,pkl,psfck,qkl,tdchk,tdkl,tdpre,tkl)
         78 |      DO 800 J=JSTA,JEND
         79 |      DO 800 I=1,IM
         80 |      LMHK=LMH(I,J)
         81 |C
         82 |C   SKIP THIS POINT IF NO PRECIP THIS TIME STEP
         83 |C
         84 |      IF (PREC(I,J).LE.PTHRESH) GOTO 800
         85 |C
         86 |C   FIND COLDEST AND WARMEST TEMPS IN SATURATED LAYER BETWEEN
         87 |C   70 MB ABOVE GROUND AND 500 MB
         88 |C   ALSO FIND HIGHEST SATURATED LAYER IN THAT RANGE
         89 |C
         90 |      PSFCK=PD(I,J)+PT+PDTOP
         91 |      TDCHK=2.0
         92 |  760 TCOLD(I,J)=T(I,J,LMHK)
         93 |      TWARM(I,J)=T(I,J,LMHK)
         94 |      LICEE(I,J)=LMHK
         95 |C
         96 |      DO 775 L=1,LMHK
         97 |      QKL=Q(I,J,L)
         98 |      QKL=AMAX1(H1M12,QKL)
         99 |      TKL=T(I,J,L)
        100 |      PKL=PDTOP*SGML1(L)+PDSL(I,J)*SGML2(L)+PT
        101 |C
        102 |C   SKIP PAST THIS IF THE LAYER IS NOT BETWEEN 70 MB ABOVE GROUND
        103 |C       AND 500 MB
        104 |C
        105 |      IF (PKL.LT.50000.0.OR.PKL.GT.PSFCK-7000.0) GOTO 775
        106 |      A=ALOG(QKL*PKL/(6.1078*(0.378*QKL+0.622)))
        107 |      TDKL=(237.3*A)/(17.269-A)+273.15
        108 |      TDPRE=TKL-TDKL
        109 |      IF (TDPRE.LT.TDCHK.AND.TKL.LT.TCOLD(I,J)) TCOLD(I,J)=TKL
        110 |      IF (TDPRE.LT.TDCHK.AND.TKL.GT.TWARM(I,J)) TWARM(I,J)=TKL
        111 |      IF (TDPRE.LT.TDCHK.AND.L.LT.LICEE(I,J)) LICEE(I,J)=L
        112 |  775 CONTINUE
        113 |C
        114 |C    IF NO SAT LAYER AT DEW POINT DEP=TDCHK, INCREASE TDCHK
        115 |C     AND START AGAIN (BUT DON'T MAKE TDCHK > 6)
        116 |C
        117 |      IF (TCOLD(I,J).EQ.T(I,J,LMHK).AND.TDCHK.LT.6.0) THEN
        118 |        TDCHK=TDCHK+2.0
        119 |        GOTO 760
        120 |      ENDIF
        121 |  800 CONTINUE
        122 |C
        123 |C    LOWEST LAYER T
        124 |C
        125 |      DO 850 J=JSTA,JEND
        126 |      DO 850 I=1,IM
        127 |      KARR(I,J)=0
        128 |      IF (PREC(I,J).LE.PTHRESH) GOTO 850
        129 |      LMHK=LMH(I,J)
        130 |      TLMHK=T(I,J,LMHK)
        131 |C
        132 |C    DECISION TREE TIME
        133 |C
        134 |      IF (TCOLD(I,J).GT.269.15) THEN
        135 |          IF (TLMHK.LE.273.15) THEN
        136 |C             TURN ON THE FLAG FOR
        137 |C             FREEZING RAIN = 4
        138 |C             IF ITS NOT ON ALREADY
        139 |C             IZR=MOD(IWX(I,J),8)/4
        140 |C             IF (IZR.LT.1) IWX(I,J)=IWX(I,J)+4
        141 |              IWX(I,J)=IWX(I,J)+4
        142 |            GOTO 850
        143 |          ELSE
        144 |C             TURN ON THE FLAG FOR
        145 |C             RAIN = 8
        146 |C             IF ITS NOT ON ALREADY
        147 |C             IRAIN=IWX(I,J)/8
        148 |C             IF (IRAIN.LT.1) IWX(I,J)=IWX(I,J)+8
        149 |              IWX(I,J)=IWX(I,J)+8
        150 |            GOTO 850
        151 |          ENDIF
        152 |      ENDIF
        153 |      KARR(I,J)=1
        154 |  850 CONTINUE
        155 |C
        156 |C   COMPUTE WET BULB ONLY AT POINTS THAT NEED IT
        157 |C
        158 |      CALL WETBULB(T,Q,PDSL,HTM,KARR,TWET)
        159 |C
        160 |!$omp  parallel do
        161 |!$omp& private(area1,areap4,areas8,dzkl,ifrzl,iwrml,lice,
        162 |!$omp&         lmhk,pintk1,pintk2,pm150,psfck,surfc,surfw,
        163 |!$omp&         tlmhk,twrmk)
        164 |      DO 1900 J=JSTA_M,JEND_M
        165 |      DO 1900 I=2,IM-1
        166 |      IF(KARR(I,J).GT.0)THEN
        167 |        LMHK=LMH(I,J)
        168 |        LICE=LICEE(I,J)
        169 |        PSFCK=PD(I,J)+PT+PDTOP
        170 |        TLMHK=T(I,J,LMHK)
        171 |        TWRMK=TWARM(I,J)
        172 |C
        173 |C    TWET AREA VARIABLES
        174 |C     CALCULATE ONLY WHAT IS NEEDED
        175 |C      FROM GROUND TO 150 MB ABOVE SURFACE
        176 |C      FROM GROUND TO TCOLD LAYER
        177 |C      AND FROM GROUND TO 1ST LAYER WHERE WET BULB T < 0.0
        178 |C
        179 |C     PINTK1 IS THE PRESSURE AT THE BOTTOM OF THE LAYER
        180 |C     PINTK2 IS THE PRESSURE AT THE TOP OF THE LAYER
        181 |C
        182 |C     AREAP4 IS THE AREA OF TWET ABOVE -4 C BELOW HIGHEST SAT LYR
        183 |C
        184 |        AREAS8=D00
        185 |        AREAP4=D00
        186 |        SURFW =D00
        187 |        SURFC =D00
        188 |        PINTK1=PSFCK
        189 |C
        190 |        DO 1945 L=LMHK,LICE,-1
        191 |        PINTK2=PDTOP*SG1(L)+PDSL(I,J)*SG2(L)+PT
        192 |        DZKL=T(I,J,L)*(Q(I,J,L)*D608+H1)*ROG*
        193 |     1        ALOG(PINTK1/PINTK2)
        194 |        AREA1=(TWET(I,J,L)-269.15)*DZKL
        195 |        IF (TWET(I,J,L).GE.269.15) AREAP4=AREAP4+AREA1
        196 |        PINTK1=PINTK2
        197 | 1945   CONTINUE
        198 |C
        199 |        IF (AREAP4.LT.3000.0) THEN
        200 |C             TURN ON THE FLAG FOR
        201 |C             SNOW = 1
        202 |C             IF ITS NOT ON ALREADY
        203 |C             ISNO=MOD(IWX(I,J),2)
        204 |C             IF (ISNO.LT.1) IWX(I,J)=IWX(I,J)+1
        205 |          IWX(I,J)=IWX(I,J)+1
        206 |          GO TO 1900
        207 |        ENDIF
        208 |C
        209 |C     AREAS8 IS THE NET AREA OF TWET W.R.T. FREEZING IN LOWEST 150MB
        210 |C
        211 |        PINTK1=PSFCK
        212 |        PM150=PSFCK-15000.
        213 |C
        214 |        DO 1955 L=LMHK,1,-1
        215 |        PINTK2=PDTOP*SG1(L)+PDSL(I,J)*SG2(L)+PT
        216 |        IF(PINTK1.LT.PM150)GO TO 1950
        217 |        DZKL=T(I,J,L)*(Q(I,J,L)*D608+H1)*ROG*
        218 |     1        ALOG(PINTK1/PINTK2)
        219 |C
        220 |C    SUM PARTIAL LAYER IF IN 150 MB AGL LAYER
        221 |C
        222 |        IF(PINTK2.LT.PM150)
        223 |     &    DZKL=T(I,J,L)*(Q(I,J,L)*D608+H1)*ROG*
        224 |     1         ALOG(PINTK1/PM150)
        225 |        AREA1=(TWET(I,J,L)-273.15)*DZKL
        226 |        AREAS8=AREAS8+AREA1
        227 | 1950   PINTK1=PINTK2
        228 | 1955   CONTINUE
        229 |C
        230 |C     SURFW IS THE AREA OF TWET ABOVE FREEZING BETWEEN THE GROUND
        231 |C       AND THE FIRST LAYER ABOVE GROUND BELOW FREEZING
        232 |C     SURFC IS THE AREA OF TWET BELOW FREEZING BETWEEN THE GROUND
        233 |C       AND THE WARMEST SAT LAYER
        234 |C
        235 |        PINTK1=PSFCK
        236 |        IFRZL=0
        237 |        IWRML=0
        238 |C
        239 |        DO 2050 L=LMHK,1,-1
        240 |        IF (IFRZL.EQ.0.AND.T(I,J,L).LT.273.15) IFRZL=1
        241 |        IF (IWRML.EQ.0.AND.T(I,J,L).GE.TWRMK) IWRML=1
        242 |C
        243 |        IF (IWRML.EQ.0.OR.IFRZL.EQ.0) THEN
        244 |          PINTK2=PDTOP*SG1(L)+PDSL(I,J)*SG2(L)+PT
        245 |          DZKL=T(I,J,L)*(Q(I,J,L)*D608+H1)*ROG*
        246 |     1         ALOG(PINTK1/PINTK2)
        247 |          AREA1=(TWET(I,J,L)-273.15)*DZKL
        248 |          PINTK1=PINTK2
        249 |          IF(IFRZL.EQ.0.AND.TWET(I,J,L).GE.273.15)SURFW=SURFW+AREA1
        250 |          IF(IWRML.EQ.0.AND.TWET(I,J,L).LE.273.15)SURFC=SURFC+AREA1
        251 |        ENDIF
        252 | 2050   CONTINUE
        253 |        IF(SURFC.LT.-3000.0.OR.
        254 |     &    (AREAS8.LT.-3000.0.AND.SURFW.LT.50.0)) THEN
        255 |C             TURN ON THE FLAG FOR
        256 |C             ICE PELLETS = 2
        257 |C             IF ITS NOT ON ALREADY
        258 |C             IIP=MOD(IWX(I,J),4)/2
        259 |C             IF (IIP.LT.1) IWX(I,J)=IWX(I,J)+2
        260 |          IWX(I,J)=IWX(I,J)+2
        261 |          GOTO 1900
        262 |        ENDIF
        263 |C
        264 |        IF(TLMHK.LT.273.15) THEN
        265 |C             TURN ON THE FLAG FOR
        266 |C             FREEZING RAIN = 4
        267 |C             IF ITS NOT ON ALREADY
        268 |C             IZR=MOD(IWX(K),8)/4
        269 |C             IF (IZR.LT.1) IWX(K)=IWX(K)+4
        270 |          IWX(I,J)=IWX(I,J)+4
        271 |        ELSE
        272 |C             TURN ON THE FLAG FOR
        273 |C             RAIN = 8
        274 |C             IF ITS NOT ON ALREADY
        275 |C             IRAIN=IWX(K)/8
        276 |C             IF (IRAIN.LT.1) IWX(K)=IWX(K)+8
        277 |          IWX(I,J)=IWX(I,J)+8
        278 |        ENDIF
        279 |      ENDIF
        280 | 1900 CONTINUE
        281 |C---------------------------------------------------------
        282 |      DEALLOCATE (TWET)
        283 |      RETURN
        284 |      END
 
