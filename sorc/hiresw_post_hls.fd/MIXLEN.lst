          1 |      SUBROUTINE MIXLEN
          2 |     I (ZINT,T,PDSL,PDTOP,SGML1,SGML2,PT,Q2,HGT,HTM,EL0
          3 |     I, LM,LM1,LP1,IM,JM
          4 |     O, EL)
          5 |C
          6 |C     CALCULATES LAYER-AVERAGED BLACKADAR'S MIXING LENGTH, AND PBL TOP
          7 |C     AS CPBLT*(ASYMPTOTIC EL); AND THEN EL, ACCOUNT TAKEN OF STABILITY,
          8 |C     PBL TOP AND VERTICAL GRID DISTANCE RESTRICTIONS (SEE BELOW)
          9 |C
         10 |C     SET FROM EXISTING CODES BY L. LOBOCKI, JUNE 5, 1992
         11 |C       MODIFIED BY FEDOR MESINGER, OCTOBER 13, NOVEMBER 19
         12 |C       MODIFIED BY JIM TUCCILLO FOR MPI IMPLEMENTATION
         13 |C   01-10-25  H CHUANG - MODIFIED TO PROCESS HYBRID MODEL OUTPUT
         14 |C
         15 |C     INPUT:
         16 |C     ------
         17 |C
         18 |C     ZINT (IM,JM,LP1) - ETA INTERFACES HEIGHT FIELD
         19 |C     T    (IM,JM,LM)  - TEMPERATURE
         20 |C     APE  (IM,JM,02)  - (P/P0)**-KAPPA IN LAYERS
         21 |C     Q2   (IM,JM,LM)  - TURBULENCE KINETIC ENERGY * 2
         22 |C     HGT  (IM,JM)     - SURFACE ELEVATION ARRAY
         23 |C     HTM  (IM,JM,LM)  - HEIGHT TOPOGRAPHY MASK ARRAY
         24 |C     EL0  (IM,JM)     - ARRAY OF ASYMPTOTIC VALUES FOR MIXING LENGTH
         25 |C     LM               - ARRAY DIMENSION FOR VERTICAL GRIDS
         26 |C     LM1              - ARRAY DIMENSION FOR VERTICAL GRIDS, MINUS ONE
         27 |C     LP1              - ARRAY DIMENSION FOR VERTICAL GRIDS, PLUS ONE
         28 |C
         29 |C     IM,JM            - ARRAY DIMENSIONS FOR HORIZONTAL GRIDS
         30 |C
         31 |C
         32 |C     OUTPUT:
         33 |C     -------
         34 |C
         35 |C     EL   (IM,JM,LM) - FIELD OF RESULTING MASTER LENGTH SCALES
         36 |C
         37 |C
         38 |C     SCRATCH AREAS:
         39 |C     --------------
         40 |C
         41 |C     VKRMZ(IM,JM)
         42 |C
         43 |C     RELEVANT CONSTANTS:
         44 |C     -------------------
         45 |C
         46 |C     VON KARMAN CONSTANT:
         47 |      PARAMETER (VKRM=0.4)
         48 |C     CONSTANTS NEEDED FOR THE EL(BL,ST,ZI) SCHEME:
         49 |      PARAMETER (FRG=4.*9.8,DRDRFF=0.54,CPBLT=10.,CSH=0.23*0.5
         50 |     &, EPSN2=1.E-7,EPSQ2=1.E-4,CAPA=0.28589641)
         51 |C
         52 |C     ------------------------------------------------------------------
         53 |C
         54 |      DIMENSION ZINT(IM,jsta_2l:jend_2u,LP1)
         55 |     &,         EL(IM,jsta_2l:jend_2u,LM),EL0(IM,JM)
         56 |     &,         PDSL(IM,JM)
         57 |     &,         HTM(IM,jsta_2l:jend_2u,LM),HGT(IM,jsta_2l:jend_2u)
         58 |     &,         Q2(IM,jsta_2l:jend_2u,LM)
         59 |     &,         T(IM,jsta_2l:jend_2u,LM),APE(IM,JM,2),SGML1(LM)
         60 |     &,         SGML2(LM)
         61 |       INCLUDE "CTLBLK.comm"
  1       1 |C-----------------------------------------------------------------------
  1       2 |      COMMON /CTLBLK/
  1       3 |     & SIGMA,RUN,IDAT(3),IHRST,FIRST,RESTRT,NFCST,NBC,LIST,IOUT
  1       4 |     &,DT,NTSD,NTSTM,NDDAMP,NPREC,IDTAD,NBOCO,NSHDE,NCP
  1       5 |     &,NPHS,NRADS,NRADL
  1       6 |      INTEGER NUM_PROCS, ME, JSTA, JEND,
  1       7 |     &        JSTA_M, JSTA_M2, JEND_M, JEND_M2, IUP, IDN, ICNT, IDSP,
  1       8 |     &        JSTA_2L, JEND_2U
  1       9 |      COMMON /PARALLEL/NUM_PROCS, ME, JSTA, JEND, JSTA_M, JEND_M,
  1      10 |     &                 JSTA_M2, JEND_M2, IUP, IDN,
  1      11 |     &                 ICNT(0:1023), IDSP(0:1023),
  1      12 |     &                 JSTA_2L, JEND_2U, NUM_SERVERS, MPI_COMM_INTER,
  1      13 |     &                 MPI_COMM_COMP
  1      14 |C-----------------------------------------------------------------------
         62 |C***********************************************************************
         63 |C
         64 |!$omp  parallel do
         65 |      DO L=1,LM
         66 |        DO J=JSTA,JEND
         67 |        DO I=1,IM
         68 |          EL(I,J,L)=0.
         69 |        ENDDO
         70 |        ENDDO
         71 |      ENDDO
         72 |C
         73 |C---THE AVERAGE EL SCHEME---------------------------(FM, AUGUST 19 MEMO)
         74 |C   FIRST GET EL IN THE LAYERS
         75 |C
         76 |!$omp  parallel do
         77 |!$omp& private(vkrmz,zl)
         78 |      DO 260 L=1,LM
         79 |      DO J=JSTA_M,JEND_M
         80 |      DO I=2,IM-1
         81 |        ZL=0.5*(ZINT(I,J,L)+ZINT(I,J,L+1))
         82 |        VKRMZ=(ZL-HGT(I,J))*VKRM
         83 |        EL(I,J,L)=EL0(I,J)*VKRMZ/(EL0(I,J)+VKRMZ)
         84 |      ENDDO
         85 |      ENDDO
         86 |  260 CONTINUE
         87 |C***
         88 |C***  GET NOW THE INTERFACE EL BY TWO-POINT AVERAGING OF LAYER VALUES
         89 |C***
         90 |      DO 280 L=1,LM1
         91 |!$omp  parallel do
         92 |      DO J=JSTA_M,JEND_M
         93 |      DO I=2,IM-1
         94 |        EL(I,J,L)=0.5*(EL(I,J,L)+EL(I,J,L+1))*HTM(I,J,L+1)
         95 |      ENDDO
         96 |      ENDDO
         97 | 280  CONTINUE
         98 |C
         99 |!$omp  parallel do
        100 |      DO J=JSTA,JEND
        101 |      DO I=1,IM
        102 |        EL(I,J,LM)=0.0
        103 |      ENDDO
        104 |      ENDDO
        105 |C---STABILITY, PBL TOP, AND VERTICAL GRID DISTANCE RESTRICTIONS:--------
        106 |C   COMPUTE EL STABLE AND
        107 |C   * USE THE SMALLER OF EL BLACKADAR, EL STABLE IF WITHIN PBL;
        108 |C   * USE THE SMALLEST OF EL STABLE, ELVGD, AND VKRMZ IF ABOVE PBL
        109 |C       (ASSUME PBL TOP IS AT CPBLT*EL0(K));
        110 |      DO J=JSTA_M,JEND_M
        111 |      DO I=2,IM-1
        112 |        APE(I,J,1)=(1.E5/(PDTOP*SGML1(1)+PDSL(I,J)*SGML2(1)+PT))**CAPA
        113 |      ENDDO
        114 |      ENDDO
        115 |C
        116 |      DO 380 L=1,LM1
        117 |!$omp  parallel do
        118 |!$omp& private(elst,elvgd,ensq,q2kl,ziag)
        119 |      DO J=JSTA_M,JEND_M
        120 |      DO I=2,IM-1
        121 |        APE(I,J,2)=(1.E5/(PDTOP*SGML1(L+1)+PDSL(I,J)*SGML2(L+1)
        122 |     1	+PT))**CAPA
        123 |        ENSQ=HTM(I,J,L+1)*
        124 |     1       FRG*(T(I,J,L)*APE(I,J,1)-T(I,J,L+1)*APE(I,J,2))/
        125 |     2       ((T(I,J,L)*APE(I,J,1)+T(I,J,L+1)*APE(I,J,2))*
        126 |     3        (ZINT(I,J,L)-ZINT(I,J,L+2))+EPSN2)
        127 |        ENSQ=AMAX1(ENSQ,EPSN2)
        128 |        Q2KL=AMAX1(EPSQ2,Q2(I,J,L))
        129 |        ELST=DRDRFF*SQRT(Q2KL/ENSQ)
        130 |CWAS    ELST=DRDRFF*SQRT(Q2(I,J,L)/ENSQ)
        131 |        ZIAG=ZINT(I,J,L+1)-HGT(I,J)
        132 |C
        133 |        IF(ZIAG.LT.CPBLT*EL0(I,J))THEN
        134 |          EL(I,J,L)=AMIN1(EL(I,J,L),ELST)
        135 |        ELSE
        136 |          ELVGD=CSH*(ZINT(I,J,L)-ZINT(I,J,L+2))
        137 |          EL(I,J,L)=AMIN1(ELST,ELVGD,VKRM*ZIAG)
        138 |        ENDIF
        139 |        APE(I,J,1)=APE(I,J,2)
        140 |      ENDDO
        141 |      ENDDO
        142 |  380 CONTINUE
        143 |C
        144 |      RETURN
        145 |      END
 
