          1 |      SUBROUTINE CALTHTE(P1D,T1D,Q1D,THTE)
          2 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          3 |C                .      .    .
          4 |C SUBPROGRAM:    CALTHTE      COMPUTES THETA-E
          5 |C   PRGRMMR: TREADON         ORG: W/NP2      DATE: 93-06-18
          6 |C
          7 |C ABSTRACT:
          8 |C     THIS ROUTINE COMPUTES THE EQUIVALENT POTENTIAL TEMPERATURE
          9 |C     GIVEN PRESSURE, TEMPERATURE, AND SPECIFIC HUMIDITY.  THE
         10 |C     EQUATIONS OF BOLTON (MWR,1980) ARE USED.
         11 |C   .
         12 |C
         13 |C PROGRAM HISTORY LOG:
         14 |C   93-06-18  RUSS TREADON
         15 |C   98-06-16  T BLACK - CONVERSION FROM 1-D TO 2-D
         16 |C   00-01-04  JIM TUCCILLO - MPI VERSION
         17 |C
         18 |C USAGE:    CALL CALTHTE(P1D,T1D,Q1D,THTE)
         19 |C   INPUT ARGUMENT LIST:
         20 |C     P1D      - PRESSURE (PA)
         21 |C     T1D      - TEMPERATURE (K)
         22 |C     Q1D      - SPECIFIC HUMIDITY (KG/KG)
         23 |C
         24 |C   OUTPUT ARGUMENT LIST:
         25 |C     THTE     - THETA-E (K)
         26 |C
         27 |C   OUTPUT FILES:
         28 |C     NONE
         29 |C
         30 |C   SUBPROGRAMS CALLED:
         31 |C     UTILITIES:
         32 |C       VAPOR    - FUNCTION TO CALCULATE VAPOR PRESSURE.
         33 |C     LIBRARY:
         34 |C       NONE
         35 |C
         36 |C   ATTRIBUTES:
         37 |C     LANGUAGE: FORTRAN
         38 |C     MACHINE : CRAY C-90
         39 |C$$$
         40 |C
         41 |C
         42 |C     INCLUDE/SET PARAMETERS.
         43 |      INCLUDE "parmeta"
  1       1 |C-------------------------------------------------------------
  1       2 |C***  SET PRIMARY GRID DIMENSIONS AND PRESSURE OUTPUT LEVELS
  1       3 |      PARAMETER (IM=89,JM=143,LM=60,LSM=39)
  1       4 |C-------------------------------------------------------------
  1       5 |C***  SET THE NUMBER OF PEs IN THE I-DIRECTION AND J-DIRECTION
  1       6 |      PARAMETER(INPES=5,JNPES=6)
  1       7 |C-------------------------------------------------------------
  1       8 |C
  1       9 |      PARAMETER(IGSTL=-5,IGSTR=5)
  1      10 |      PARAMETER(JGSTL=-5,JGSTR=5)
  1      11 |      PARAMETER(ITAIL=IM-(INPES*(IM/INPES)))
  1      12 |      PARAMETER(JTAIL=JM-(JNPES*(JM/JNPES)))
  1      13 |C
  1      14 |      PARAMETER(IDIM1=IGSTL,IDIM2=IM/INPES+IGSTR+1)
  1      15 |      PARAMETER(JDIM1=JGSTL,JDIM2=JM/JNPES+JGSTR+1)
  1      16 |C
  1      17 |C-------------------------------------------------------------
         44 |      INCLUDE "params"
  2       1 |C
  2       2 |C     ARRAY DIMENSIONS.
  2       3 |      PARAMETER	
  2       4 |     & (IMT=2*IM-1,JMT=JM/2+1)
  2       5 |      PARAMETER
  2       6 |     & (IM1=IM-1,JM2=JM-2,JAM=6+2*(JM-10)
  2       7 |     &, NINC=2*IM-1
  2       8 |     &, LM1=LM-1,LP1=LM+1
  2       9 |     &, LB=2*IM+JM-3)
  2      10 |C
  2      11 |C     SMOOTHING PARAMETERS.
  2      12 |      PARAMETER
  2      13 |     & (KSMUD=4,KSLPD=1,KSSLP=2,KSFI=2,KST=2
  2      14 |     &, KSUV=2,KSRH=2,KSAV=2,KSLI=2,KSLP=2
  2      15 |     &, SMTHA=-1.8E-4,SMTHB=16.,NSUMD=1)
  2      16 |C
  2      17 |C     DIMENSIONS FOR PROFILE OUPUT ARRAYS.
  2      18 |      PARAMETER
  2      19 |     & (NSTAT=100,NWORD=6+6*LM+9,NWORD3=NWORD-3,NWORD6=NWORD-6)
  2      20 |C
  2      21 |C     CONSTANTS.
  2      22 |      PARAMETER
  2      23 |     & (CM1=2937.4,CM2=4.9283,CM3=23.5518)
  2      24 |      PARAMETER
  2      25 |     & (D00=0.0,D1=0.1,D01=0.01,D001=0.001,D50=0.50,H999=999.
  2      26 |     &, H1=1.0,H2=2.0,H4=4.0,H99999=99999.,H1M12=1.E-12
  2      27 |     &, H1000=1000.,H1M5=1.E-5,D125=.125,D25=0.25,H100=100.
  2      28 |     &, H10000=10000.,H10E5=1.E5,CAPA=0.28589641E0
  2      29 |     &, D115=0.115,D608=0.608,D05=0.05,D0065=0.0065
  2      30 |     &, H1E2=100.,H1E4=1.E4,H1M2=1.E-2)
  2      31 |      PARAMETER
  2      32 |     & (G=9.8,RD=287.04,ROG=RD/G,GI=1./G
  2      33 |     &, OVERRC=1.75,AD05=OVERRC*D05,CFT0=OVERRC-H1
  2      34 |     &, DTR=1.745329E-2,RTD=1./DTR,ERAD=6.376E6,CP=1004.6
  2      35 |     &, RCAPA=1./CAPA,P1000=1000.E2,DP10M=110
  2      36 |     &, ELWV=2.50E6,CPDR=CP/RD,RG=1./GI,ELDR=ELWV/RD
  2      37 |     &, BETA=.00367,BTG=BETA*9.8)
  2      38 |      PARAMETER
  2      39 |     & (FMW=18.015,FMD=28.964,EPS=FMW/FMD,ONEPS=1.-EPS,TFRZ=273.16
  2      40 |     &, RDOCP=RD/CP,LHEAT=2.5E6,LHTOCP=LHEAT/CP,EPSQ2=0.2
  2      41 |     &, PQ0=379.90516,A2=17.2693882,A3=273.16,A4=35.86)
         45 |      INCLUDE "CTLBLK.comm"
  3       1 |C-----------------------------------------------------------------------
  3       2 |      COMMON /CTLBLK/
  3       3 |     & SIGMA,RUN,IDAT(3),IHRST,FIRST,RESTRT,NFCST,NBC,LIST,IOUT
  3       4 |     &,DT,NTSD,NTSTM,NDDAMP,NPREC,IDTAD,NBOCO,NSHDE,NCP
  3       5 |     &,NPHS,NRADS,NRADL
  3       6 |      INTEGER NUM_PROCS, ME, JSTA, JEND,
  3       7 |     &        JSTA_M, JSTA_M2, JEND_M, JEND_M2, IUP, IDN, ICNT, IDSP,
  3       8 |     &        JSTA_2L, JEND_2U
  3       9 |      COMMON /PARALLEL/NUM_PROCS, ME, JSTA, JEND, JSTA_M, JEND_M,
  3      10 |     &                 JSTA_M2, JEND_M2, IUP, IDN,
  3      11 |     &                 ICNT(0:1023), IDSP(0:1023),
  3      12 |     &                 JSTA_2L, JEND_2U, NUM_SERVERS, MPI_COMM_INTER,
  3      13 |     &                 MPI_COMM_COMP
  3      14 |C-----------------------------------------------------------------------
         46 |C
         47 |      PARAMETER (KG2G=1.E3)
         48 |      PARAMETER (D35=3.5,D4805=4.805,H2840=2840.,H55=55.)
         49 |      PARAMETER (D2845=0.2845,D00028=0.00028,D3376=3.376)
         50 |      PARAMETER (D00254=0.00254,D00081=0.00081,D81=0.81)
         51 |      PARAMETER (D28=0.28,H2675=2675.)
         52 |C
         53 |C     DECLARE VARIABLES.
         54 |C
         55 |      REAL P1D(IM,JM),T1D(IM,JM),Q1D(IM,JM),THTE(IM,JM)
         56 |C
         57 |C***************************************************************
         58 |C     START CALTHTE.
         59 |C
         60 |C     ZERO THETA-E ARRAY
         61 |      DO J=JSTA,JEND
         62 |      DO I=1,IM
         63 |        THTE(I,J)=D00
         64 |      ENDDO
         65 |      ENDDO
         66 |C
         67 |C     COMPUTE THETA-E.
         68 |C
         69 |      DO J=JSTA_M,JEND_M
         70 |      DO I=2,IM-1
         71 |        P        = P1D(I,J)
         72 |        T        = T1D(I,J)
         73 |        Q        = Q1D(I,J)
         74 |        EVP      = P*Q/(EPS+ONEPS*Q)
         75 |        RMX      = EPS*EVP/(P-EVP)
         76 |        CKAPA    = D2845*(1.-D28*RMX)
         77 |        RKAPA    = 1./CKAPA
         78 |        ARG      = EVP*D01
         79 |        ARG      = AMAX1(H1M12,ARG)
         80 |        DENOM    = D35*ALOG(T) - ALOG(EVP*D01) - D4805
         81 |        TLCL     = H2840/DENOM + H55
         82 |        PLCL     = P*(TLCL/T)**RKAPA
         83 |        FAC      = (P1000/P)**CKAPA
         84 |        ETERM    = (D3376/TLCL-D00254)*(RMX*KG2G*(H1+D81*RMX))
         85 |        THETAE   = T*FAC*EXP(ETERM)
         86 |        THTE(I,J)= THETAE
         87 |      ENDDO
         88 |      ENDDO
         89 |C
         90 |C     END OF ROUTINE.
         91 |C
         92 |      RETURN
         93 |      END
 
