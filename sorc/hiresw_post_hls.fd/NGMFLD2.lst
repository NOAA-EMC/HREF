        109 |C
        110 |C********************************************************************
        111 |C     START NGMFLD HERE.
        112 |C
        113 |      CLIMIT =1.0E-20
        114 |C
        115 |C     INITIALIZE ARRAYS.
        116 |!$omp  parallel do
        117 |      DO J=JSTA,JEND
        118 |      DO I=1,IM
        119 |         QM8510(I,J) = D00
        120 |         RH4710(I,J) = D00
        121 |         RH8498(I,J) = D00
        122 |         RH4796(I,J) = D00
        123 |         RH1847(I,J) = D00
        124 |         Z8510(I,J)  = D00
        125 |         Z8498(I,J)  = D00
        126 |         Z4710(I,J)  = D00
        127 |         Z4796(I,J)  = D00
        128 |         Z1847(I,J)  = D00
        129 |         IWL  (I,J)  = D00
        130 |      ENDDO
        131 |      ENDDO
        132 |C
        133 |C     LOOP OVER HORIZONTAL GRID.
        134 |C
        135 |!$omp  parallel do
        136 |!$omp& private(ai,bi,dz,ie,iw,iwl,iwm1,p100,p18,p47,p84,p85,
        137 |!$omp&         p96,p98,pm,qdiv,qi,qint,qk,qkhn,qkhs,qkm1,qm,qm8510,
        138 |!$omp&         qmcvg,qs,qudx,qvdy,qw,r2dx,r2dy,rh,rh1847,rh4710,
        139 |!$omp&         rh4796,rh8498,tm,tmt0,tmt15,z1847,z4710,z4796,
        140 |!$omp&         z8498,z8510)
        141 |      DO L=1,LM
        142 |      CALL EXCH2_P(Q(1,jsta_2l,L))
        143 |      DO J=JSTA_M2,JEND_M2
        144 |      DO I=2,IM-1
        145 |C
        146 |C        SET TARGET PRESSURES.
        147 |         P100  = SIG100*(PD(I,J)+PT)
        148 |         P98   = SIG98*(PD(I,J)+PT)
        149 |         P96   = SIG96*(PD(I,J)+PT)
        150 |         P85   = SIG85*(PD(I,J)+PT)
        151 |         P84   = SIG84*(PD(I,J)+PT)
        152 |         P47   = SIG47*(PD(I,J)+PT)
        153 |         P18   = SIG18*(PD(I,J)+PT)
        154 |C
        155 |C
        156 |C        COMPUTE LAYER MEAN FIELDS AT THE GIVEN K.
        157 |C
        158 |C          COMPUTE P, Z, T, AND Q AT THE MIDPOINT OF THE CURRENT ETA LAYER.
        159 |           ALPM = D50*(ALPINT(I,J,L)+ALPINT(I,J,L+1))
        160 |           DZ   = ZINT(I,J,L)-ZINT(I,J,L+1)
        161 |           PM   = EXP(ALPM)
        162 |           TM   = T(I,J,L)
        163 |           QM   = Q(I,J,L)
        164 |           QM   = AMAX1(QM,H1M12)
        165 |C
        166 |           IWM1=IWL(I,J)
        167 |           IF(CWM(I,J,L).GT.CLIMIT) THEN
        168 |             IF(TM.LT.258.15)THEN
        169 |               IWL(I,J)=1.
        170 |             ELSEIF(TM.GE.273.15)THEN
        171 |               IWL(I,J)=0.
        172 |             ELSE
        173 |               IF(IWM1.EQ.1.0)IWL(I,J)=1.
        174 |             ENDIF
        175 |           ELSE
        176 |             IWL(I,J)=0.
        177 |           ENDIF
        178 |C
        179 |C
        180 |C          COMPUTE RELATIVE HUMIDITY.
        181 |C
        182 |           TMT0=TM-273.16
        183 |           TMT15=AMIN1(TMT0,-15.)
        184 |           AI=0.008855
        185 |           BI=1.
        186 |           IF(TMT0.LT.-20.)THEN
        187 |             AI=0.007225
        188 |             BI=0.9674
        189 |           ENDIF
        190 |           QW=PQ0/PM
        191 |     1          *EXP(A2*(TM-A3)/(TM-A4))
        192 |           QI=QW*(BI+AI*AMIN1(TMT0,0.))
        193 |           QINT=QW*(1.-0.00032*TMT15*(TMT15+15.))
        194 |           IF(TMT0.LT.-15.)THEN
        195 |               QS=QI
        196 |           ELSEIF(TMT0.GE.0.)THEN
        197 |               QS=QINT
        198 |           ELSE
        199 |             IF(IWL(I,J).GT.0.0) THEN
        200 |               QS=QI
        201 |             ELSE
        202 |               QS=QINT
        203 |             ENDIF
        204 |           ENDIF
        205 |CMEB 12/22/98 SWITCH TO RH VS WATER NO MATTER WHAT
        206 |C             DELETE THIS LINE TO SWITCH BACK TO RH VS ICE
        207 |            QS=QW
        208 |CMEB 12/22/98 SWITCH TO RH VS WATER NO MATTER WHAT
        209 |
        210 |C
        211 |           RH   = QM/QS
        212 |           IF (RH.GT.H1) THEN
        213 |              RH = H1
        214 |              QM = RH*QS
        215 |           ENDIF
        216 |           IF (RH.LT.D01) THEN
        217 |              RH = D01
        218 |              QM = RH*QS
        219 |           ENDIF
        220 |C
        221 |C          COMPUTE MOISTURE CONVERGENCE.  WE NEED Q AT V POINTS.
        222 |           QMCVG = D00
        223 |           IE=I+IHE(J)
        224 |           IW=I+IHW(J)
        225 |           QK=D25*(Q(I+1,J,L)+Q(I,J,L)+Q(IE,J+1,L)+Q(IE,J-1,L))
        226 |           QKM1=D25*(Q(I,J,L)+Q(I-1,J,L)+Q(IW,J+1,L)+Q(IW,J-1,L))
        227 |           QKHN=D25*(Q(IW,J+1,L)+Q(IE,J+1,L)+Q(I,J+2,L)+Q(I,J,L))
        228 |           QKHS=D25*(Q(IW,J-1,L)+Q(IE,J-1,L)+Q(I,J,L)+Q(I,J-2,L))
        229 |           R2DX =1./(2.*DX(I,J))
        230 |           R2DY =1./(2.*DY)
        231 |           QUDX =(QK*U(IE,J,L)-QKM1*U(IW,J,L))*R2DX
        232 |           QVDY =(QKHN*V(I,J+1,L)-QKHS*V(I,J-1,L))*R2DY
        233 |           QDIV =QUDX+QVDY
        234 |           QMCVG=-1.*QDIV*HBM2(I,J)
        235 |C
        236 |C          SIGMA 0.85-1.00 MOISTURE CONVERGENCE.
        237 |           IF ((PM.LE.P100).AND.(PM.GE.P85)) THEN
        238 |              Z8510(I,J)  = Z8510(I,J) + DZ
        239 |              QM8510(I,J) = QM8510(I,J) + QMCVG*DZ
        240 |           ENDIF
        241 |C
        242 |C          SIGMA 0.47-1.00 RELATIVE HUMIDITY.
        243 |           IF ((PM.LE.P100).AND.(PM.GE.P47)) THEN
        244 |              Z4710(I,J)  = Z4710(I,J) + DZ
        245 |              RH4710(I,J) = RH4710(I,J) + RH*DZ
        246 |           ENDIF
        247 |C
        248 |C          SIGMA 0.84-0.98 RELATIVE HUMIDITY.
        249 |           IF ((PM.LE.P98).AND.(PM.GE.P84)) THEN
        250 |              Z8498(I,J)  = Z8498(I,J) + DZ
        251 |              RH8498(I,J) = RH8498(I,J) + RH*DZ
        252 |           ENDIF
        253 |C
        254 |C          SIGMA 0.47-0.96 RELATIVE HUMIDITY.
        255 |           IF ((PM.LE.P96).AND.(PM.GE.P47)) THEN
        256 |              Z4796(I,J)  = Z4796(I,J) + DZ
        257 |              RH4796(I,J) = RH4796(I,J) + RH*DZ
        258 |           ENDIF
        259 |C
        260 |C          SIGMA 0.18-0.47 RELATIVE HUMIDITY.
        261 |           IF ((PM.LE.P47).AND.(PM.GE.P18)) THEN
        262 |              Z1847(I,J)  = Z1847(I,J) + DZ
        263 |              RH1847(I,J) = RH1847(I,J) + RH*DZ
        264 |           ENDIF
        265 |C
        266 |      ENDDO
        267 |      ENDDO
        268 |      ENDDO
        269 |C
        270 |      DO J=JSTA_M2,JEND_M2
        271 |      DO I=2,IM-1
        272 |C        NORMALIZE TO GET LAYER MEAN VALUES.
        273 |         IF (Z8510(I,J).GT.0) THEN
        274 |            QM8510(I,J) = QM8510(I,J)/Z8510(I,J)
        275 |         ELSE
        276 |            QM8510(I,J) = SPVAL
        277 |         ENDIF
        278 |         IF (ABS(QM8510(I,J)-SPVAL).LT.SMALL)QM8510(I,J)=H1M12
        279 |C
        280 |         IF (Z4710(I,J).GT.0) THEN
        281 |            RH4710(I,J) = RH4710(I,J)/Z4710(I,J)
        282 |         ELSE
        283 |            RH4710(I,J) = SPVAL
        284 |         ENDIF
