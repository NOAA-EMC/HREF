  2     278 |      integer*4 MPI_TYPECLASS_COMPLEX
  2     279 |      parameter(MPI_TYPECLASS_REAL=1,MPI_TYPECLASS_INTEGER=2)
  2     280 |      parameter(MPI_TYPECLASS_COMPLEX=3)
