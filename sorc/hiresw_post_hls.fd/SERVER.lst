  2     282 |      integer*4 MP_BW_MPI, MP_BW_LAPI
  2     283 |      parameter(MP_BW_MPI=2,MP_BW_LAPI=1)
         41 |      LOGICAL :: DONE, NEWFILE
         42 |      INTEGER :: STATUS(MPI_STATUS_SIZE)
         43 |      INTEGER :: IERR, COUNT, LUN
         44 |      CHARACTER*80 :: FNAME
         45 |      CHARACTER*1, ALLOCATABLE :: BUF(:)
         46 |C
         47 |C---------------------------------------------------------------------
         48 |C
         49 |C     THIS CODE IS EXPECTING THE FOLLOWING MESSAGE STRUCTURE
         50 |C
         51 |C     VARIABLE     TYPE           DESCRIPTION     TAG
         52 |C=====================================================
         53 |C     DONE         LOGICAL        ARE WE DONE?    1
         54 |C     NEWFILE      LOGICAL        OPEN THE FILE?  2
         55 |C     LUN          INTEGER        FORTRAN UNIT #  3
         56 |C     FNAME        CHARACTER*80   FILE NAME       4
         57 |C     BUF          CHARACTER*1(*) BURF RECORD     5
         58 |C
         59 |C---------------------------------------------------------------------
         60 |C
         61 |      PRINT *, ' STARTING UP IO SERVER ...'
         62 |666   CONTINUE
         63 |C
         64 |C     THE FIRST MESSAGE IS A LOGICAL TO TELL US WHETHER WE ARE
         65 |C     FINISHED OR NOT
         66 |C
         67 |      CALL MPI_RECV(DONE,1,MPI_LOGICAL,
         68 |     *              0,1,MPI_COMM_INTER,STATUS,IERR)
         69 |C
         70 |      IF ( DONE ) THEN
         71 |         PRINT *, ' SHUTTING DOWN IO SERVER ...'
         72 |         RETURN   !    RETURNING TO MAIN
         73 |      END IF
         74 |C
         75 |C     DO WE NEED TO OPEN THE FILE ?
         76 |C
         77 |      CALL MPI_RECV(NEWFILE,1,MPI_LOGICAL,
         78 |     *              0,2,MPI_COMM_INTER,STATUS,IERR)
         79 |C
         80 |C     FORTRAN UNIT NUMBER
         81 |C
         82 |      CALL MPI_RECV(LUN,1,MPI_INTEGER,
         83 |     *              0,3,MPI_COMM_INTER,STATUS,IERR)
         84 |C
         85 |C     FILENAME
         86 |C
         87 |      CALL MPI_RECV(FNAME,80,MPI_CHARACTER,
         88 |     *              0,4,MPI_COMM_INTER,STATUS,IERR)
         89 |C
         90 |C     OPEN THE FILE, IF NECESSARY
         91 |C
         92 |      IF ( NEWFILE ) THEN
         93 |          CLOSE(LUN)
         94 |          CALL BAOPEN(LUN,FNAME,IER)
         95 |          PRINT *, ' FILE ',FNAME,' OPENED AS UNIT ',LUN
         96 |      END IF
         97 |C
         98 |C     DETERMINE THE SIZE OF THE BUFR RECORD AND ALLOCATE A BUFFER FOR IT
         99 |C
        100 |      CALL MPI_PROBE(0,5,MPI_COMM_INTER,STATUS,IERR)
        101 |      CALL MPI_GET_COUNT(STATUS,MPI_CHARACTER,COUNT,IERR)
        102 |      ALLOCATE( BUF( COUNT ) )
        103 |C
        104 |C     FINALLY, GET THE BUFR RECORD
        105 |C
        106 |      CALL MPI_RECV(BUF,COUNT,MPI_CHARACTER,
        107 |     *              0,5,MPI_COMM_INTER,STATUS,IERR)
        108 |C
        109 |C     OUT TO DISK WE GO ...
        110 |C
        111 |      CALL WRYTE(LUN,COUNT,BUF)
        112 |      DEALLOCATE(BUF)
        113 |      GOTO 666
        114 |      END
 
