          1 |      SUBROUTINE NOPACK(IFLD,ILVL,GRID,IMOUT,JMOUT)
          2 |C
          3 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          4 |C                .      .    .
          5 |C SUBPROGRAM:    NOPACK      POST FIELDS IN CRAY BINARY
          6 |C   PRGRMMR: TREADON         ORG: W/NP2
          7 |C
          8 |C ABSTRACT:
          9 |C     THIS ROUTINE POSTS THE DATA IN THE PASSED ARRAY GRID
         10 |C     TO THE OUTPUT FILE AS AN UNFORMATTED CRAY BINARY FILE.
         11 |C   .
         12 |C
         13 |C PROGRAM HISTORY LOG:
         14 |C   92-12-22  RUSS TREADON
         15 |C
         16 |C USAGE:    CALL NOPACK(IFLD,ILVL,GRID,IMOUT,JMOUT)
         17 |C   INPUT ARGUMENT LIST:
         18 |C     IFLD     - FIELD ID TAG.
         19 |C     ILVL     - INTEGER TAG FOR LEVEL OF FIELD.
         20 |C     GRID     - FIELD TO BE POSTED IN CRAY BINARY.
         21 |C     IMOUT    - FIRST DIMENSION OF OUTPUT GRID.
         22 |C     JMOUT    - SECOND DIMENSION OF OUTPUT GRID.
         23 |C
         24 |C   OUTPUT ARGUMENT LIST:
         25 |C
         26 |C   OUTPUT FILES:
         27 |C     STDOUT  - RUN TIME STANDARD OUT.
         28 |C     LUNOUT  - UNIT DATA IS POSTED TO.
         29 |C
         30 |C   SUBPROGRAMS CALLED:
         31 |C     UTILITIES:
         32 |C       MINMAX   - DETERMINES MIN/MAX VALUES IN AN ARRAY.
         33 |C     LIBRARY:
         34 |C       COMMON   - CTLBLK
         35 |C                  MAPOT
         36 |C                  RQSTFLD
         37 |C                  IOUNIT
         38 |C                  OUTGRD
         39 |C
         40 |C   ATTRIBUTES:
         41 |C     LANGUAGE: FORTRAN
         42 |C     MACHINE : CRAY C-90
         43 |C$$$
         44 |C
         45 |C
         46 |C
         47 |C     INCLUDE ETA GRID DIMENSIONS.  SET/DERIVE PARAMETERS.
         48 |C
         49 |      INCLUDE "parmeta"
  1       1 |C-------------------------------------------------------------
  1       2 |C***  SET PRIMARY GRID DIMENSIONS AND PRESSURE OUTPUT LEVELS
  1       3 |      PARAMETER (IM=89,JM=143,LM=60,LSM=39)
  1       4 |C-------------------------------------------------------------
  1       5 |C***  SET THE NUMBER OF PEs IN THE I-DIRECTION AND J-DIRECTION
  1       6 |      PARAMETER(INPES=5,JNPES=6)
  1       7 |C-------------------------------------------------------------
  1       8 |C
  1       9 |      PARAMETER(IGSTL=-5,IGSTR=5)
  1      10 |      PARAMETER(JGSTL=-5,JGSTR=5)
  1      11 |      PARAMETER(ITAIL=IM-(INPES*(IM/INPES)))
  1      12 |      PARAMETER(JTAIL=JM-(JNPES*(JM/JNPES)))
  1      13 |C
  1      14 |      PARAMETER(IDIM1=IGSTL,IDIM2=IM/INPES+IGSTR+1)
  1      15 |      PARAMETER(JDIM1=JGSTL,JDIM2=JM/JNPES+JGSTR+1)
  1      16 |C
  1      17 |C-------------------------------------------------------------
         50 |      INCLUDE "parmout"
  2       1 |C     MAXIMUM DIMENSIONS FOR OUTPUT GRID, FIELDS, AND LEVELS.
  2       2 |C
  2       3 |      PARAMETER(IMX=IM,JMX=JM,MXFLD=250,MXLVL=LM)
         51 |      PARAMETER (LP1=LM+1,D50=0.5E0)
         52 |C
         53 |C     DECLARE VARIABLES.
         54 |C
         55 |      LOGICAL RUN,FIRST,RESRT,SIGMA,OLDRD,STDRD
         56 |      LOGICAL NORTH,HEADER
         57 |      CHARACTER*4  RESTHR
         58 |      CHARACTER*6  PROJ,BLANK
         59 |      CHARACTER*8  DESCR2
         60 |      CHARACTER*50 ENVAR
         61 |      CHARACTER*80 FNAME
         62 |      REAL GRID(IMOUT,JMOUT)
         63 |C
         64 |C     INCLUDE COMMON BLOCKS.
         65 |      INCLUDE "CTLBLK.comm"
  3       1 |C-----------------------------------------------------------------------
  3       2 |      COMMON /CTLBLK/
  3       3 |     & SIGMA,RUN,IDAT(3),IHRST,FIRST,RESTRT,NFCST,NBC,LIST,IOUT
  3       4 |     &,DT,NTSD,NTSTM,NDDAMP,NPREC,IDTAD,NBOCO,NSHDE,NCP
  3       5 |     &,NPHS,NRADS,NRADL
  3       6 |      INTEGER NUM_PROCS, ME, JSTA, JEND,
  3       7 |     &        JSTA_M, JSTA_M2, JEND_M, JEND_M2, IUP, IDN, ICNT, IDSP,
  3       8 |     &        JSTA_2L, JEND_2U
  3       9 |      COMMON /PARALLEL/NUM_PROCS, ME, JSTA, JEND, JSTA_M, JEND_M,
  3      10 |     &                 JSTA_M2, JEND_M2, IUP, IDN,
  3      11 |     &                 ICNT(0:1023), IDSP(0:1023),
  3      12 |     &                 JSTA_2L, JEND_2U, NUM_SERVERS, MPI_COMM_INTER,
  3      13 |     &                 MPI_COMM_COMP
  3      14 |C-----------------------------------------------------------------------
         66 |      INCLUDE "MAPOT.comm"
  4       1 |C-----------------------------------------------------------------------
  4       2 |                             C O M M O N /MAPOT/
  4       3 |     & LSL,IXM,IYM
  4       4 |     &,ISHDE(99)
  4       5 |     &,R,PT,TSPH
  4       6 |     &,WBD,SBD,TLM0D,TPH0D,DLMD,DPHD,CMLD,DP30,X1P,Y1P
  4       7 |     &,DISLP,Z0SLP,SPL(LSM),ALSL(LSM)
  4       8 |     &,DETA(LM),AETA(LM),ETA(LP1),TSHDE(99)
  4       9 |     &,ERLAM0,CPHI0,SPHI0
  4      10 |     &,KUNIT,LUNIT,MUNIT,IUNIT,NUNIT
  4      11 |     &,DSG1(LM),SGML1(LM),SG1(LP1),PDTOP
  4      12 |     &,DSG2(LM),SGML2(LM),SG2(LP1)
  4      13 |C-----------------------------------------------------------------------
         67 |      INCLUDE "IOUNIT.comm"
  5       1 |C
  5       2 |      INTEGER STDOUT
  5       3 |C
  5       4 |      COMMON /IOUNIT/
  5       5 |     & STDOUT, LCNTRL, LUNCO, LUNLL, LUNOUT
  5       6 |C
         68 |      INCLUDE "RQSTFLD.comm"
  6       1 |C
  6       2 |      CHARACTER*20 AVBL(MXFLD),FIELD(MXFLD)
  6       3 |      CHARACTER*6 DATSET
  6       4 |C
  6       5 |      LOGICAL RITEHD,RITE2
  6       6 |C
  6       7 |      COMMON /RQSTFLD/
  6       8 |     & KGTYPE,IOUTYP,SVALUE,NFLD,IGET(MXFLD),DEC(MXFLD)
  6       9 |     &,IQ(MXFLD),IS(MXFLD),ISMSTG(MXFLD),ISMFUL(MXFLD)
  6      10 |     &,ISMOUT(MXFLD),LVLS(MXLVL,MXFLD),IDENT(MXFLD)
  6      11 |     &,IFILV(MXFLD),ID(25)
  6      12 |     &,RITEHD,RITE2,DATSET,FIELD,AVBL
         69 |      INCLUDE "OUTGRD.comm"
  7       1 |C
  7       2 |      COMMON /OUTGRD/ ALATVT,ALONVT,POLEI,POLEJ,XMESHL,
  7       3 |     X	   IGOUT,JGOUT,IMDLTY,RE,GI2,NORTH,PROJ
         70 |      DATA BLANK/'      '/
         71 |C
         72 |C
         73 |C****************************************************************************
         74 |C     START NOPACK HERE.
         75 |C
         76 |C     SET OUTPUT GRID TYPE.  WE ASSUME KGYTPE HOLDS THE ON84
         77 |C     ID FOR THE OUTPUT GRID.
         78 |C     UNIQUE ON84 LABELS FOR VARIOUS ETA DOMAINS BELOW:
         79 |C              90 = STAGGERED 80KM E-GRID
         80 |C              91 = FILLED    80KM E-GRID
         81 |C              92 = STAGGERED 40KM E-GRID
         82 |C              93 = FILLED    40KM E-GRID
         83 |C              94 = STAGGERED 30KM E-GRID
         84 |C              95 = FILLED    30KM E-GRID
         85 |C              96 = STAGGERED "BIG" 40KM E-GRID (EDAS)
         86 |C
         87 |      KGTYP = KGTYPE
         88 |C     IF NEED BE, OPEN A UNIT TO THE OUTPUT FILE.  IF REQUESTED,
         89 |C     WRITE DATE AND GRID INFO IF REQUESTED.
         90 |C
         91 |      IF (RITEHD) THEN
         92 |C
         93 |C        APPEND FORECAST HOUR TO SUFFIX FOR SEQUENTIAL BINARY FILE.
         94 |         IHR = NTSD/TSPH + 0.5
         95 |C
         96 |C        GET FULL PATH FOR OUTPUT FILE FROM ENVIRONMENT VARIABLE
         97 |C        COMSP WHICH IS SET IN THE SCRIPT RUNNING THE MODEL.
         98 |C
         99 |C        CONSTRUCT FULL PATH-FILENAME FOR OUTPUT FILE
        100 |         ENVAR = ' '
        101 |         CALL GETENV('COMSP',ENVAR)
        102 |         IF (ENVAR(1:6).EQ.BLANK) THEN
        103 |          WRITE(DESCR2,1000) IHR
        104 | 1000     FORMAT('.SbinF',I2.2)
        105 |          KDAT = INDEX(DATSET,' ') -1
        106 |          IF (KDAT.LE.0) KDAT = LEN(DATSET)
        107 |          FNAME = DATSET(1:KDAT) // DESCR2
        108 |         ELSE
        109 |          CALL GETENV('tmmark',RESTHR)
        110 |          WRITE(DESCR2,1011) IHR
        111 | 1011     FORMAT(I2.2)
        112 |          KENV = INDEX(ENVAR,' ') -1
        113 |          IF (KENV.LE.0) KENV = LEN(ENVAR)
        114 |          KDAT = INDEX(DATSET,' ') -1
        115 |          IF (KDAT.LE.0) KDAT = LEN(DATSET)
        116 |          FNAME = ENVAR(1:KENV) // DATSET(1:KDAT) // DESCR2(1:2)
        117 |     &              //'.'// RESTHR
        118 |         ENDIF
        119 |C
        120 |C        ASSIGN AND OPEN UNIT FOR DATA FILE.
        121 |         CLOSE(LUNOUT)
        122 |C        CALL ASNUNIT(LUNOUT,'-F cos -N ibm',ier)
        123 |C        IF (IER.NE.0) WRITE(STDOUT,*)
        124 |C    X        'NOPACK:  ASNUNIT ERROR IER=',IER
        125 |         OPEN(LUNOUT,FILE=FNAME,FORM='UNFORMATTED')
        126 |         REWIND(LUNOUT)
        127 |C
        128 |         RITEHD = .FALSE.
        129 |         WRITE(STDOUT,*)'NOPACK:  OPENED ',LUNOUT,' ',FNAME,
        130 |     X        ' ',DATSET
        131 |C
        132 |C        IF REQUESTED, THE FIRST TWO "RECORDS" INCLUDE DATE
        133 |C        AND OUTPUT GRID INFORMATION.  WE WRITE THEM ONLY
        134 |C        ONCE TO EACH OUTPUT GRID.
        135 |C
        136 |         IF (INDEX(DATSET,'NOHEAD').EQ.0) THEN
        137 |            IHH = NTSD/TSPH+D50
        138 |            IYY = IDAT(3)-1900
        139 |            IMM = IDAT(1)
        140 |            IDD = IDAT(2)
        141 |            WRITE(LUNOUT) IHRST,IMM,IDD,IYY,IHH
        142 |            WRITE(LUNOUT) KGTYP,PROJ,NORTH,IMOUT,JMOUT,
        143 |     X           POLEI,POLEJ,ALATVT,ALONVT,XMESHL
        144 |            WRITE(STDOUT,*)'NOPACK:  WROTE DATE AND GRID INFO'
        145 |            WRITE(STDOUT,1010) IHRST,IMM,IDD,IYY,IHH
        146 |            WRITE(STDOUT,1020) KGTYP,PROJ,NORTH,IMOUT,JMOUT
        147 |            WRITE(STDOUT,1030) POLEI,POLEJ,ALATVT,ALONVT
        148 |            WRITE(STDOUT,1030) XMESHL
        149 | 1010       FORMAT(5(I5,1X))
        150 | 1020       FORMAT(I5,1X,A6,1X,L1,1X,I5,1X,I5)
        151 | 1030       FORMAT(4(G12.6,1X))
        152 |         ENDIF
        153 |      ENDIF
        154 |C
        155 |C     SET SURFACE VALUE FOR DATA.
        156 |C
        157 |      SFC = 0.
        158 |      IF (IS(IDENT(IFLD)).EQ.8) THEN
        159 |         SFC = SPL(ILVL)
        160 |      ELSEIF (IS(IDENT(IFLD)).EQ.149) THEN
        161 |C
        162 |C   TKE IS ON THE ETA INTERFACE AT THE BOTTOM OF THE LAYER ILVL
        163 |C
        164 |         SFC = AETA(ILVL)
        165 |         IF (IQ(IDENT(IFLD)).EQ.158) SFC = ETA(ILVL+1)
        166 |      ENDIF
        167 |C
        168 |C     IF DATSET CONTAINS "NOHEAD" WE SKIP THE HEADER STUFF
        169 |C     THAT FOLLOWS AND GO DIRECTLY TO THE DATA.  A USER
        170 |C     WOULD USED THIS OPTION IF (S)HE WANTS TO DIRECTLY
        171 |C     CONTOUR DATA USING NMCIDAS.
        172 |C
        173 |      HEADER = .FALSE.
        174 |      IF (INDEX(DATSET,'NOHEAD').NE.0) GOTO 10
        175 |      WRITE(LUNOUT) FIELD(IFLD),SFC
        176 |      HEADER = .TRUE.
        177 |C
        178 |C     WRITE DATA TO OUTPUT FILE.
        179 |C
        180 | 10   CONTINUE
        181 |      WRITE(LUNOUT) GRID
        182 |      CALL MINMAX(GRID,IMOUT,JMOUT,FMIN,FMAX)
        183 |      WRITE(STDOUT,1040) IFLD,FIELD(IFLD),SFC,FMIN,FMAX,
        184 |     X     KGTYP,HEADER
        185 | 1040 FORMAT('NOPACK:',I3,1X,A20,1X,3(G12.6,1X),I5,1X,L1)
        186 |C
        187 |C     END OF ROUTINE.
        188 |C
        189 |      RETURN
        190 |      END
 
