        132 |C***  BLOCK 1.  SOUNDING DERIVED FIELDS.
        133 |C
        134 |C     ETA SURFACE TO 500MB LIFTED INDEX.  TO BE CONSISTENT WITH THE
        135 |
        136 |C     LFM AND NGM POSTING WE ADD 273.15 TO THE LIFTED INDEX
        137 |C     NOTE:  25 JUNE 1993, RUSS TREADON.
        138 |C               ON THE LFM FORECAST GRID (026) WE POST
        139 |C               THE FIRST BOUNDARY LAYER LIFTED INDEX.
        140 |C               SEE SUBROUTINE MISCLN.
        141 |C
        142 |C     THE BEST (SIX LAYER) AND BOUNDARY LAYER LIFTED INDICES ARE
        143 |C     COMPUTED AND POSTED IN SUBROUTINE MISCLN.
        144 |C
        145 |      IF ( (IGET(030).GT.0).AND.(KGTYPE.NE.026) ) THEN
        146 |         DO J=JSTA,JEND
        147 |         DO I=1,IM
        148 |           EGRID1(I,J) = SPVAL
        149 |         ENDDO
        150 |         ENDDO
        151 |C
        152 |         CALL OTLIFT2(T500,EGRID1)
        153 |C
        154 |         DO J=JSTA,JEND
        155 |         DO I=1,IM
        156 |           EGRID1(I,J) = EGRID1(I,J) + C2K
        157 |         ENDDO
        158 |         ENDDO
        159 |C
        160 |         CALL E2OUT(030,000,EGRID1,EGRID2,GRID1,GRID2,IMOUT,JMOUT)
        161 |         ID(1:25)=0
        162 |         ID(10)  =50
        163 |         ID(11)  =100
        164 |         CALL OUTPUT(IOUTYP,IGET(030),LVLS(1,IGET(030)),
        165 |     X        GRID1,IMOUT,JMOUT)
        166 |      ENDIF
        167 |C
        168 |C     SOUNDING DERIVED AREA INTEGRATED ENERGIES - CAPE AND CIN.
        169 |      IF ((IGET(032).GT.0).OR.(IGET(107).GT.0)) THEN
        170 |         IF ( (LVLS(1,IGET(032)).GT.0) .OR.
        171 |     X        (LVLS(1,IGET(107)).GT.0) ) THEN
        172 |            ITYPE = 1
        173 |            CALL CALCAPE(ITYPE,P1D,T1D,Q1D,L1D,EGRID1,EGRID2)
        174 |C
        175 |C           CONVECTIVE AVAILABLE POTENTIAL ENERGY.
        176 |            IF (IGET(032).GT.0) THEN
        177 |               CALL E2OUT(032,000,EGRID1,EGRID2,GRID1,GRID2,IMOUT,JMOUT)
        178 |               CALL BOUND(GRID1,D00,H99999,IMOUT,JMOUT)
        179 |               ID(1:25)=0
        180 |               CALL OUTPUT(IOUTYP,IGET(032),LVLS(1,IGET(032)),
        181 |     X              GRID1,IMOUT,JMOUT)
        182 |            ENDIF
        183 |C
        184 |C           CONVECTIVE INHIBITION.
        185 |            IF (IGET(107).GT.0) THEN
        186 |               CALL E2OUT(107,000,EGRID2,EGRID1,GRID1,GRID2,IMOUT,JMOUT)
        187 |               DO J=JSTA,JEND
        188 |               DO I=1,IMOUT
        189 |                 GRID1(I,J) = -1.*GRID1(I,J)
        190 |               ENDDO
        191 |               ENDDO
        192 |               CALL BOUND(GRID1,D00,H99999,IMOUT,JMOUT)
        193 |               DO J=JSTA,JEND
        194 |               DO I=1,IMOUT
        195 |                 GRID1(I,J) = -1.*GRID1(I,J)
        196 |               ENDDO
        197 |               ENDDO
        198 |               ID(1:25)=0
        199 |               CALL OUTPUT(IOUTYP,IGET(107),LVLS(1,IGET(107)),
        200 |     X              GRID1,IMOUT,JMOUT)
        201 |            ENDIF
        202 |         ENDIF
        203 |      ENDIF
