  1     271 |      parameter (MPI_COMBINER_HINDEXED=8,MPI_COMBINER_INDEXED_BLOCK=9)
  1     272 |      parameter (MPI_COMBINER_STRUCT_INTEGER=10,MPI_COMBINER_STRUCT=11)
  1     273 |      parameter (MPI_COMBINER_SUBARRAY=12,MPI_COMBINER_DARRAY=13)
  1     274 |      parameter (MPI_COMBINER_F90_REAL=14,MPI_COMBINER_F90_COMPLEX=15)
  1     275 |      parameter (MPI_COMBINER_F90_INTEGER=16,MPI_COMBINER_RESIZED=17)
  1     276 |
  1     277 |      integer*4 MPI_TYPECLASS_REAL,MPI_TYPECLASS_INTEGER
  1     278 |      integer*4 MPI_TYPECLASS_COMPLEX
  1     279 |      parameter(MPI_TYPECLASS_REAL=1,MPI_TYPECLASS_INTEGER=2)
  1     280 |      parameter(MPI_TYPECLASS_COMPLEX=3)
  1     281 |
  1     282 |      integer*4 MP_BW_MPI, MP_BW_LAPI
  1     283 |      parameter(MP_BW_MPI=2,MP_BW_LAPI=1)
         62 |      integer comdup
         63 |      integer, allocatable :: irank ( : )
         64 |      integer  INUMQ(*)
         65 |      logical yes
         66 |      character*4 get
         67 |C-----------------------------------------------------------------------
         68 |C
         69 |C     INITIALIZE MPI
         70 |C     RETRIEVE THE NUMBER OF TOTAL MPI TASKS AND MY RANK
         71 |C
         72 |      call mpi_init(ierr)
         73 |      call mpi_comm_rank(MPI_COMM_WORLD,mype,ierr)
         74 |      call mpi_comm_size(MPI_COMM_WORLD,npes,ierr)
         75 |C
         76 |C     SPECIFY ONE I/O SERVER AS LONG AS THERE ARE MORE THAN 1 MPI TASK
         77 |C
         78 |      if ( npes .gt. 1 ) then
         79 |         npes_mod = npes - 1
         80 |      else
         81 |         npes_mod = 1
         82 |      end if
         83 |C
         84 |C     AT THIS POINT NPES IS THE TOTAL NUMBER OF MPI TASKS. WE WILL
         85 |C     RESET THIS AT THE END OF THE SUBROUTINE TO THE NUMBER OF MPI
         86 |C     TASKS THAT ARE WORKING ON THE POSTING
         87 |C
         88 |C     FIRST, HOWEVER, WE NEED TO MAKE SURE THAT A SUFFICIENT NUMBER
         89 |C     OF MPI TASKS HAVE BEEN INITIATED. IF NOT, WE WILL STOP.
         90 |C
         91 |      IF ( NPES .LT. NPES_MOD ) THEN
         92 |         PRINT *, ' ***********************************************'
         93 |         PRINT *, ' ***********************************************'
         94 |         PRINT *, ' *************MAJOR PROBLEM*********************'
         95 |         PRINT *, ' *************MAJOR PROBLEM*********************'
         96 |         PRINT *, ' *************MAJOR PROBLEM*********************'
         97 |         PRINT *, ' *************MAJOR PROBLEM*********************'
         98 |         PRINT *
         99 |         PRINT *, ' THERE ARE INSUFFICIENT MPI TASKS TO CONTINUE'
        100 |         PRINT *, ' YOU MUST SPECIFY AT LEAST ',NPES_MOD,' TASKS'
        101 |         PRINT *, ' STOPPING NOW'
        102 |         PRINT *, ' HASTA LA VISTA BABY'
        103 |         PRINT *
        104 |         PRINT *, ' *************MAJOR PROBLEM*********************'
        105 |         PRINT *, ' *************MAJOR PROBLEM*********************'
        106 |         PRINT *, ' *************MAJOR PROBLEM*********************'
        107 |         PRINT *, ' *************MAJOR PROBLEM*********************'
        108 |         PRINT *, ' ***********************************************'
        109 |         PRINT *, ' ***********************************************'
        110 |         CALL MPI_ABORT(MPI_COMM_WORLD,1,IERR)
        111 |      END IF
        112 |C
        113 |C     OK, WE HAVE A SUFFICIENT NUMBER OF MPI TASKS TO CONTINUE
        114 |C
        115 |C     HOW MANY GROUPS OF SERVERS ? THE DEFAULT IS 1 GROUP
        116 |C     THE ENVIRONMENT VARIABLE, SERVER_GROUPS, CAN BE USED TO
        117 |C     SPECIFY MORE SERVER GROUPS
        118 |C
        119 |      get = '1'
        120 |cwas  call getenv('SERVER_GROUPS',get)
        121 |      read(get,fmt='(i4)') iquilt_group
        122 |      iquilt_group = max(iquilt_group,1)
        123 |C
        124 |C     ERROR CHECK NUMBER OF GROUPS - THE MAXIMUM IS 100 - THIS IS A LOT
        125 |C
        126 |      if ( iquilt_group .gt. 100 ) then
        127 |         print *, ' ***** IQUILT_GROUP IS GREATER THAN 100'
        128 |         print *, ' ***** DO YOU REALLY WANT THIS ?'
        129 |         print *, ' ***** IF SO THEN INCREASE SIZE IN mpp.h'
        130 |         print *, ' ***** ALSO, CHANGE IF CHECK IN SETUP_SERVERS'
        131 |         print *, ' ***** RESETTING THE NUMBER OF SERVER GROUPS TO 100'
        132 |         print *, ' ***** WE ARE CONTINUING ....   '
        133 |         iquilt_group = 100
        134 |      end if
        135 |      if ( mype .eq. 0 ) then
        136 |      print *, ' we will try to run with ',iquilt_group,' server groups'
        137 |      end if
        138 |C
        139 |C     COMPUTE THE NUMBER OF SERVERS PER GROUP
        140 |C     ALL MPI TASKS BEYOND NPES_MOD WILL BE SERVERS
        141 |C     IF THE NUMBER OF SERVERS IS NOT EQUALLY DIVISIBLE BY
        142 |C     THE NUMBER OF GROUPS OF SERVERS THEN SOME GROUPS MAY HAVE
        143 |C     MORE SERVERS THEN OTHERS - THIS IS FINE
        144 |C     NOTE THAT WE REQUIRE AT LEAST ONE SERVER PER GROUP
        145 |C     WE MAY NEED TO REDUCE THE NUMBER OF SERVER GROUPS IF
        146 |C     IT EXCEEDS THE NUMBER OF SERVERS
        147 |C
        148 |      iqserver = NPES - NPES_MOD
        149 |      if ( iqserver .eq. 0 ) then
        150 |         if ( mype .eq. 0 ) then
        151 |           print *, ' *** you specified 0 I/O servers '
        152 |           print *, ' CHKOUT will write a file'
        153 |         end if
        154 |         iquilt_group = 0
        155 |      end if
        156 |      if ( iquilt_group .gt. iqserver )  then
        157 |          iquilt_group = iqserver
        158 |          print *, ' ***** NOT ENOUGH SERVERS'
        159 |          print *, ' ***** WE NEED TO REDUCE THE NUMB OF SERVER GROUPS'
        160 |          print *, ' ***** NUMB OF SERVER GROUPS IS ', iquilt_group
        161 |      end if
        162 |      do i = 0, iquilt_group - 1
        163 |         call para_range(1,iqserver,iquilt_group,i,istaq,iendq)
        164 |         inumq(i+1) = iendq-istaq+1
        165 |      if ( mype .eq. 0 ) print *, ' i, inumq = ',i+1,inumq(i+1)
        166 |      end do
        167 |C
        168 |C     SETUP THE "COLOR" FOR MPI_COMM_SPLIT
        169 |C     THOSE TASKS WHICH WILL DO MODEL INTEGRATION WILL BE COLOR 0
        170 |C     THE SERVER TASKS WILL HAVE THE COLOR OF THE GROUP NUMBER THAT
        171 |C     THEY WILL BELONG
        172 |C
        173 |      if ( mype .lt. NPES_MOD ) then
        174 |         icolor = 0
        175 |      else
        176 |         istaxx = NPES_MOD
        177 |         do i = 1, iquilt_group
        178 |            iendxx = istaxx + inumq(i) - 1
        179 |            if ( mype .ge. istaxx .and. mype .le. iendxx ) then
        180 |               icolor = i
        181 |            end if
        182 |            istaxx = iendxx + 1
        183 |         end do
        184 |      end if
        185 |C
        186 |C     SPLIT THE COMMUNICATOR - THE NEW INTRACOMMUNICATOR FOR ALL TASKS
        187 |C     IS MPI_COMM_COMP. MPI_COMM_WORLD IS STILL AVAILABLE BUT IT DOES
        188 |C     REFER TO ALL THE MPI TASKS ( MODEL INTEGRATION AND I/O SERVING )
        189 |C
        190 |      call mpi_comm_dup(MPI_COMM_WORLD,comdup,ierr)
        191 |      call mpi_comm_split(comdup,icolor,mype,mpi_comm_comp,ierr)
        192 |C
        193 |C     AT THIS POINT WE HAVE A NEW COMMUNICATOR, MPI_COMM_COMP,
        194 |C     THAT CAN BE USED BY THE FORECASTS TASKS AND THE I/O SERVER TASKS
        195 |C     FOR THEIR INTERNAL COMMUNICATIONS. ONTO THE INTERCOMMUNICATORS ...
        196 |C
        197 |C     NOW WE MUST CREATE THE INTERCOMMUNICATORS FOR USE BETWEEN THE MPI
        198 |C     TASKS DOING THE MODEL INTEGRATION AND THE MPI TASKS FOR EACH
        199 |C     SERVER GROUP. THE FIRST STEP IS TO EXCLUDE THE TASKS THAT DONT
        200 |C     BELONG. WE WILL DO THIS FOR EACH SERVER GROUP BY EXCLUDING THE TASKS
        201 |C     FROM ALL OF THE OTHER SERVER GROUPS.
        202 |C
        203 |      allocate ( irank ( iqserver ) )
        204 |      ixx = NPES_MOD
        205 |      do i = 1, iquilt_group
        206 |         yes = .true.
        207 |         if ( mype .lt. NPES_MOD ) then
        208 |            irlr = ixx
        209 |         else
        210 |            irlr = 0
        211 |         end if
        212 |      icc = 0
        213 |      iss = NPES_MOD
        214 |C     THIS IS THE FIRST POSSIBLE TASK ID THAT COULD BE EXCLUDED
        215 |      do jj = 1, iquilt_group
        216 |         if ( jj .ne. i ) then
        217 |            issl = iss
        218 |            do kk = 1, inumq(jj)
        219 |               icc = icc + 1
        220 |               irank(icc)= issl
        221 |               if ( mype .eq. issl ) yes = .false.
        222 |               issl = issl + 1
        223 |            end do
        224 |         end if
        225 |         iss = iss + inumq(jj)
        226 |      end do
        227 |C
        228 |C     AT THIS POINT WE HAVE AN ARRAY, IRANK, WITH TASK IDS TO EXCLUDE
        229 |C     THERE ARE ICC OF THEM.
        230 |C     CREATE A NEW GROUP WITH THE TASKS FROM THE OTHER SERVER GROUPS
        231 |C     EXCLUDED AND THEN CREATE A NEW COMMUNICATOR ( IWORLD_MINUS ) THAT
        232 |C     CONTAINS ONLY THE MPI TASKS DOING THE MODEL INTEGRATION AND THE
        233 |C     TASKS THAT BLONG TO THE SERVER GROUP WE ARE CONSIDERING.
        234 |C
        235 |      iworld = MPI_COMM_WORLD
        236 |      call mpi_comm_group(iworld,igroup,ierr)
        237 |      call mpi_group_excl(igroup,icc,irank,igroup_x,ierr)
        238 |      call mpi_comm_create(iworld,igroup_x,iworld_minus,ierr)
        239 |      call mpi_group_free(igroup,ierr)
        240 |      call mpi_group_free(igroup_x,ierr)
        241 |C
        242 |C     AT THIS POINT WE HAVE A COMMUNICATOR THAT EXCLUDES THE TASKS WE DONT WANT.
        243 |C     CREATE AN INTERCOMMUNICATOR FOR USE BETWEEN THE MPI TASKS DOING THE MODEL
        244 |C     INTEGRATION AND THE I/O SERVER GROUP WE ARE CONSIDERING. THIS PROCESS IS
        245 |C     A COLLECTIVE ROUTINE SO IT CAN ONLY BE DONE BY THE TASKS THAT HAVE NOT
        246 |C     BEEN EXCLUDED. SAVE THIS NEW COMMUNICATOR IN MPI_COMM_INTER FOR USE BY
        247 |C     THE TASKS THAT BELONG TO THE SERVER GROUP THAT WE ARE CONSIDERING. THE
        248 |C     TASKS THAT ARE PERFORMING THE MODEL INTEGRATION WILL REFERENCE
        249 |C     MPI_COMM_INTER_ARRAY() SINCE WE WILL NEED TO SELECT WHICH SERVER
        250 |C     GROUP WE WISH TO COMMUNICATE WITH.
        251 |c
        252 |      if ( yes ) then
        253 |      call mpi_intercomm_create(mpi_comm_comp,0,iworld_minus,irlr,0,
        254 |     *   mpi_comm_inter,ierr)
        255 |      end if
        256 |C
        257 |      call mpi_barrier(MPI_COMM_WORLD,ierr)
        258 |C
        259 |      end do     ! end do for loop over the number of server groups
        260 |C
        261 |C***
        262 |C***  NPES IS REALLY THE NUMBER OF TASKS WORKING ON THE MODEL INTEGRATION
        263 |C***
        264 |      NPES = NPES  - IQSERVER
        265 |C
        266 |      IF(MYPE.EQ.0) THEN
        267 |         print *, ' The Posting is using ',npes,' MPI task'
        268 |         print *, ' There are ',iqserver,' I/O servers'
        269 |      END IF
        270 |C***
        271 |      deallocate ( irank )
        272 |C
        273 |      END
 
