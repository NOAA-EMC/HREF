         97 |C
         98 |C****************************************************************
         99 |C     START CALHEL HERE
        100 |C
        101 |C     INITIALIZE ARRAYS.
        102 |C
        103 |!$omp  parallel do
        104 |      DO J=JSTA,JEND
        105 |      DO I=1,IM
        106 |         UST(I,J)    = 0.0
        107 |         VST(I,J)    = 0.0
        108 |         HELI(I,J)   = 0.0
        109 |         UST1(I,J)   = 0.0
        110 |         VST1(I,J)   = 0.0
        111 |         UST5(I,J)   = 0.0
        112 |         VST5(I,J)   = 0.0
        113 |         UST6(I,J)   = 0.0
        114 |         VST6(I,J)   = 0.0
        115 |         COUNT6(I,J) = 0
        116 |         COUNT5(I,J) = 0
        117 |         COUNT1(I,J) = 0
        118 |      ENDDO
        119 |      ENDDO
        120 |C
        121 |C     LOOP OVER HORIZONTAL GRID.
        122 |C
        123 |      CALL EXCH_P(RES)
        124 |      CALL EXCH_P(PD)
        125 |      DO L = 1,LP1
        126 |        CALL EXCH_P(ZINT(1,jsta_2l,L))
        127 |      END DO
        128 |C
        129 |!$omp  parallel do
        130 |!$omp& private(htsfc,ie,iw,pdslvk,pkl,psfck)
        131 |      DO L = 1,LM
        132 |        DO J=JSTA_M,JEND_M
        133 |        DO I=2,IM-1
        134 |          IE=I+IVE(J)
        135 |          IW=I+IVW(J)
        136 |          PDSLVK=(PD(IW,J)*RES(IW,J)+PD(IE,J)*RES(IE,J)+
        137 |     1           PD(I,J+1)*RES(I,J+1)+PD(I,J-1)*RES(I,J-1))*0.25
        138 |          PSFCK=PDTOP*SGML1(LMV(I,J))+SGML2(LMV(I,J))*PDSLVK+PT
        139 |          HTSFC(I,J)=0.25*(ZINT(IW,J,LMV(I,J)+1)+ZINT(IE,J,LMV(I,J)+1)+
        140 |     1                ZINT(I,J+1,LMV(I,J)+1)+ZINT(I,J-1,LMV(I,J)+1))
        141 |C
        142 |C     COMPUTE MASS WEIGHTED MEAN WIND IN THE 0-6 KM LAYER, THE
        143 |C  0-0.5 KM LAYER, AND THE 5.5-6 KM LAYER
        144 |C
        145 |          Z2=0.125*(ZINT(IW,J,L)+ZINT(IW,J,L+1)+
        146 |     1            ZINT(IE,J,L)+ZINT(IE,J,L+1)+
        147 |     1            ZINT(I,J+1,L)+ZINT(I,J+1,L+1)+
        148 |     1            ZINT(I,J-1,L)+ZINT(I,J-1,L+1))
        149 |          DZABV=Z2-HTSFC(I,J)
        150 |
        151 |          IF (DZABV.LE.D6000 .AND. L.LE.LMV(I,J)) THEN
        152 |               UST6(I,J) = UST6(I,J) + U(I,J,L)
        153 |               VST6(I,J) = VST6(I,J) + V(I,J,L)
        154 |               COUNT6(I,J) = COUNT6(I,J) + 1
        155 |          ENDIF
        156 |
        157 |          IF (DZABV.LT.D6000 .AND. DZABV.GE.D5500 .AND.
        158 |     &       L.LE.LMV(I,J)) THEN
        159 |               UST5(I,J) = UST5(I,J) + U(I,J,L)
        160 |               VST5(I,J) = VST5(I,J) + V(I,J,L)
        161 |               COUNT5(I,J) = COUNT5(I,J) + 1
        162 |          ENDIF
        163 |
        164 |          IF (DZABV.LT.D500 .AND. L.LE.LMV(I,J)) THEN
        165 |               UST1(I,J) = UST1(I,J) + U(I,J,L)
        166 |               VST1(I,J) = VST1(I,J) + V(I,J,L)
        167 |               COUNT1(I,J) = COUNT1(I,J) + 1
        168 |          ENDIF
        169 |
        170 |        ENDDO
        171 |        ENDDO
        172 |      ENDDO
        173 |C
        174 |C CASE WHERE THERE IS NO LEVEL WITH HEIGHT BETWEEN 5500 AND 6000
        175 |C
        176 |      DO J=JSTA_M,JEND_M
        177 |      DO I=2,IM-1
        178 |        IF (COUNT5(I,J) .EQ. 0) THEN
        179 |         DO L=LM,1,-1
        180 |          IE=I+IVE(J)
        181 |          IW=I+IVW(J)
        182 |          Z2=0.125*(ZINT(IW,J,L)+ZINT(IW,J,L+1)+
        183 |     1            ZINT(IE,J,L)+ZINT(IE,J,L+1)+
        184 |     1            ZINT(I,J+1,L)+ZINT(I,J+1,L+1)+
        185 |     1            ZINT(I,J-1,L)+ZINT(I,J-1,L+1))
        186 |          DZABV=Z2-HTSFC(I,J)
        187 |          IF (DZABV.LT.D7000 .AND. DZABV.GE.D6000) THEN
        188 |               UST5(I,J) = UST5(I,J) + U(I,J,L)
        189 |               VST5(I,J) = VST5(I,J) + V(I,J,L)
        190 |               COUNT5(I,J) = 1
        191 |               GOTO 30
        192 |          ENDIF
        193 |         ENDDO
        194 |        ENDIF
        195 |30    CONTINUE
        196 |      ENDDO
        197 |      ENDDO
        198 |
        199 |C
        200 |!$omp  parallel do
        201 |!$omp& private(umean6,vmean6,umean5,vmean5,umean1,vmean1,ushr,vshr)
        202 |
        203 |      DO J=JSTA_M,JEND_M
        204 |      DO I=2,IM-1
        205 |         IF (COUNT6(I,J).GT.0.0 .AND. COUNT1(I,J) .GT. 0.0
        206 |     1      .AND. COUNT5(I,J) .GT. 0.0) THEN
        207 |           UMEAN5 = UST5(I,J) / COUNT5(I,J)
        208 |           VMEAN5 = VST5(I,J) / COUNT5(I,J)
        209 |           UMEAN1 = UST1(I,J) / COUNT1(I,J)
        210 |           VMEAN1 = VST1(I,J) / COUNT1(I,J)
        211 |           UMEAN6 = UST6(I,J) / COUNT6(I,J)
        212 |           VMEAN6 = VST6(I,J) / COUNT6(I,J)
        213 |
        214 |C
        215 |C      COMPUTE STORM MOTION VECTOR
        216 |C      IT IS DEFINED AS 7.5 M/S TO THE RIGHT OF THE 0-6 KM MEAN
        217 |C      WIND CONSTRAINED ALONG A LINE WHICH IS BOTH PERPENDICULAR
        218 |C      TO THE 0-6 KM MEAN VERTICAL WIND SHEAR VECTOR AND PASSES
        219 |C      THROUGH THE 0-6 KM MEAN WIND.  THE WIND SHEAR VECTOR IS
        220 |C      SET AS THE DIFFERENCE BETWEEN THE 5.5-6 KM WIND (THE HEAD
        221 |C      OF THE SHEAR VECTOR) AND THE 0-0.5 KM WIND (THE TAIL).
        222 |C      THIS IS FOR THE RIGHT-MOVING CASE;  WE IGNORE THE LEFT MOVER.
        223 |
        224 |           USHR = UMEAN5 - UMEAN1
        225 |           VSHR = VMEAN5 - VMEAN1
        226 |
        227 |           UST(I,J) = UMEAN6 + (7.5*VSHR/SQRT(USHR*USHR+VSHR*VSHR))
        228 |           VST(I,J) = VMEAN6 - (7.5*USHR/SQRT(USHR*USHR+VSHR*VSHR))
        229 |         ELSE
        230 |           UST(I,J) = 0.0
        231 |           VST(I,J) = 0.0
        232 |        ENDIF
        233 |      ENDDO
        234 |      ENDDO
        235 |C
        236 |C       COMPUTE STORM-RELATIVE HELICITY
        237 |C
        238 |!$omp  parallel do
        239 |!$omp& private(du1,du2,dv1,dv2,dz,dz1,dz2,dzabv,ie,iw,z1,z2,z3)
        240 |      DO L = 2,LM-1
        241 |        DO J=JSTA_M,JEND_M
        242 |        DO I=2,IM-1
        243 |          IW=I+IVW(J)
        244 |          IE=I+IVE(J)
        245 |          Z2=0.125*(ZINT(IW,J,L)+ZINT(IW,J,L+1)+
        246 |     &              ZINT(IE,J,L)+ZINT(IE,J,L+1)+
        247 |     &              ZINT(I,J+1,L)+ZINT(I,J+1,L+1)+
        248 |     &              ZINT(I,J-1,L)+ZINT(I,J-1,L+1))
        249 |          DZABV=Z2-HTSFC(I,J)
        250 |C
        251 |          IF(DZABV.LT.DEPTH.AND.L.LE.LMV(I,J))THEN
        252 |            Z1=0.125*(ZINT(IW,J,L+1)+ZINT(IW,J,L+2)+
        253 |     &                ZINT(IE,J,L+1)+ZINT(IE,J,L+2)+
        254 |     &                ZINT(I,J+1,L+1)+ZINT(I,J+1,L+2)+
        255 |     &                ZINT(I,J-1,L+1)+ZINT(I,J-1,L+2))
        256 |            Z3=0.125*(ZINT(IW,J,L-1)+ZINT(IW,J,L)+
        257 |     &                ZINT(IE,J,L-1)+ZINT(IE,J,L)+
        258 |     &                ZINT(I,J+1,L-1)+ZINT(I,J+1,L)+
        259 |     &                ZINT(I,J-1,L-1)+ZINT(I,J-1,L))
        260 |            DZ=0.25*((ZINT(IW,J,L)+ZINT(IE,J,L)+
        261 |     &                ZINT(I,J-1,L)+ZINT(I,J+1,L))-
