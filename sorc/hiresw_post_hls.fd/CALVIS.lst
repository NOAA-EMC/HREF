          1 |c**********************************************************************c
          2 |      SUBROUTINE CALVIS(QV,QC,QR,QI,QS,TT,PP,VIS)
          3 |c
          4 |c   This routine computes horizontal visibility at the
          5 |c   surface or lowest model layer, from qc, qr, qi, and qs.
          6 |c   qv--water vapor mixing ratio (kg/kg)
          7 |c   qc--cloud water mixing ratio (kg/kg)
          8 |c   qr--rain water mixing ratio  (kg/kg)
          9 |c   qi--cloud ice mixing ratio   (kg/kg)
         10 |c   qs--snow mixing ratio        (kg/kg)
         11 |c   tt--temperature              (k)
         12 |c   pp--pressure                 (Pa)
         13 |c
         14 |c   If iice=0:
         15 |c      qprc=qr     qrain=qr and qclw=qc if T>0C
         16 |c      qcld=qc          =0          =0  if T<0C
         17 |c                  qsnow=qs and qclice=qc  if T<0C
         18 |c                       =0            =0   if T>0C
         19 |c   If iice=1:
         20 |c      qprc=qr+qs   qrain=qr and qclw=qc
         21 |c      qcld=qc+qi   qsnow=qs and qclice=qc
         22 |c
         23 |c   Independent of the above definitions, the scheme can use different
         24 |c   assumptions of the state of hydrometeors:
         25 |c        meth='d': qprc is all frozen if T<0, liquid if T>0
         26 |c        meth='b': Bocchieri scheme used to determine whether qprc
         27 |c           is rain or snow. A temperature assumption is used to
         28 |c           determine whether qcld is liquid or frozen.
         29 |c        meth='r': Uses the four mixing ratios qrain, qsnow, qclw,
         30 |c           and qclice
         31 |c
         32 |c   The routine uses the following
         33 |c   expressions for extinction coefficient, beta (in km**-1),
         34 |c   with C being the mass concentration (in g/m**3):
         35 |c
         36 |c      cloud water:  beta = 144.7 * C ** (0.8800)
         37 |c      rain water:   beta =  2.24 * C ** (0.7500)
         38 |c      cloud ice:    beta = 327.8 * C ** (1.0000)
         39 |c      snow:         beta = 10.36 * C ** (0.7776)
         40 |c
         41 |c   These expressions were obtained from the following sources:
         42 |c
         43 |c      for cloud water: from Kunkel (1984)
         44 |c      for rainwater: from M-P dist'n, with No=8e6 m**-4 and
         45 |c         rho_w=1000 kg/m**3
         46 |c      for cloud ice: assume randomly oriented plates which follow
         47 |c         mass-diameter relationship from Rutledge and Hobbs (1983)
         48 |c      for snow: from Stallabrass (1985), assuming beta = -ln(.02)/vis
         49 |c
         50 |c   The extinction coefficient for each water species present is
         51 |c   calculated, and then all applicable betas are summed to yield
         52 |c   a single beta. Then the following relationship is used to
         53 |c   determine visibility (in km), where epsilon is the threshhold
         54 |c   of contrast, usually taken to be .02:
         55 |c
         56 |c      vis = -ln(epsilon)/beta      [found in Kunkel (1984)]
         57 |c
         58 |C------------------------------------------------------------------
         59 |      INCLUDE "parmeta"
  1       1 |C-------------------------------------------------------------
  1       2 |C***  SET PRIMARY GRID DIMENSIONS AND PRESSURE OUTPUT LEVELS
  1       3 |      PARAMETER (IM=89,JM=143,LM=60,LSM=39)
  1       4 |C-------------------------------------------------------------
  1       5 |C***  SET THE NUMBER OF PEs IN THE I-DIRECTION AND J-DIRECTION
  1       6 |      PARAMETER(INPES=5,JNPES=6)
  1       7 |C-------------------------------------------------------------
  1       8 |C
  1       9 |      PARAMETER(IGSTL=-5,IGSTR=5)
  1      10 |      PARAMETER(JGSTL=-5,JGSTR=5)
  1      11 |      PARAMETER(ITAIL=IM-(INPES*(IM/INPES)))
  1      12 |      PARAMETER(JTAIL=JM-(JNPES*(JM/JNPES)))
  1      13 |C
  1      14 |      PARAMETER(IDIM1=IGSTL,IDIM2=IM/INPES+IGSTR+1)
  1      15 |      PARAMETER(JDIM1=JGSTL,JDIM2=JM/JNPES+JGSTR+1)
  1      16 |C
  1      17 |C-------------------------------------------------------------
         60 |      INCLUDE "params"
  2       1 |C
  2       2 |C     ARRAY DIMENSIONS.
  2       3 |      PARAMETER	
  2       4 |     & (IMT=2*IM-1,JMT=JM/2+1)
  2       5 |      PARAMETER
  2       6 |     & (IM1=IM-1,JM2=JM-2,JAM=6+2*(JM-10)
  2       7 |     &, NINC=2*IM-1
  2       8 |     &, LM1=LM-1,LP1=LM+1
  2       9 |     &, LB=2*IM+JM-3)
  2      10 |C
  2      11 |C     SMOOTHING PARAMETERS.
  2      12 |      PARAMETER
  2      13 |     & (KSMUD=4,KSLPD=1,KSSLP=2,KSFI=2,KST=2
  2      14 |     &, KSUV=2,KSRH=2,KSAV=2,KSLI=2,KSLP=2
  2      15 |     &, SMTHA=-1.8E-4,SMTHB=16.,NSUMD=1)
  2      16 |C
  2      17 |C     DIMENSIONS FOR PROFILE OUPUT ARRAYS.
  2      18 |      PARAMETER
  2      19 |     & (NSTAT=100,NWORD=6+6*LM+9,NWORD3=NWORD-3,NWORD6=NWORD-6)
  2      20 |C
  2      21 |C     CONSTANTS.
  2      22 |      PARAMETER
  2      23 |     & (CM1=2937.4,CM2=4.9283,CM3=23.5518)
  2      24 |      PARAMETER
  2      25 |     & (D00=0.0,D1=0.1,D01=0.01,D001=0.001,D50=0.50,H999=999.
  2      26 |     &, H1=1.0,H2=2.0,H4=4.0,H99999=99999.,H1M12=1.E-12
  2      27 |     &, H1000=1000.,H1M5=1.E-5,D125=.125,D25=0.25,H100=100.
  2      28 |     &, H10000=10000.,H10E5=1.E5,CAPA=0.28589641E0
  2      29 |     &, D115=0.115,D608=0.608,D05=0.05,D0065=0.0065
  2      30 |     &, H1E2=100.,H1E4=1.E4,H1M2=1.E-2)
  2      31 |      PARAMETER
  2      32 |     & (G=9.8,RD=287.04,ROG=RD/G,GI=1./G
  2      33 |     &, OVERRC=1.75,AD05=OVERRC*D05,CFT0=OVERRC-H1
  2      34 |     &, DTR=1.745329E-2,RTD=1./DTR,ERAD=6.376E6,CP=1004.6
  2      35 |     &, RCAPA=1./CAPA,P1000=1000.E2,DP10M=110
  2      36 |     &, ELWV=2.50E6,CPDR=CP/RD,RG=1./GI,ELDR=ELWV/RD
  2      37 |     &, BETA=.00367,BTG=BETA*9.8)
  2      38 |      PARAMETER
  2      39 |     & (FMW=18.015,FMD=28.964,EPS=FMW/FMD,ONEPS=1.-EPS,TFRZ=273.16
  2      40 |     &, RDOCP=RD/CP,LHEAT=2.5E6,LHTOCP=LHEAT/CP,EPSQ2=0.2
  2      41 |     &, PQ0=379.90516,A2=17.2693882,A3=273.16,A4=35.86)
         61 |      INCLUDE "CTLBLK.comm"
  3       1 |C-----------------------------------------------------------------------
  3       2 |      COMMON /CTLBLK/
  3       3 |     & SIGMA,RUN,IDAT(3),IHRST,FIRST,RESTRT,NFCST,NBC,LIST,IOUT
  3       4 |     &,DT,NTSD,NTSTM,NDDAMP,NPREC,IDTAD,NBOCO,NSHDE,NCP
  3       5 |     &,NPHS,NRADS,NRADL
  3       6 |      INTEGER NUM_PROCS, ME, JSTA, JEND,
  3       7 |     &        JSTA_M, JSTA_M2, JEND_M, JEND_M2, IUP, IDN, ICNT, IDSP,
  3       8 |     &        JSTA_2L, JEND_2U
  3       9 |      COMMON /PARALLEL/NUM_PROCS, ME, JSTA, JEND, JSTA_M, JEND_M,
  3      10 |     &                 JSTA_M2, JEND_M2, IUP, IDN,
  3      11 |     &                 ICNT(0:1023), IDSP(0:1023),
  3      12 |     &                 JSTA_2L, JEND_2U, NUM_SERVERS, MPI_COMM_INTER,
  3      13 |     &                 MPI_COMM_COMP
  3      14 |C-----------------------------------------------------------------------
         62 |C
         63 |      REAL QV(IM,JM),QC(IM,JM),QI(IM,JM),QR(IM,JM),QS(IM,JM)
         64 |     1,    TT(IM,JM),PP(IM,JM),VIS(IM,JM)
         65 |      CHARACTER METH*1
         66 |C------------------------------------------------------------------
         67 |C------------------------------------------------------------------
         68 |      CELKEL=273.15
         69 |      TICE=CELKEL-10.
         70 |      COEFLC=144.7
         71 |      COEFLP=2.24
         72 |      COEFFC=327.8
         73 |      COEFFP=10.36
         74 |      EXPONLC=0.8800
         75 |      EXPONLP=0.7500
         76 |      EXPONFC=1.0000
         77 |      EXPONFP=0.7776
         78 |      CONST1=-LOG(.02)
         79 |      RHOICE=917.
         80 |      RHOWAT=1000.
         81 |C
         82 |      DO J=JSTA,JEND
         83 |      DO I=1,IM
         84 |c       IF(IICE.EQ.0)THEN
         85 |c         QPRC=QR
         86 |c         QCLD=QC
         87 |c         IF(TT.LT.CELKEL)THEN
         88 |c           QRAIN=0.
         89 |c           QSNOW=QPRC
         90 |c           QCLW=0.
         91 |c           QCLICE=QCLD
         92 |c         ELSE
         93 |c           QRAIN=QPRC
         94 |c           QSNOW=0.
         95 |c           QCLW=QCLD
         96 |c           QCLICE=0.
         97 |c         ENDIF
         98 |c       ELSE
         99 |          QPRC=QR(I,J)+QS(I,J)
        100 |          QCLD=QC(I,J)+QI(I,J)
        101 |          QRAIN=QR(I,J)
        102 |          QSNOW=QS(I,J)
        103 |          QCLW=QC(I,J)
        104 |          QCLICE=QI(I,J)
        105 |c       ENDIF
        106 |c       TV=VIRTUAL(TT,QV)
        107 |        TV=TT(I,J)*(H1+D608*QV(I,J))
        108 |        RHOAIR=PP(I,J)/(RD*TV)
        109 |c       IF(METH.EQ.'D')THEN
        110 |c         IF(TT.LT.CELKEL)THEN
        111 |c           VOVERMD=(1.+QV)/RHOAIR+(QPRC+QCLD)/RHOICE
        112 |c           CONCLC = 0.
        113 |c           CONCLP = 0.
        114 |c           CONCFC = QCLD/VOVERMD*1000.
        115 |c           CONCFP = QPRC/VOVERMD*1000.
        116 |c         ELSE
        117 |c           VOVERMD=(1.+QV)/RHOAIR+(QPRC+QCLD)/RHOWAT
        118 |c           CONCLC = QCLD/VOVERMD*1000.
        119 |c           CONCLP = QPRC/VOVERMD*1000.
        120 |c           CONCFC = 0.
        121 |c           CONCFP = 0.
        122 |c         ENDIF
        123 |c       ELSEIF(METH.EQ.'B')THEN
        124 |c         IF(TT.LT.TICE)THEN
        125 |c           VOVERMD=(1.+QV)/RHOAIR+(QPRC+QCLD)/RHOICE
        126 |c           CONCLC = 0.
        127 |c           CONCLP = 0.
        128 |c           CONCFC = QCLD/VOVERMD*1000.
        129 |c           CONCFP = QPRC/VOVERMD*1000.
        130 |c         ELSEIF(PRSNOW.GE.50.)THEN
        131 |c           VOVERMD=(1.+QV)/RHOAIR+QPRC/RHOICE+QCLD/RHOWAT
        132 |c           CONCLC = QCLD/VOVERMD*1000.
        133 |c           CONCLP = 0.
        134 |c           CONCFC = 0.
        135 |c           CONCFP = QPRC/VOVERMD*1000.
        136 |c         ELSE
        137 |c           VOVERMD=(1.+QV)/RHOAIR+(QPRC+QCLD)/RHOWAT
        138 |c           CONCLC = QCLD/VOVERMD*1000.
        139 |c           CONCLP = QPRC/VOVERMD*1000.
        140 |c           CONCFC = 0.
        141 |c           CONCFP = 0.
        142 |c         ENDIF
        143 |c       ELSEIF(METH.EQ.'R')THEN
        144 |          VOVERMD=(1.+QV(I,J))/RHOAIR+(QCLW+QRAIN)/RHOWAT+
        145 |     1            (QCLICE+QSNOW)/RHOICE
        146 |          CONCLC=QCLW/VOVERMD*1000.
        147 |          CONCLP=QRAIN/VOVERMD*1000.
        148 |          CONCFC=QCLICE/VOVERMD*1000.
        149 |          CONCFP=QSNOW/VOVERMD*1000.
        150 |c       ENDIF
        151 |        BETAV=COEFFC*CONCFC**EXPONFC+COEFFP*CONCFP**EXPONFP
        152 |     1       +COEFLC*CONCLC**EXPONLC+COEFLP*CONCLP**EXPONLP
        153 |     2       +1.E-10
        154 |c CHANGED GSM 3-10-00 -->  no point in distinguishing values
        155 |c       above 20 km, so make that value the max (prev max was 80)
        156 |        VIS(I,J)=1.E3*MIN(20.,CONST1/BETAV)   ! max of 20km
        157 |      ENDDO
        158 |      ENDDO
        159 |C
        160 |      RETURN
        161 |      END
 
