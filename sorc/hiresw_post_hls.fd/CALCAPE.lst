        144 |C
        145 |C**************************************************************
        146 |C     START CALCAPE HERE.
        147 |C
        148 |      ALLOCATE(TPAR(IM,JSTA_2L:JEND_2U,LM))
        149 |C
        150 |C     COMPUTE CAPE/CINS
        151 |C
        152 |C        WHICH IS: THE SUM FROM THE LCL TO THE EQ LEVEL OF
        153 |C             G * (LN(THETAP) - LN(THETAA)) * DZ
        154 |C
        155 |C             (POSITIVE AREA FOR CAPE, NEGATIVE FOR CINS)
        156 |C
        157 |C        WHERE:
        158 |C             THETAP IS THE PARCEL THETA
        159 |C             THETAA IS THE AMBIENT THETA
        160 |C             DZ IS THE THICKNESS OF THE LAYER
        161 |C
        162 |C         USING LCL AS LEVEL DIRECTLY BELOW SATURATION POINT
        163 |C         AND EQ LEVEL IS THE HIGHEST POSITIVELY BUOYANT LEVEL.
        164 |C
        165 |C         IEQL = EQ LEVEL
        166 |C
        167 |C     INITIALIZE CAPE AND CINS ARRAYS
        168 |C
        169 |!$omp  parallel do
        170 |      DO J=JSTA,JEND
        171 |      DO I=1,IM
        172 |        CAPE(I,J) = D00
        173 |        CINS(I,J) = D00
        174 |        LCL(I,J)  = D00
        175 |        THESP(I,J)= D00
        176 |        IEQL(I,J) = LM+1
        177 |      ENDDO
        178 |      ENDDO
        179 |C
        180 |!$omp  parallel do
        181 |      DO L=1,LM
        182 |        DO J=JSTA,JEND
        183 |        DO I=1,IM
        184 |          TPAR(I,J,L)= D00
        185 |        ENDDO
        186 |        ENDDO
        187 |      ENDDO
        188 |C
        189 |C     FOR TYPE 2 CAPE/CINS SMOOTH DATA PRIOR TO CALCULATION.
        190 |C     NOTE THAT FOR TYPE 1 CAPE/CINS ARRAYS P1D, T1D, Q1D
        191 |C     ARE DUMMY ARRAYS.
        192 |C
        193 |      IF (ITYPE.EQ.2) THEN
        194 |         CALL P2FILT(ISMTHP,HBM2,P1D)
        195 |         CALL P2FILT(ISMTHT,HBM2,T1D)
        196 |         CALL P2FILT(ISMTHQ,HBM2,Q1D)
        197 |         DO J=JSTA,JEND
        198 |            DO I=1,IM
        199 |              Q1D(I,J) = AMIN1(AMAX1(H1M12,Q1D(I,J)),H99999)
        200 |            ENDDO
        201 |         ENDDO
        202 |      ENDIF
        203 |C-------FOR ITYPE=1-----------------------------------------------------
        204 |C---------FIND MAXIMUM THETA E LAYER IN LOWEST DPBND ABOVE GROUND-------
        205 |C-------FOR ITYPE=2-----------------------------------------------------
        206 |C---------FIND THETA E LAYER OF GIVEN T1D, Q1D, P1D---------------------
        207 |C--------------TRIAL MAXIMUM BUOYANCY LEVEL VARIABLES-------------------
        208 |      DO 20 KB=1,LM
        209 |       IF (ITYPE.EQ.2.AND.KB.GT.1) GOTO 20
        210 |!$omp  parallel do
        211 |!$omp& private(apebtk,apespk,bqk,bqs00k,bqs10k,iq,it,ittbk,lmhk,
        212 |!$omp&         p00k,p01k,p10k,p11k,pkl,psfck,qbtk,sqk,sqs00k,
        213 |!$omp&         sqs10k,tbtk,tpspk,tqk,tthbtk,tthesk,tthk)
        214 |       DO 10 J=JSTA_M,JEND_M
        215 |       DO 10 I=2,IM-1
        216 |        LMHK   =LMH(I,J)
        217 |        PSFCK  =PDTOP*SGML1(LMHK)+SGML2(LMHK)*PDSL(I,J)+PT
        218 |          PKL = PDTOP*SGML1(KB)+SGML2(KB)*PDSL(I,J)+PT
        219 |          IF (ITYPE.EQ.1.AND.(PKL.LT.PSFCK-DPBND.OR.PKL.GT.PSFCK))
        220 |     &             GOTO 10
        221 |          IF (ITYPE.EQ.1) THEN
        222 |            TBTK   =T(I,J,KB)
        223 |            QBTK   =Q(I,J,KB)
        224 |            APEBTK =(H10E5/(PDTOP*SGML1(KB)+PDSL(I,J)*SGML2(KB)+PT))
        225 |     &	    **CAPA
        226 |          ELSE
        227 |            PKL    =P1D(I,J)
        228 |            TBTK   =T1D(I,J)
        229 |            QBTK   =Q1D(I,J)
        230 |            APEBTK =(H10E5/PKL)**CAPA
        231 |          ENDIF
        232 |C--------------SCALING POTENTIAL TEMPERATURE & TABLE INDEX--------------
        233 |          TTHBTK =TBTK*APEBTK
        234 |          TTHK   =(TTHBTK-THL)*RDTH
        235 |          QQ(I,J)=TTHK-AINT(TTHK)
        236 |          ITTBK  =INT(TTHK)+1
        237 |C--------------KEEPING INDICES WITHIN THE TABLE-------------------------
        238 |          IF(ITTBK.LT.1)    THEN
        239 |            ITTBK  =1
        240 |            QQ(I,J)=D00
        241 |          ENDIF
        242 |          IF(ITTBK.GE.JTB)  THEN
        243 |            ITTBK  =JTB-1
        244 |            QQ(I,J)=D00
        245 |          ENDIF
        246 |C--------------BASE AND SCALING FACTOR FOR SPEC. HUMIDITY---------------
        247 |          BQS00K=QS0(ITTBK)
        248 |          SQS00K=SQS(ITTBK)
        249 |          BQS10K=QS0(ITTBK+1)
        250 |          SQS10K=SQS(ITTBK+1)
        251 |C--------------SCALING SPEC. HUMIDITY & TABLE INDEX---------------------
        252 |          BQK    =(BQS10K-BQS00K)*QQ(I,J)+BQS00K
        253 |          SQK    =(SQS10K-SQS00K)*QQ(I,J)+SQS00K
        254 |          TQK    =(QBTK-BQK)/SQK*RDQ
        255 |          PP(I,J)=TQK-AINT(TQK)
        256 |          IQ     =INT(TQK)+1
        257 |C--------------KEEPING INDICES WITHIN THE TABLE-------------------------
        258 |          IF(IQ.LT.1)    THEN
        259 |            IQ     =1
        260 |            PP(I,J)=D00
        261 |          ENDIF
        262 |          IF(IQ.GE.ITB)  THEN
        263 |            IQ     =ITB-1
        264 |            PP(I,J)=D00
        265 |          ENDIF
        266 |C--------------SATURATION PRESSURE AT FOUR SURROUNDING TABLE PTS.-------
        267 |          IT=ITTBK
        268 |          P00K=PTBL(IQ  ,IT  )
        269 |          P10K=PTBL(IQ+1,IT  )
        270 |          P01K=PTBL(IQ  ,IT+1)
        271 |          P11K=PTBL(IQ+1,IT+1)
        272 |C--------------SATURATION POINT VARIABLES AT THE BOTTOM-----------------
        273 |          TPSPK=P00K+(P10K-P00K)*PP(I,J)+(P01K-P00K)*QQ(I,J)
        274 |     2      +(P00K-P10K-P01K+P11K)*PP(I,J)*QQ(I,J)
        275 |          APESPK=(H10E5/TPSPK)**CAPA
        276 |          TTHESK=TTHBTK*EXP(ELOCP*QBTK*APESPK/TTHBTK)
        277 |C--------------CHECK FOR MAXIMUM THETA E--------------------------------
        278 |          IF(TTHESK.GT.THESP(I,J)) THEN
        279 |            PSP  (I,J)=TPSPK
        280 |            THESP(I,J)=TTHESK
        281 |          ENDIF
        282 | 10   CONTINUE
        283 | 20     CONTINUE
        284 |C-----CHOOSE LAYER DIRECTLY BELOW PSP AS LCL AND------------------------
        285 |C-----ENSURE THAT THE LCL IS ABOVE GROUND.------------------------------
        286 |C-------(IN SOME RARE CASES FOR ITYPE=2, IT IS NOT)---------------------
        287 |      DO L=1,LM
        288 |!$omp  parallel do
        289 |!$omp& private(pkl)
        290 |        DO J=JSTA_M,JEND_M
        291 |        DO I=2,IM-1
        292 |          PKL = PDTOP*SGML1(L)+SGML2(L)*PDSL(I,J)+PT
        293 |          IF (PKL.LT.PSP(I,J)) LCL(I,J)=L+1
        294 |        ENDDO
        295 |        ENDDO
        296 |      ENDDO
        297 |!$omp  parallel do
        298 |        DO J=JSTA_M,JEND_M
        299 |        DO I=2,IM-1
        300 |          IF (LCL(I,J).GT.LMH(I,J)) LCL(I,J)=LMH(I,J)
        301 |        ENDDO
        302 |        ENDDO
        303 |C-----------------------------------------------------------------------
        304 |C---------FIND TEMP OF PARCEL LIFTED ALONG MOIST ADIABAT (TPAR)---------
        305 |C-----------------------------------------------------------------------
        306 |!!$omp  parallel do
        307 |      DO 30 L=LM,1,-1
        308 |C--------------SCALING PRESSURE & TT TABLE INDEX------------------------
        309 |      KNUML=0
        310 |      KNUMH=0
        311 |      DO J=JSTA_M,JEND_M
        312 |      DO I=2,IM-1
        313 |        KLRES(I,J)=0
        314 |        KHRES(I,J)=0
        315 |        PKL=SGML1(L)*PDTOP+SGML2(L)*PDSL(I,J)+PT
        316 |        IF(L.LE.LCL(I,J)) THEN
        317 |          IF(PKL.LT.PLQ)THEN
        318 |            KNUML=KNUML+1
        319 |            KLRES(I,J)=1
        320 |          ELSE
        321 |            KNUMH=KNUMH+1
        322 |            KHRES(I,J)=1
        323 |          ENDIF
        324 |        ENDIF
        325 |      ENDDO
        326 |      ENDDO
        327 |C***
        328 |C***  COMPUTE PARCEL TEMPERATURE ALONG MOIST ADIABAT FOR PRESSURE<PLQ
        329 |C**
        330 |      IF(KNUML.GT.0)THEN
        331 |        CALL TTBLEX(TPAR(1,JSTA_2L,L),TTBL,ITB,JTB,KLRES,PDSL,SGML1(L)
        332 |     1, SGML2(L),PDTOP,HTM(1,JSTA_2L,L),PT,PL,QQ,PP,RDP,THE0,STHE,RDTHE
        333 |     2, THESP,IPTB,ITHTB)
        334 |      ENDIF
        335 |C***
        336 |C***  COMPUTE PARCEL TEMPERATURE ALONG MOIST ADIABAT FOR PRESSURE>PLQ
        337 |C**
        338 |      IF(KNUMH.GT.0)THEN
        339 |        CALL TTBLEX(TPAR(1,JSTA_2L,L),TTBLQ,ITBQ,JTBQ,KHRES,PDSL
        340 |     1,SGML1(L),SGML2(L),PDTOP,HTM(1,JSTA_2L,L),PT,PLQ,QQ,PP,RDPQ
        341 |     2,THE0Q,STHEQ,RDTHEQ,THESP,IPTB,ITHTB)
        342 |      ENDIF
        343 |
        344 |C------------SEARCH FOR EQ LEVEL----------------------------------------
        345 |      DO J=JSTA_M,JEND_M
        346 |      DO I=2,IM-1
        347 |        IF(KHRES(I,J).GT.0)THEN
        348 |          IF(TPAR(I,J,L).GT.T(I,J,L)) IEQL(I,J)=L
        349 |        ENDIF
        350 |      ENDDO
        351 |      ENDDO
        352 |C
        353 |      DO J=JSTA_M,JEND_M
        354 |      DO I=2,IM-1
        355 |        IF(KLRES(I,J).GT.0)THEN
        356 |          IF(TPAR(I,J,L).GT.T(I,J,L)) IEQL(I,J)=L
        357 |        ENDIF
        358 |      ENDDO
        359 |      ENDDO
        360 |C-----------------------------------------------------------------------
        361 | 30   CONTINUE
        362 |C------------COMPUTE CAPE AND CINS--------------------------------------
        363 |c      DO K=KHL01,KHH01
        364 |c        LCLK=LCL(K)
        365 |c        IEQK=IEQL(K)
        366 |cCDIR$ SHORTLOOP
        367 |c        DO L=IEQK,LCLK
        368 |c          PRESK=AETA(L)*PDSL(K)+PT
        369 |c          DZKL=T(K,L)*(Q(K,L)*D608+H1)*ROG*PDSL(K)*DETA(L)/PRESK
        370 |c          THETAP=TPAR(K,L)*(H10E5/PRESK)**CAPA
        371 |c          THETAA=T(K,L)*(H10E5/PRESK)**CAPA
        372 |c          IF (THETAP.LT.THETAA)
        373 |c     &      CINS(K)=CINS(K)+G*(ALOG(THETAP)-ALOG(THETAA))*DZKL
        374 |c          IF (THETAP.GT.THETAA)
        375 |c     &      CAPE(K)=CAPE(K)+G*(ALOG(THETAP)-ALOG(THETAA))*DZKL
        376 |c        ENDDO
        377 |c      ENDDO
        378 |      LBEG=100
        379 |      LEND=0
        380 |C
        381 |!$omp  parallel do
        382 |!$omp& private(lbeg,lend)
        383 |      DO J=JSTA_M,JEND_M
        384 |      DO I=2,IM-1
        385 |        LBEG=MIN(IEQL(I,J),LBEG)
        386 |        LEND=MAX(LCL(I,J),LEND)
        387 |      ENDDO
        388 |      ENDDO
        389 |C
        390 |      DO L=LBEG,LEND
        391 |        DO J=JSTA_M,JEND_M
        392 |        DO I=2,IM-1
        393 |          IDX(I,J)=0
        394 |          IF(L.GE.IEQL(I,J).AND.L.LE.LCL(I,J))THEN
        395 |            IDX(I,J)=1
        396 |          ENDIF
        397 |        ENDDO
        398 |C
        399 |        ENDDO
        400 |C
        401 |!$omp  parallel do
        402 |!$omp& private(dzkl,presk,thetaa,thetap)
        403 |        DO J=JSTA_M,JEND_M
        404 |        DO I=2,IM-1
        405 |          IF(IDX(I,J).GT.0)THEN
        406 |            PRESK=SGML1(L)*PDTOP+SGML2(L)*PDSL(I,J)+PT
        407 |            DZKL=T(I,J,L)*(Q(I,J,L)*D608+H1)
        408 |     *           *ROG*(PDTOP*DSG1(L)+PDSL(I,J)*DSG2(L))/PRESK
        409 |            THETAP=TPAR(I,J,L)*(H10E5/PRESK)**CAPA
        410 |            THETAA=T(I,J,L)*(H10E5/PRESK)**CAPA
        411 |            IF(THETAP.LT.THETAA)THEN
        412 |              CINS(I,J)=CINS(I,J)
        413 |cjjt *                 +G*(ALOG(THETAP/THETAA))*DZKL
        414 |     *                 +G*(ALOG(THETAP)-ALOG(THETAA))*DZKL
        415 |            ELSEIF(THETAP.GT.THETAA)THEN
        416 |              CAPE(I,J)=CAPE(I,J)
        417 |cjjt *                 +G*(ALOG(THETAP/THETAA))*DZKL
        418 |     *                 +G*(ALOG(THETAP)-ALOG(THETAA))*DZKL
        419 |            ENDIF
        420 |          ENDIF
        421 |        ENDDO
        422 |        ENDDO
        423 |      ENDDO
        424 |
        425 |C
        426 |C     ENFORCE LOWER LIMIT OF 0.0 ON CAPE AND UPPER
        427 |C     LIMIT OF 0.0 ON CINS.
        428 |C
        429 |!$omp  parallel do
        430 |      DO 40 J=JSTA,JEND
        431 |      DO 40 I=1,IM
        432 |c         IF (CAPE(K).LT.D00)
        433 |c     X        WRITE(STDOUT,*)
        434 |c     X        'CALCAPE:  NEGATIVE CAPE AT K=',K,CAPE(K)
        435 |c         IF (CINS(K).GT.D00)
        436 |c     X        WRITE(STDOUT,*)
        437 |c     X        'CALCAPE:  POSTIVE CINS AT K = ',K,CINS(K)
        438 |         CAPE(I,J) = AMAX1(D00,CAPE(I,J))
        439 |         CINS(I,J) = AMIN1(CINS(I,J),D00)
        440 | 40   CONTINUE
        441 |C
        442 |      DEALLOCATE(TPAR)
        443 |C     END OF ROUTINE.
        444 |C
        445 |      RETURN
        446 |      END
 
