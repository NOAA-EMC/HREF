          1 |      SUBROUTINE P2FILT(MKOUNT,HBM2,FIELD)
          2 |CFPP$ NOCONCUR R
          3 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          4 |C                .      .    .
          5 |C SUBPROGRAM:    P2FILT      4-TH ORDER SMOOTHER FOR MASS PTS
          6 |C   PRGRMMR: TREADON         ORG: W/NP2      DATE: 93-03-02
          7 |C
          8 |C ABSTRACT:
          9 |C     THIS ROUTINE WILL APPLY A FOURTH OTHER FILTER (SMOOTHER)
         10 |C     OF A CONTINOUS FIELD AT MASS POINTS ON THE STAGGERED
         11 |C     E-GRID.  MULTIPLE APPLICATIONS OF THIS FILTER MAY BE
         12 |C     MADE.
         13 |C
         14 |C     IN THIS VERSION, EACH MPI TASK WORKS ON ITS OWN DATA.
         15 |C   .
         16 |C
         17 |C PROGRAM HISTORY LOG:
         18 |C   ??-??-??  ??? - SUBROUTINE P2FILT IN ETA MODEL.
         19 |C   93-03-02  RUSS TREADON - ADDED DOCBLOC
         20 |C   98-06-04  BLACK - CONVERSION TO 2-D
         21 |C   00-01-04  JIM TUCCILLO - MPI VERSION
         22 |C
         23 |C USAGE:    CALL P2FILT(MKOUNT,HBM2,FIELD)
         24 |C   INPUT ARGUMENT LIST:
         25 |C     MKOUNT   - NUMBER OF APPLICATIONS OF THE SMOOTHER.
         26 |C     HBM2     - MASS POINT BOUNDARY MASK.
         27 |C     FIELD    - MASS POINT FIELD TO BE SMOOTHED.
         28 |C
         29 |C   OUTPUT ARGUMENT LIST:
         30 |C     FIELD    - SMOOTHED MASS POINT FIELD.
         31 |C
         32 |C   OUTPUT FILES:
         33 |C     NONE
         34 |C
         35 |C   SUBPROGRAMS CALLED:
         36 |C     UTILITIES:
         37 |C       NONE
         38 |C     LIBRARY:
         39 |C       NONE
         40 |C
         41 |C   ATTRIBUTES:
         42 |C     LANGUAGE: FORTRAN
         43 |C     MACHINE : CRAY C-90
         44 |C$$$
         45 |C
         46 |C
         47 |C     INCLUDE GLOBAL PARAMETERS.  SET DEPENDENT VARIABLES.
         48 |C
         49 |C-----------------------------------------------------------------------
         50 |      INCLUDE "parmeta"
  1       1 |C-------------------------------------------------------------
  1       2 |C***  SET PRIMARY GRID DIMENSIONS AND PRESSURE OUTPUT LEVELS
  1       3 |      PARAMETER (IM=89,JM=143,LM=60,LSM=39)
  1       4 |C-------------------------------------------------------------
  1       5 |C***  SET THE NUMBER OF PEs IN THE I-DIRECTION AND J-DIRECTION
  1       6 |      PARAMETER(INPES=5,JNPES=6)
  1       7 |C-------------------------------------------------------------
  1       8 |C
  1       9 |      PARAMETER(IGSTL=-5,IGSTR=5)
  1      10 |      PARAMETER(JGSTL=-5,JGSTR=5)
  1      11 |      PARAMETER(ITAIL=IM-(INPES*(IM/INPES)))
  1      12 |      PARAMETER(JTAIL=JM-(JNPES*(JM/JNPES)))
  1      13 |C
  1      14 |      PARAMETER(IDIM1=IGSTL,IDIM2=IM/INPES+IGSTR+1)
  1      15 |      PARAMETER(JDIM1=JGSTL,JDIM2=JM/JNPES+JGSTR+1)
  1      16 |C
  1      17 |C-------------------------------------------------------------
         51 |C-----------------------------------------------------------------------
         52 |      INCLUDE "INDX.comm"
  2       1 |C-----------------------------------------------------------------------
  2       2 |                        C O M M O N /INDX/
  2       3 |     & IHE(JM),IHW(JM),IVE(JM),IVW(JM)
  2       4 |C-----------------------------------------------------------------------
         53 |      INCLUDE "CTLBLK.comm"
  3       1 |C-----------------------------------------------------------------------
  3       2 |      COMMON /CTLBLK/
  3       3 |     & SIGMA,RUN,IDAT(3),IHRST,FIRST,RESTRT,NFCST,NBC,LIST,IOUT
  3       4 |     &,DT,NTSD,NTSTM,NDDAMP,NPREC,IDTAD,NBOCO,NSHDE,NCP
  3       5 |     &,NPHS,NRADS,NRADL
  3       6 |      INTEGER NUM_PROCS, ME, JSTA, JEND,
  3       7 |     &        JSTA_M, JSTA_M2, JEND_M, JEND_M2, IUP, IDN, ICNT, IDSP,
  3       8 |     &        JSTA_2L, JEND_2U
  3       9 |      COMMON /PARALLEL/NUM_PROCS, ME, JSTA, JEND, JSTA_M, JEND_M,
  3      10 |     &                 JSTA_M2, JEND_M2, IUP, IDN,
  3      11 |     &                 ICNT(0:1023), IDSP(0:1023),
  3      12 |     &                 JSTA_2L, JEND_2U, NUM_SERVERS, MPI_COMM_INTER,
  3      13 |     &                 MPI_COMM_COMP
  3      14 |C-----------------------------------------------------------------------
         54 |C-----------------------------------------------------------------------
         55 |                            D I M E N S I O N
         56 |     1 HBM2(IM,JM),FIELD(IM,JM),WFIELD(IM,JM),FNE(IM,JM),FSE(IM,JM)
         57 |C
         58 |C***********************************************************************
         59 |C     START P2FILT HERE.
         60 |C
         61 |C     IF NUMBER OF PASSES IS LESS THAN 1, EXIT ROUTINE.
         62 |C
         63 |      IF(MKOUNT.LT.1)        RETURN
         64 |C
         65 |C     OTHERWISE, APPLY SMOOTHER MKOUNT TIMES.
         66 |C
         67 |      DO 400 KOUNT=1,MKOUNT
         68 |C
         69 |C        FILTERING STARTS
         70 |C
         71 |!$omp  parallel do
         72 |      DO J=JSTA,JEND
         73 |      DO I=1,IM
         74 |        FNE   (I,J)=0.0
         75 |        FSE   (I,J)=0.0
         76 |        WFIELD(I,J)=0.0
         77 |      ENDDO
         78 |      ENDDO
         79 |C
         80 |C        2-ND ORDER DIAGONAL CONTRIBUTIONS
         81 |C
         82 |       CALL EXCH(FIELD)
         83 |C
         84 |!$omp  parallel do
         85 |      DO J=JSTA,JEND_M
         86 |      DO I=1,IM-1
         87 |        FNE(I,J)=FIELD(I+IHE(J),J+1)-FIELD(I,J)
         88 |      ENDDO
         89 |      ENDDO
         90 |C
         91 |!$omp  parallel do
         92 |      DO J=JSTA_M,JEND
         93 |      DO I=1,IM-1
         94 |        FSE(I,J)=FIELD(I+IHE(J),J-1)-FIELD(I,J)
         95 |      ENDDO
         96 |      ENDDO
         97 |C
         98 |      CALL EXCH(FNE)
         99 |      CALL EXCH(FSE)
        100 |C
        101 |!$omp  parallel do
        102 |      DO J=JSTA_M2,JEND_M2
        103 |      DO I=2,IM-1
        104 |        WFIELD(I,J)=(FNE(I,J)-FNE(I+IHW(J),J-1)
        105 |     1           +FSE(I,J)-FSE(I+IHW(J),J+1))*HBM2(I,J)*0.125
        106 |      ENDDO
        107 |      ENDDO
        108 |C
        109 |C     4-TH ORDER DIAGONAL CONTRIBUTIONS
        110 |C
        111 |      CALL EXCH(WFIELD)
        112 |C
        113 |!$omp  parallel do
        114 |      DO J=JSTA,JEND_M
        115 |      DO I=1,IM-1
        116 |        FNE(I,J)=WFIELD(I+IHE(J),J+1)-WFIELD(I,J)
        117 |      ENDDO
        118 |      ENDDO
        119 |C
        120 |!$omp  parallel do
        121 |      DO J=JSTA_M,JEND
        122 |      DO I=1,IM-1
        123 |        FSE(I,J)=WFIELD(I+IHE(J),J-1)-WFIELD(I,J)
        124 |      ENDDO
        125 |      ENDDO
        126 |C
        127 |      CALL EXCH(FNE)
        128 |      CALL EXCH(FSE)
        129 |C
        130 |!$omp  parallel do
        131 |      DO J=JSTA_M2,JEND_M2
        132 |      DO I=2,IM-1
        133 |        FIELD(I,J)=FIELD(I,J)-(FNE(I,J)-FNE(I+IHW(J),J-1)
        134 |     1                        +FSE(I,J)-FSE(I+IHW(J),J+1))
        135 |     2                        *HBM2(I,J)*0.125
        136 |      ENDDO
        137 |      ENDDO
        138 |C
        139 | 400  CONTINUE
        140 |C
        141 |      CALL EXCH(FIELD)
        142 |C
        143 |C     END OF ROUTINE
        144 |      RETURN
        145 |      END
 
