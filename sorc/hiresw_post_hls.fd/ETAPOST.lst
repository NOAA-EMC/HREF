        123 |C
        124 |C     DECLARE VARIABLES.
        125 |C
        126 |C
        127 |C     INCLUDE COMMON BLOCKS.
        128 |      INCLUDE "OUTFIL.comm"
  4       1 |C
  4       2 |      CHARACTER*13 RSTFIL
  4       3 |      COMMON /OUTFIL/ ITAG,LRSTRT,RSTFIL
  4       4 |C
        129 |      INCLUDE "IOUNIT.comm"
  5       1 |C
  5       2 |      INTEGER STDOUT
  5       3 |C
  5       4 |      COMMON /IOUNIT/
  5       5 |     & STDOUT, LCNTRL, LUNCO, LUNLL, LUNOUT
  5       6 |C
        130 |      INCLUDE "CTLBLK.comm"
  6       1 |C-----------------------------------------------------------------------
  6       2 |      COMMON /CTLBLK/
  6       3 |     & SIGMA,RUN,IDAT(3),IHRST,FIRST,RESTRT,NFCST,NBC,LIST,IOUT
  6       4 |     &,DT,NTSD,NTSTM,NDDAMP,NPREC,IDTAD,NBOCO,NSHDE,NCP
  6       5 |     &,NPHS,NRADS,NRADL
  6       6 |      INTEGER NUM_PROCS, ME, JSTA, JEND,
  6       7 |     &        JSTA_M, JSTA_M2, JEND_M, JEND_M2, IUP, IDN, ICNT, IDSP,
  6       8 |     &        JSTA_2L, JEND_2U
  6       9 |      COMMON /PARALLEL/NUM_PROCS, ME, JSTA, JEND, JSTA_M, JEND_M,
  6      10 |     &                 JSTA_M2, JEND_M2, IUP, IDN,
  6      11 |     &                 ICNT(0:1023), IDSP(0:1023),
  6      12 |     &                 JSTA_2L, JEND_2U, NUM_SERVERS, MPI_COMM_INTER,
  6      13 |     &                 MPI_COMM_COMP
  6      14 |C-----------------------------------------------------------------------
        131 |C
        132 |C     SET HEADER WRITER FLAGS TO TRUE.
        133 |c
        134 |      common/tim_info/ETAFLD2_tim,ETA2P_tim,SURFCE2_tim, CLDRAD_tim,
        135 |     *                MISCLN_tim,FIXED_tim
        136 |C
        137 |      common/jjt/time_output, time_e2out
        138 |      real(8) time_output, time_e2out, time_initpost, rtc, ist
        139 |C
        140 |C     START HERE
        141 |C
        142 |      call start()
        143 |C
        144 |C     INITIALIZE MPI
        145 |
        146 |      CALL SETUP_SERVERS(ME,
        147 |     *                   NUM_PROCS,
        148 |     *                   NUM_SERVERS,
        149 |     *                   MPI_COMM_COMP,
        150 |     *                   MPI_COMM_INTER)
        151 |C
        152 |C     ME IS THE RANK
        153 |C     NUM_PROCS IS THE NUMBER OF TASKS DOING POSTING
        154 |C     NUM_SERVERS IS ONE IF THERE ARE MORE THAN ONE TOTAL MPI TASKS, OTHERWISE ZERO
        155 |C     MPI_COMM_COMP IS THE INTRACOMMUNICATOR
        156 |C     MPI_COMM_INTER IS THE INTERCOMMUNICATOR FOR COMMUNCATION BETWEEN TASK 0 OF THE
        157 |C        TASKS DOING THE POSTING AND THE I/O SERVER
        158 |C
        159 |C
        160 |C     IF WE HAVE MORE THAN 1 MPI TASK THEN WE WILL FIRE UP THE IO SERVER
        161 |C     THE LAST TASK ( IN THE CONTEXT OF MPI_COMM_WORLD ) IS THE I/O SERVER
        162 |C
        163 |      if ( me .ge. num_procs ) then
        164 |C
        165 |         call server
        166 |C
        167 |      else
        168 |C
        169 |      time_output = 0.
        170 |      time_e2out = 0.
        171 |      time_initpost = 0.
        172 |      CALL MPI_FIRST
        173 |C
        174 |      ETAFLD2_tim = 0.0
        175 |      ETA2P_tim = 0.0
        176 |      SURFCE2_tim = 0.0
        177 |      CLDRAD_tim = 0.0
        178 |      MISCLN_tim =0.0
        179 |      FIXED_tim =  0.0
        180 |      bbtim = timef()
        181 |c
        182 |C
        183 |C
        184 |c     IF(ME.EQ.0)THEN
        185 |c       CALL W3TAGB('ETA_ETAPOST',2000,0032,0094,'NP22')
        186 |c     ENDIF
        187 |C
        188 |C
        189 |C**************************************************************************
        190 |C
        191 |C     START PROGRAM ETAPOST.
        192 |C
        193 |      IF(ME.EQ.0)THEN
        194 |        READ(5,*)ITAG,NRSTRT,NPINCR
        195 |      ENDIF
        196 |C
        197 |      CALL MPI_BCAST(ITAG  ,1,MPI_INTEGER,0,MPI_COMM_COMP,IRTN)
        198 |      CALL MPI_BCAST(NRSTRT,1,MPI_INTEGER,0,MPI_COMM_COMP,IRTN)
        199 |      CALL MPI_BCAST(NPINCR,1,MPI_INTEGER,0,MPI_COMM_COMP,IRTN)
        200 |C
        201 |      CALL MPI_BARRIER(MPI_COMM_COMP,IRTN)
        202 |C
        203 |C     LOOP OVER THE RESTRT FILES
        204 |C
        205 |      DO 1000 NR=1,NRSTRT
        206 |      LUNCO=19
        207 |      LUNLL=29
        208 |C
        209 |C     REWIND ALL INPUT FILE UNIT NUMBERS (FOR CONTROL FILE
        210 |C     AND INTERPOLATION WEIGHTS)
        211 |C
        212 |      REWIND LCNTRL
        213 |      DO KER=19,39
        214 |        REWIND KER
        215 |      ENDDO
        216 |C
        217 |C     INITIALIZE POST COMMON BLOCKS
        218 |C
        219 |      ist = rtc()
        220 |      CALL INITPOST
        221 |      time_initpost = time_initpost + rtc() - ist
        222 |      IF(ME.EQ.0)THEN
        223 |        WRITE(STDOUT,*)'ETAPOST:  INITIALIZED POST COMMON BLOCKS'
        224 |      ENDIF
        225 |C
        226 |C     LOOP OVER THE OUTPUT GRID(S).  FIELD(S) AND
        227 |C     OUTPUT GRID(S) ARE SPECIFIED IN THE CONTROL
        228 |C     FILE.  WE PROCESS ONE GRID AND ITS FIELDS
        229 |C     AT A TIME.  THAT'S WHAT THIS LOOP DOES.
        230 |C
        231 | 10   CONTINUE
        232 |C
        233 |C        READ CONTROL FILE DIRECTING WHICH FIELDS ON WHICH
        234 |C        LEVELS AND TO WHICH GRID TO INTERPOLATE DATA TO.
        235 |C        VARIABLE IEOF.NE.0 WHEN THERE ARE NO MORE GRIDS
        236 |C        TO PROCESS.
        237 |C
        238 |         CALL READCNTRL2(IEOF)
        239 |         IF(ME.EQ.0)THEN
        240 |           WRITE(STDOUT,*)'ETAPOST:  RETURN FROM READCNTRL.  ',
        241 |     1          'IEOF=',IEOF
        242 |         ENDIF
        243 |         IF (IEOF.NE.0) GOTO 20
        244 |C
        245 |C        PROCESS SELECTED FIELDS.  FOR EACH SELECTED FIELD/LEVEL
        246 |C        WE GO THROUGH THE FOLLOWING STEPS:
        247 |C           (1) COMPUTE FIELD IF NEED BE
        248 |C           (2) INTERPOLATE FROM E-GRID TO OUTPUT GRID.
        249 |C           (3) WRITE FIELD TO OUTPUT FILE IN REQUESTED FORMAT.
        250 |C
        251 |         CALL PROCESS
        252 |         IF(ME.EQ.0)THEN
        253 |           WRITE(STDOUT,*)' '
        254 |           WRITE(STDOUT,*)'ETAPOST:  PREPARE TO PROCESS NEXT GRID'
        255 |         ENDIF
        256 |C
        257 |C     PROCESS NEXT GRID.
        258 |C
        259 |      GO TO 10
        260 |C
        261 |C     ALL GRIDS PROCESSED.
        262 |C
        263 | 20   CONTINUE
        264 |      IF(ME.EQ.0)THEN
        265 |        WRITE(STDOUT,*)' '
        266 |        WRITE(STDOUT,*)'ALL GRIDS PROCESSED.'
        267 |        WRITE(STDOUT,*)' '
        268 |      ENDIF
        269 |C
        270 |      ITAG=ITAG+NPINCR
        271 | 1000 CONTINUE
        272 |C
        273 |      print*, 'ETAFLD2_tim = ',  ETAFLD2_tim*1.0e-3
        274 |      print*, 'ETA2P_tim =  ',ETA2P_tim *1.0e-3
        275 |      print*, 'SURFCE2_tim =  ',SURFCE2_tim*1.0e-3
        276 |      print*, 'CLDRAD_tim =  ',CLDRAD_tim *1.0e-3
        277 |      print*, 'MISCLN_tim = ',MISCLN_tim*1.0e-3
        278 |      print*, 'FIXED_tim = ',FIXED_tim*1.0e-3
        279 |      print*, 'Total time = ',(timef() - bbtim) * 1.0e-3
        280 |      print*, 'Time for OUTPUT = ',time_output
        281 |      print*, 'Time for E2OUT = ',time_e2out
        282 |      print*, 'Time for INITPOST = ',time_initpost
        283 |
        284 |C
        285 |C     END OF PROGRAM.
        286 |C
        287 |C
        288 |c     IF(ME.EQ.0)THEN
        289 |c       CALL W3TAGE('ETA_ETAPOST')
        290 |c     ENDIF
        291 |C
        292 |C     MPI_LAST WILL SHUTDOWN THE IO SERVER, IF IT EXISTS
        293 |C
        294 |      CALL MPI_LAST
        295 |C
        296 |C
        297 |      end if
        298 |C
        299 |C
        300 |C
        301 |      call summary()
        302 |      CALL MPI_FINALIZE(MPI_COMM_WORLD,IERR)
        303 |      STOP0
        304 |      END
 
