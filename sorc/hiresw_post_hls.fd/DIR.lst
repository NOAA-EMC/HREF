         35 |C     MACHINE : CRAY Y-MP
         36 |C$$$
         37 |C
         38 |C***************************************************************************
         39 |C     START FUNCTION DIR HERE.
         40 |C
         41 |       IF(X.EQ.0.0)THEN
         42 |         IF(Y.GT.0.0)THEN
         43 |           DIR=90.
         44 |         ELSEIF(X.EQ.0.0)THEN
         45 |           DIR=360.
         46 |         ELSE
         47 |           DIR=270.
         48 |         ENDIF
         49 |       ELSE
         50 |         DIR=ATAN(Y/X)*180./3.14159
         51 |         IF(X.LT.0.0)THEN
         52 |           DIR=DIR+180.
         53 |         ENDIF
         54 |         IF(DIR.LT.0.0)THEN
         55 |           DIR=DIR+360.
         56 |         ENDIF
         57 |       ENDIF
         58 |       RETURN
         59 |       END
 
