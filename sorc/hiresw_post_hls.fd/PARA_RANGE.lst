          1 |      SUBROUTINE PARA_RANGE (N1,N2,NPROCS,IRANK,ISTA,IEND)
          2 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          3 |C                .      .    .
          4 |C SUBPROGRAM:    PARA_RANGE  SET UP DECOMPOSITION VALUES
          5 |C   PRGRMMR: TUCCILLO        ORG: IBM
          6 |C
          7 |C ABSTRACT:
          8 |C     SETS UP DECOMOSITION VALUES
          9 |C   .
         10 |C
         11 |C PROGRAM HISTORY LOG:
         12 |C   00-01-06  TUCCILLO - ORIGINAL
         13 |C
         14 |C USAGE:    CALL COLLECT(A)
         15 |C   INPUT ARGUMENT LIST:
         16 |C     N1 - FIRST INTERATE VALUE
         17 |C     N2 - LAST INTERATE VALUE
         18 |C     NPROCS - NUMBER OF MPI TASKS
         19 |C     IRANK - MY TAKS ID
         20 |C
         21 |C   OUTPUT ARGUMENT LIST:
         22 |C     ISTA - FIRST LOOP VALUE
         23 |C     IEND - LAST LOOP VALUE
         24 |C
         25 |C   OUTPUT FILES:
         26 |C     STDOUT  - RUN TIME STANDARD OUT.
         27 |C
         28 |C   SUBPROGRAMS CALLED:
         29 |C     UTILITIES:
         30 |C       NONE
         31 |C     LIBRARY:
         32 |C
         33 |C   ATTRIBUTES:
         34 |C     LANGUAGE: FORTRAN
         35 |C     MACHINE : IBM RS/6000 SP
         36 |C$$$
         37 |      implicit none
         38 |      integer n1,n2,nprocs,irank,ista,iend
         39 |      integer iwork1, iwork2
         40 |      iwork1 = ( n2 - n1 + 1 ) / nprocs
         41 |      iwork2 = mod ( n2 - n1 + 1, nprocs )
         42 |      ista = irank * iwork1 + n1 + min ( irank, iwork2 )
         43 |      iend = ista + iwork1 - 1
         44 |      if ( iwork2 .gt. irank ) iend = iend + 1
         45 |      end
 
