          1 |      SUBROUTINE MINMAX(A,IDIM,JDIM,AMIN,AMAX)
          2 |C
          3 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          4 |C                .      .    .
          5 |C SUBPROGRAM:    MINMAX      RETURNS (MIN,MAX) IN ARRAY
          6 |C   PRGRMMR: TREADON         ORG: W/NP2      DATE: 93-01-08
          7 |C
          8 |C ABSTRACT:
          9 |C     THIS ROUTINE FINDS THE MINIMUM AND MAXIMUM VALUES
         10 |C     IN THE PASSED ARRAY "A" WHICH CONTAINS NPTS ELEMENTS.
         11 |C   .
         12 |C
         13 |C PROGRAM HISTORY LOG:
         14 |C   ??-??-??  DAVID PLUMMER - SUBROUTINE MINMAX IN ETAPACKC.
         15 |C   93-01-08  RUSS TREADON - ADDED COMMENTS.
         16 |C
         17 |C USAGE:    CALL MINMAX(A,IDIM,JDIM,AMIN,AMAX)
         18 |C   INPUT ARGUMENT LIST:
         19 |C     A        - INPUT ARRAY TO SCAN.
         20 |C     IDIM,JDIM- DIMENSIONS OF ARRAY "A".
         21 |C
         22 |C   OUTPUT ARGUMENT LIST:
         23 |C     AMIN     - MINIMUM VALUE IN ARRAY "A".
         24 |C     AMAX     - MAXIMUM VALUE IN ARRAY "A".
         25 |C
         26 |C   OUTPUT FILES:
         27 |C     NONE
         28 |C
         29 |C   SUBPROGRAMS CALLED:
         30 |C     UTILITIES:
         31 |C       NONE
         32 |C     LIBRARY:
         33 |C       NONE
         34 |C
         35 |C   ATTRIBUTES:
         36 |C     LANGUAGE: FORTRAN
         37 |C     MACHINE : CRAY C-90
         38 |C$$$
         39 |C
         40 |C
         41 |C     DECLARE VARIABLES.
         42 |      REAL AMIN, AMAX
         43 |      REAL A(IDIM,JDIM)
         44 |C
         45 |      INCLUDE "CTLBLK.comm"
  1       1 |C-----------------------------------------------------------------------
  1       2 |      COMMON /CTLBLK/
  1       3 |     & SIGMA,RUN,IDAT(3),IHRST,FIRST,RESTRT,NFCST,NBC,LIST,IOUT
  1       4 |     &,DT,NTSD,NTSTM,NDDAMP,NPREC,IDTAD,NBOCO,NSHDE,NCP
  1       5 |     &,NPHS,NRADS,NRADL
  1       6 |      INTEGER NUM_PROCS, ME, JSTA, JEND,
  1       7 |     &        JSTA_M, JSTA_M2, JEND_M, JEND_M2, IUP, IDN, ICNT, IDSP,
  1       8 |     &        JSTA_2L, JEND_2U
  1       9 |      COMMON /PARALLEL/NUM_PROCS, ME, JSTA, JEND, JSTA_M, JEND_M,
  1      10 |     &                 JSTA_M2, JEND_M2, IUP, IDN,
  1      11 |     &                 ICNT(0:1023), IDSP(0:1023),
  1      12 |     &                 JSTA_2L, JEND_2U, NUM_SERVERS, MPI_COMM_INTER,
  1      13 |     &                 MPI_COMM_COMP
  1      14 |C-----------------------------------------------------------------------
         46 |C
         47 |C*******************************************************************
         48 |C     START MINMAX HERE.
         49 |C
         50 |C     INITIALIZE VARIABLES.
         51 |      AMIN =  1.E50
         52 |      AMAX = -1.E50
         53 |C
         54 |C     FIND MIN AND MAX VALUES IN ARRAY.
         55 |C
         56 |      DO J=JSTA,JEND
         57 |      DO I=1,IDIM
         58 |        AMIN = AMIN1(AMIN,A(I,J))
         59 |        AMAX = AMAX1(AMAX,A(I,J))
         60 |      ENDDO
         61 |      ENDDO
         62 |C
         63 |C     END OF ROUTINE.
         64 |      RETURN
         65 |      END
 
