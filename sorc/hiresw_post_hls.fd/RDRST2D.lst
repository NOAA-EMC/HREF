          1 |      SUBROUTINE RDRST2D(ARR,IM,JM,LUNIT,NREC0,NREC1,NF,PRE)
          2 |C
          3 |C------------------------------------------------------------
          4 |      PARAMETER (KM=7)
          5 |C
          6 |      INCLUDE "CTLBLK.comm"
  1       1 |C-----------------------------------------------------------------------
  1       2 |      COMMON /CTLBLK/
  1       3 |     & SIGMA,RUN,IDAT(3),IHRST,FIRST,RESTRT,NFCST,NBC,LIST,IOUT
  1       4 |     &,DT,NTSD,NTSTM,NDDAMP,NPREC,IDTAD,NBOCO,NSHDE,NCP
  1       5 |     &,NPHS,NRADS,NRADL
  1       6 |      INTEGER NUM_PROCS, ME, JSTA, JEND,
  1       7 |     &        JSTA_M, JSTA_M2, JEND_M, JEND_M2, IUP, IDN, ICNT, IDSP,
  1       8 |     &        JSTA_2L, JEND_2U
  1       9 |      COMMON /PARALLEL/NUM_PROCS, ME, JSTA, JEND, JSTA_M, JEND_M,
  1      10 |     &                 JSTA_M2, JEND_M2, IUP, IDN,
  1      11 |     &                 ICNT(0:1023), IDSP(0:1023),
  1      12 |     &                 JSTA_2L, JEND_2U, NUM_SERVERS, MPI_COMM_INTER,
  1      13 |     &                 MPI_COMM_COMP
  1      14 |C-----------------------------------------------------------------------
          7 |C
          8 |      REAL ARR(IM,JM),RDDUM(IM,JM)
          9 |C
         10 |      INTEGER IDATx(3)
         11 |      LOGICAL RUN,PRE
         12 |      CHARACTER LABEL*32
         13 |C------------------------------------------------------------
         14 |C***
         15 |C***  ARR: ARRAY NEEDED FROM RESTRT FILE
         16 |C***
         17 |C***  RDDUM: DUMMY ARRAY USED TO READ THE RESTRT FILE
         18 |C***
         19 |C***  IM,JM: HORIZONTAL DIMENSIONS OF ARR AND RDDUM
         20 |C***
         21 |C***  KM: VERTICAL DIMENSION OF RDDUM; THIS IS THE MAXIMUM
         22 |C***      NUMBER OF 2-D ARRAYS THAT OCCUR ON ANY SINGLE
         23 |C***      RECORD OF THE FILE
         24 |C***
         25 |C***  LUNIT: UNIT NUMBER OF THE RESTRT FILE BEING READ
         26 |C***
         27 |C***  NREC0: THE RECORD AFTER WHICH THE POINTER IS SET
         28 |C***         WHEN THIS ROUTINE IS ENTERED
         29 |C***
         30 |C***  NREC1: THE NUMBER OF THE RECORD FROM WHICH
         31 |C***         WE WANT TO EXTRACT
         32 |C***
         33 |C***  NF: SINCE THERE MAY BE MORE THAN ONE 2-D ARRAY IN EACH
         34 |C***      RECORD, THIS IS THE NUMBER OF THE FIELD WE ACTUALLY
         35 |C***      WANT IN THE GIVEN RECORD; NF CANNOT EXCEED KM
         36 |C***
         37 |C***  PRE: A LOGICAL FLAG INDICATING WHETHER OR NOT TO READ
         38 |C***       THE EXTRA WORDS WHICH OCCUR ON SOME OF THE RECORDS
         39 |C***       PRECEDING THE 2-D ARRAYS
         40 |C***
         41 |C------------------------------------------------------------
         42 |
         43 |c     print *, ' *** IN RDRST2D ***'
         44 |      NDIF=NREC1-NREC0
         45 |C***
         46 |C***  GO TO START OF THE DESIRED RECORD
         47 |C***
         48 |      IF(NDIF.GT.0)THEN
         49 |        DO KOUNT=1,NDIF
         50 |          READ(LUNIT)
         51 |        ENDDO
         52 |      ELSEIF(NDIF.LT.0)THEN
         53 |        NDIF=-NDIF
         54 |        DO KOUNT=1,NDIF
         55 |          BACKSPACE LUNIT
         56 |        ENDDO
         57 |      ENDIF
         58 |C***
         59 |C***  READ THE ARRAY FROM THIS RECORD
         60 |C***
         61 |      IF(PRE)THEN
         62 |        READ(LUNIT)RUN,IDATx,IHRST,NTSD,LABEL,
         63 |     1              (((RDDUM(I,J),I=1,IM),J=1,JM),K=1,NF)
         64 |      ELSE
         65 |        READ(LUNIT)(((RDDUM(I,J),I=1,IM),J=1,JM),K=1,NF)
         66 |      ENDIF
         67 |C
         68 |      CALL EXTRACT(RDDUM, ARR )
         69 |c     DO J=JSTA,JEND
         70 |c     DO I=1,IM
         71 |c       ARR(I,J)=RDDUM(I,J)
         72 |c     ENDDO
         73 |c     ENDDO
         74 |c
         75 |      RETURN
         76 |      END
 
