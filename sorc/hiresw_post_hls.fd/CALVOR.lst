  1      15 |      PARAMETER(JDIM1=JGSTL,JDIM2=JM/JNPES+JGSTR+1)
  1      16 |C
  1      17 |C-------------------------------------------------------------
         51 |      INCLUDE "params"
  2       1 |C
  2       2 |C     ARRAY DIMENSIONS.
  2       3 |      PARAMETER	
  2       4 |     & (IMT=2*IM-1,JMT=JM/2+1)
  2       5 |      PARAMETER
  2       6 |     & (IM1=IM-1,JM2=JM-2,JAM=6+2*(JM-10)
  2       7 |     &, NINC=2*IM-1
  2       8 |     &, LM1=LM-1,LP1=LM+1
  2       9 |     &, LB=2*IM+JM-3)
  2      10 |C
  2      11 |C     SMOOTHING PARAMETERS.
  2      12 |      PARAMETER
  2      13 |     & (KSMUD=4,KSLPD=1,KSSLP=2,KSFI=2,KST=2
  2      14 |     &, KSUV=2,KSRH=2,KSAV=2,KSLI=2,KSLP=2
  2      15 |     &, SMTHA=-1.8E-4,SMTHB=16.,NSUMD=1)
  2      16 |C
  2      17 |C     DIMENSIONS FOR PROFILE OUPUT ARRAYS.
  2      18 |      PARAMETER
  2      19 |     & (NSTAT=100,NWORD=6+6*LM+9,NWORD3=NWORD-3,NWORD6=NWORD-6)
  2      20 |C
  2      21 |C     CONSTANTS.
  2      22 |      PARAMETER
  2      23 |     & (CM1=2937.4,CM2=4.9283,CM3=23.5518)
  2      24 |      PARAMETER
  2      25 |     & (D00=0.0,D1=0.1,D01=0.01,D001=0.001,D50=0.50,H999=999.
  2      26 |     &, H1=1.0,H2=2.0,H4=4.0,H99999=99999.,H1M12=1.E-12
  2      27 |     &, H1000=1000.,H1M5=1.E-5,D125=.125,D25=0.25,H100=100.
  2      28 |     &, H10000=10000.,H10E5=1.E5,CAPA=0.28589641E0
  2      29 |     &, D115=0.115,D608=0.608,D05=0.05,D0065=0.0065
  2      30 |     &, H1E2=100.,H1E4=1.E4,H1M2=1.E-2)
  2      31 |      PARAMETER
  2      32 |     & (G=9.8,RD=287.04,ROG=RD/G,GI=1./G
  2      33 |     &, OVERRC=1.75,AD05=OVERRC*D05,CFT0=OVERRC-H1
  2      34 |     &, DTR=1.745329E-2,RTD=1./DTR,ERAD=6.376E6,CP=1004.6
  2      35 |     &, RCAPA=1./CAPA,P1000=1000.E2,DP10M=110
  2      36 |     &, ELWV=2.50E6,CPDR=CP/RD,RG=1./GI,ELDR=ELWV/RD
  2      37 |     &, BETA=.00367,BTG=BETA*9.8)
  2      38 |      PARAMETER
  2      39 |     & (FMW=18.015,FMD=28.964,EPS=FMW/FMD,ONEPS=1.-EPS,TFRZ=273.16
  2      40 |     &, RDOCP=RD/CP,LHEAT=2.5E6,LHTOCP=LHEAT/CP,EPSQ2=0.2
  2      41 |     &, PQ0=379.90516,A2=17.2693882,A3=273.16,A4=35.86)
         52 |      PARAMETER (OMEGA=7.292E-5,TWOMG=2.*OMEGA)
         53 |C
         54 |C     DECLARE VARIABLES.
         55 |C
         56 |      LOGICAL RUN,FIRST,RESTRT,SIGMA,OLDRD,STRD
         57 |      REAL ABSV(IM,JM), UWND(IM,JM), VWND(IM,JM)
         58 |C
         59 |C     DECLARE COMMONS.
         60 |      INCLUDE "CTLBLK.comm"
  3       1 |C-----------------------------------------------------------------------
  3       2 |      COMMON /CTLBLK/
  3       3 |     & SIGMA,RUN,IDAT(3),IHRST,FIRST,RESTRT,NFCST,NBC,LIST,IOUT
  3       4 |     &,DT,NTSD,NTSTM,NDDAMP,NPREC,IDTAD,NBOCO,NSHDE,NCP
  3       5 |     &,NPHS,NRADS,NRADL
  3       6 |      INTEGER NUM_PROCS, ME, JSTA, JEND,
  3       7 |     &        JSTA_M, JSTA_M2, JEND_M, JEND_M2, IUP, IDN, ICNT, IDSP,
  3       8 |     &        JSTA_2L, JEND_2U
  3       9 |      COMMON /PARALLEL/NUM_PROCS, ME, JSTA, JEND, JSTA_M, JEND_M,
  3      10 |     &                 JSTA_M2, JEND_M2, IUP, IDN,
  3      11 |     &                 ICNT(0:1023), IDSP(0:1023),
  3      12 |     &                 JSTA_2L, JEND_2U, NUM_SERVERS, MPI_COMM_INTER,
  3      13 |     &                 MPI_COMM_COMP
  3      14 |C-----------------------------------------------------------------------
         61 |      INCLUDE "DYNAMD.comm"
  4       1 |C-----------------------------------------------------------------------
  4       2 |                             C O M M O N /DYNAM/
  4       3 |     & DY,CPGFV,EN,ENT,RDYN,PTDYN,F4D,F4Q,EF4T
  4       4 |     &,DETAD(LM),RDETA(LM),AETAD(LM),F4Q2(LM),ETAD(LP1),DFL(LP1)
  4       5 |C
  4       6 |     &,EM    (JAM),EMT   (JAM)
  4       7 |C
  4       8 |     &,DX   (IM,JM),WPDAR(IM,JM),CPGFU(IM,JM),CURV (IM,JM),FCP  (IM,JM)
  4       9 |     &,FDIV (IM,JM),FAD  (IM,JM),F    (IM,JM),DDMPU(IM,JM),DDMPV(IM,JM)
  4      10 |C-----------------------------------------------------------------------
         62 |      INCLUDE "MAPOT.comm"
  5       1 |C-----------------------------------------------------------------------
  5       2 |                             C O M M O N /MAPOT/
  5       3 |     & LSL,IXM,IYM
  5       4 |     &,ISHDE(99)
  5       5 |     &,R,PT,TSPH
  5       6 |     &,WBD,SBD,TLM0D,TPH0D,DLMD,DPHD,CMLD,DP30,X1P,Y1P
  5       7 |     &,DISLP,Z0SLP,SPL(LSM),ALSL(LSM)
  5       8 |     &,DETA(LM),AETA(LM),ETA(LP1),TSHDE(99)
  5       9 |     &,ERLAM0,CPHI0,SPHI0
  5      10 |     &,KUNIT,LUNIT,MUNIT,IUNIT,NUNIT
  5      11 |     &,DSG1(LM),SGML1(LM),SG1(LP1),PDTOP
  5      12 |     &,DSG2(LM),SGML2(LM),SG2(LP1)
  5      13 |C-----------------------------------------------------------------------
         63 |      INCLUDE "OPTIONS.comm"
  6       1 |C
  6       2 |      COMMON /OPTIONS/ SPVAL,IBESSL,KSB,IOFFS,IFLAG,SATDEL
         64 |      INCLUDE "INDX.comm"
  7       1 |C-----------------------------------------------------------------------
  7       2 |                        C O M M O N /INDX/
  7       3 |     & IHE(JM),IHW(JM),IVE(JM),IVW(JM)
  7       4 |C-----------------------------------------------------------------------
         65 |C
         66 |C***************************************************************************
         67 |C     START CALVOR HERE.
         68 |C
         69 |C     LOOP TO COMPUTE ABSOLUTE VORTICITY FROM WINDS.
         70 |C
         71 |!$omp  parallel do
         72 |      DO J=JSTA,JEND
         73 |      DO I=1,IM
         74 |        ABSV(I,J) = D00
         75 |      ENDDO
         76 |      ENDDO
         77 |C
         78 |      CALL EXCH(UWND)
         79 |      CALL EXCH(F)
         80 |!$omp  parallel do
         81 |!$omp& private(favg,tphi,uavg)
         82 |      DO J=JSTA_M2,JEND_M2
         83 |        TPHI=(J-JMT)*DPHD*DTR
         84 |        DO I=2,IM-1
         85 |          UAVG=0.25*(UWND(I+IHE(J),J)+UWND(I+IHW(J),J)
         86 |     1              +UWND(I,J+1)+UWND(I,J-1))
         87 |          FAVG=0.25*(F(I+IHE(J),J)+F(I+IHW(J),J)
         88 |     1              +F(I,J+1)+F(I,J-1))
         89 |          ABSV(I,J)=((VWND(I+IHE(J),J)-VWND(I+IHW(J),J))/(2.*DX(I,J))
         90 |     1              -(UWND(I,J+1)-UWND(I,J-1))/(2.*DY)
         91 |     2              +UAVG*TAN(TPHI)/ERAD+2.*FAVG/DT)*HBM2(I,J)
         92 |        ENDDO
         93 |      ENDDO
         94 |C
         95 |C     END OF ROUTINE.
         96 |C
         97 |      RETURN
         98 |      END
 
