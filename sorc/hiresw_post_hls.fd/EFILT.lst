          1 |      SUBROUTINE EFILT(EGRID)
          2 |C$$$  SUBPROGRAM DOCUMENTATION BLOCK
          3 |C                .      .    .
          4 |C SUBPROGRAM:    EFILT       HEAVY HANDED E-GRID SMOOTHER
          5 |C   PRGRMMR: TREADON         ORG: W/NP2      DATE: 93-03-24
          6 |C
          7 |C ABSTRACT:
          8 |C     THIS ROUTINE IS A HEAVY HANDED SMOOTHER THAT WORKS ON
          9 |C     E-GRID MASS POINTS.  IT IS BASED ON CODE IN THE OLD
         10 |C     ETA POST PROCESSOR SUBROUTINE, OUTMAP.  CURRENTLY,
         11 |C     THIS SMOOTHER IS HARDCODED TO BE USED ONLY ON VORTICITY
         12 |C     FIELDS IF THE USER ACTIVATES E-GRID SMOOTHING OF
         13 |C     VORTICITY. IT DOES A VERY NICE JOB OF SMOOTHING THE
         14 |C     VORTICITY FIELD.
         15 |C   .
         16 |C
         17 |C PROGRAM HISTORY LOG:
         18 |C   93-03-24  RUSS TREADON
         19 |C   98-06-04  BLACK - CONVERSION TO 2-D
         20 |C
         21 |C USAGE:    CALL EFILT(EGRID)
         22 |C   INPUT ARGUMENT LIST:
         23 |C     EGRID    - ARRAY OF DATA ON E-GRID
         24 |C
         25 |C   OUTPUT ARGUMENT LIST:
         26 |C     EGRID    - ARRAY OF SMOOTHED DATA ON E-GRID
         27 |C
         28 |C   OUTPUT FILES:
         29 |C     NONE
         30 |C
         31 |C   SUBPROGRAMS CALLED:
         32 |C     UTILITIES:
         33 |C       NONE
         34 |C     LIBRARY:
         35 |C       COMMON   - MASKS
         36 |C                  DYNAMD
         37 |C                  INDX
         38 |C
         39 |C   ATTRIBUTES:
         40 |C     LANGUAGE: FORTRAN
         41 |C     MACHINE : CRAY Y-MP
         42 |C$$$
         43 |C
         44 |      use masks
         45 |C
         46 |C     INCLUDE PARAMETERS.
         47 |      INCLUDE "parmeta"
  1       1 |C-------------------------------------------------------------
  1       2 |C***  SET PRIMARY GRID DIMENSIONS AND PRESSURE OUTPUT LEVELS
  1       3 |      PARAMETER (IM=89,JM=143,LM=60,LSM=39)
  1       4 |C-------------------------------------------------------------
  1       5 |C***  SET THE NUMBER OF PEs IN THE I-DIRECTION AND J-DIRECTION
  1       6 |      PARAMETER(INPES=5,JNPES=6)
  1       7 |C-------------------------------------------------------------
  1       8 |C
  1       9 |      PARAMETER(IGSTL=-5,IGSTR=5)
  1      10 |      PARAMETER(JGSTL=-5,JGSTR=5)
  1      11 |      PARAMETER(ITAIL=IM-(INPES*(IM/INPES)))
  1      12 |      PARAMETER(JTAIL=JM-(JNPES*(JM/JNPES)))
  1      13 |C
  1      14 |      PARAMETER(IDIM1=IGSTL,IDIM2=IM/INPES+IGSTR+1)
  1      15 |      PARAMETER(JDIM1=JGSTL,JDIM2=JM/JNPES+JGSTR+1)
  1      16 |C
  1      17 |C-------------------------------------------------------------
         48 |      INCLUDE "params"
  2       1 |C
  2       2 |C     ARRAY DIMENSIONS.
  2       3 |      PARAMETER	
  2       4 |     & (IMT=2*IM-1,JMT=JM/2+1)
  2       5 |      PARAMETER
  2       6 |     & (IM1=IM-1,JM2=JM-2,JAM=6+2*(JM-10)
  2       7 |     &, NINC=2*IM-1
  2       8 |     &, LM1=LM-1,LP1=LM+1
  2       9 |     &, LB=2*IM+JM-3)
  2      10 |C
  2      11 |C     SMOOTHING PARAMETERS.
  2      12 |      PARAMETER
  2      13 |     & (KSMUD=4,KSLPD=1,KSSLP=2,KSFI=2,KST=2
  2      14 |     &, KSUV=2,KSRH=2,KSAV=2,KSLI=2,KSLP=2
  2      15 |     &, SMTHA=-1.8E-4,SMTHB=16.,NSUMD=1)
  2      16 |C
  2      17 |C     DIMENSIONS FOR PROFILE OUPUT ARRAYS.
  2      18 |      PARAMETER
  2      19 |     & (NSTAT=100,NWORD=6+6*LM+9,NWORD3=NWORD-3,NWORD6=NWORD-6)
  2      20 |C
  2      21 |C     CONSTANTS.
  2      22 |      PARAMETER
  2      23 |     & (CM1=2937.4,CM2=4.9283,CM3=23.5518)
  2      24 |      PARAMETER
  2      25 |     & (D00=0.0,D1=0.1,D01=0.01,D001=0.001,D50=0.50,H999=999.
  2      26 |     &, H1=1.0,H2=2.0,H4=4.0,H99999=99999.,H1M12=1.E-12
  2      27 |     &, H1000=1000.,H1M5=1.E-5,D125=.125,D25=0.25,H100=100.
  2      28 |     &, H10000=10000.,H10E5=1.E5,CAPA=0.28589641E0
  2      29 |     &, D115=0.115,D608=0.608,D05=0.05,D0065=0.0065
  2      30 |     &, H1E2=100.,H1E4=1.E4,H1M2=1.E-2)
  2      31 |      PARAMETER
  2      32 |     & (G=9.8,RD=287.04,ROG=RD/G,GI=1./G
  2      33 |     &, OVERRC=1.75,AD05=OVERRC*D05,CFT0=OVERRC-H1
  2      34 |     &, DTR=1.745329E-2,RTD=1./DTR,ERAD=6.376E6,CP=1004.6
  2      35 |     &, RCAPA=1./CAPA,P1000=1000.E2,DP10M=110
  2      36 |     &, ELWV=2.50E6,CPDR=CP/RD,RG=1./GI,ELDR=ELWV/RD
  2      37 |     &, BETA=.00367,BTG=BETA*9.8)
  2      38 |      PARAMETER
  2      39 |     & (FMW=18.015,FMD=28.964,EPS=FMW/FMD,ONEPS=1.-EPS,TFRZ=273.16
  2      40 |     &, RDOCP=RD/CP,LHEAT=2.5E6,LHTOCP=LHEAT/CP,EPSQ2=0.2
  2      41 |     &, PQ0=379.90516,A2=17.2693882,A3=273.16,A4=35.86)
         49 |C
         50 |C     DECLARE VARIABLES.
         51 |      REAL EGRID(IM,JM),WORK1(IM,JM),WORK2(IM,JM),WORK3(IM,JM)
         52 |C
         53 |C     INCLUDE COMMON BLOCKS.
         54 |      INCLUDE "DYNAMD.comm"
  3       1 |C-----------------------------------------------------------------------
  3       2 |                             C O M M O N /DYNAM/
  3       3 |     & DY,CPGFV,EN,ENT,RDYN,PTDYN,F4D,F4Q,EF4T
  3       4 |     &,DETAD(LM),RDETA(LM),AETAD(LM),F4Q2(LM),ETAD(LP1),DFL(LP1)
  3       5 |C
  3       6 |     &,EM    (JAM),EMT   (JAM)
  3       7 |C
  3       8 |     &,DX   (IM,JM),WPDAR(IM,JM),CPGFU(IM,JM),CURV (IM,JM),FCP  (IM,JM)
  3       9 |     &,FDIV (IM,JM),FAD  (IM,JM),F    (IM,JM),DDMPU(IM,JM),DDMPV(IM,JM)
  3      10 |C-----------------------------------------------------------------------
         55 |      INCLUDE "INDX.comm"
  4       1 |C-----------------------------------------------------------------------
  4       2 |                        C O M M O N /INDX/
  4       3 |     & IHE(JM),IHW(JM),IVE(JM),IVW(JM)
  4       4 |C-----------------------------------------------------------------------
         56 |C
         57 |C*******************************************************************
         58 |C     START EFILT HERE.
         59 |C
         60 |C     LOAD PASSED E-GRID FIELD INTO WORK ARRAY.
         61 |
         62 |!$omp  parallel do
         63 |      DO J=1,JM
         64 |      DO I=1,IM
         65 |        WORK1(I,J)=EGRID(I,J)
         66 |      ENDDO
         67 |      ENDDO
         68 |C
         69 |C     LOAD WEIGHT ARRAY.
         70 |!$omp  parallel do
         71 |      DO J=3,JM-2
         72 |      DO I=2,IM-1
         73 |         WORK3(I,J)=1./(4.* HBM2(I,J)+
         74 |     1        2.*(HBM2(I+IHE(J),J+1)+HBM2(I+IHW(J),J+1)+
         75 |     2        HBM2(I+IHE(J),J-1)+HBM2(I+IHW(J),J-1))+
         76 |     3        HBM2(I+1,J)+HBM2(I-1,J)+
         77 |     4        HBM2(I,J+2)+HBM2(I,J-2))
         78 |      ENDDO
         79 |      ENDDO
         80 |C
         81 |C     SMOOTHING LOOP.  SMTHA AND SMTHB ARE PARAMETERS SET IN
         82 |C     INCLUDE FILE PARAMS.
         83 |      MXFILT = NINT(SMTHA*DY+SMTHB)
         84 |C
         85 |      DO 50 NFILT = 1,MXFILT
         86 |!$omp  parallel do
         87 |         DO J=3,JM-2
         88 |         DO I=2,IM-1
         89 |            WORK2(I,J) = 4.*WORK1(I,J)+
         90 |     1           2.*(WORK1(I+IHE(J),J+1)+WORK1(I+IHW(J),J+1)+
         91 |     2           WORK1(I+IHE(J),J-1)+WORK1(I+IHW(J),J-1))+
         92 |     3           WORK1(I+1,J)+WORK1(I-1,J)+
         93 |     4           WORK1(I,J+2)+WORK1(I,J-2)
         94 |         ENDDO
         95 |         ENDDO
         96 |C
         97 |!$omp  parallel do
         98 |         DO J=3,JM-2
         99 |         DO I=2,IM-1
        100 |           WORK1(I,J)=WORK3(I,J)*WORK2(I,J)*HBM2(I,J)
        101 |         ENDDO
        102 |         ENDDO
        103 |   50 CONTINUE
        104 |C
        105 |C     LOAD SMOOTHED EGRID INTO OUTPUT ARRAY.
        106 |!$omp  parallel do
        107 |      DO J=1,JM
        108 |      DO I=1,IM
        109 |        EGRID(I,J)=WORK1(I,J)*HBM2(I,J)
        110 |      ENDDO
        111 |      ENDDO
        112 |C
        113 |C     END OF ROUTINE.
        114 |      RETURN
        115 |      END
 
