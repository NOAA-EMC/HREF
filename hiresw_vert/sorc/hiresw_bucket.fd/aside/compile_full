set -x

cp parmeta.centeast parmeta

ncepxlf -o WRFBUCKET.x WRFBUCKET.F \
-L/nwprod/lib/ -lw3_4 -lbacio_4

cp WRFBUCKET.x hiresw_pcpbucket_cent
mv WRFBUCKET.x hiresw_pcpbucket_east

cp parmeta.west parmeta

ncepxlf -o WRFBUCKET.x WRFBUCKET.F \
-L/nwprod/lib/ -lw3_4 -lbacio_4

cp WRFBUCKET.x hiresw_pcpbucket_west

cp parmeta.alaska parmeta

ncepxlf -o WRFBUCKET.x WRFBUCKET.F \
-L/nwprod/lib/ -lw3_4 -lbacio_4

cp WRFBUCKET.x hiresw_pcpbucket_ak

cp parmeta.hipr parmeta

ncepxlf -o WRFBUCKET.x WRFBUCKET.F \
-L/nwprod/lib/ -lw3_4 -lbacio_4

cp WRFBUCKET.x hiresw_pcpbucket_hi
mv WRFBUCKET.x hiresw_pcpbucket_pr

cp parmeta.full4km parmeta
ncepxlf -o WRFBUCKET.x WRFBUCKET.F \
-L/nwprod/lib/ -lw3_4 -lbacio_4
cp WRFBUCKET.x hiresw_pcpbucket_full4km

cp parmeta.ak4km parmeta
ncepxlf -o WRFBUCKET.x WRFBUCKET.F \
-L/nwprod/lib/ -lw3_4 -lbacio_4
cp WRFBUCKET.x hiresw_pcpbucket_ak4km

cp parmeta.hipr4km parmeta
ncepxlf -o WRFBUCKET.x WRFBUCKET.F \
-L/nwprod/lib/ -lw3_4 -lbacio_4
cp WRFBUCKET.x hiresw_pcpbucket_hipr4km

cp parmeta.full5km parmeta
ncepxlf -o WRFBUCKET.x WRFBUCKET.F \
-L/nwprod/lib/ -lw3_4 -lbacio_4
cp WRFBUCKET.x hiresw_pcpbucket_full5km

cp parmeta.ak5km parmeta
ncepxlf -o WRFBUCKET.x WRFBUCKET.F \
-L/nwprod/lib/ -lw3_4 -lbacio_4
cp WRFBUCKET.x hiresw_pcpbucket_ak5km

cp parmeta.hipr5km parmeta
ncepxlf -o WRFBUCKET.x WRFBUCKET.F \
-L/nwprod/lib/ -lw3_4 -lbacio_4
cp WRFBUCKET.x hiresw_pcpbucket_hipr5km
