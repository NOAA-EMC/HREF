&share
 wrf_core = 'NMM',
 max_dom = 1,
 start_date = 'YSTART-MSTART-DSTART_HSTART:00:00'
 end_date   = 'YEND-MEND-DEND_HEND:00:00'
 interval_seconds = 3600
 io_form_geogrid = 1,
 debug_level = 1 
/

&geogrid
