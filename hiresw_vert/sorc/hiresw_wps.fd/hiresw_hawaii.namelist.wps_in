 dx = .0260,
 dy = .0256, 
 map_proj = 'rotated_ll',
 ref_lat   = 20.25
 ref_lon   = -157.35
 truelat1  =  20.25,
 truelat2  =  20.25,
 stand_lon = -157.35,
 geog_data_path = '/meso/noscrub/wx20py/geog'
/
 ref_x     = ,
 ref_y     = ,
 opt_geogrid_tbl_path = 'geogrid/'

&ungrib
