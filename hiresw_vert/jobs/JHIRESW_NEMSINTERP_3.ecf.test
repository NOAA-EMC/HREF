export NEST=$1
export MODEL=$2
export cyc=$3

export MEMBER=ctl
export envir=prod

########################################
# Preliminary data setup step
########################################

#MP $SMSBIN/smsinit $LOADL_STEP_ID

set -xa
# #### 07/30/2004 ###################
# SET SHELL PROCESSING VARIABLES
# ###################################
export PS4='$SECONDS + ' 
date

########################################################### 
# obtain unique process id (pid) and make temp directories
###########################################################
export pid=$$
#MP export DATA=/tmpnwprd/hiresw_${NEST}_${MODEL}_${MEMBER}_${cyc}_${envir}
# export DATA=/scratch2/portfolios/NCEPDEV/meso/scrub/Matthew.Pyle/tmpnwprd/hiresw_${NEST}_${MODEL}_${MEMBER}_${cyc}_${envir}
export DATA=/ptmpd1/Matthew.Pyle/tmpnwprd/hiresw_${NEST}_${MODEL}_${MEMBER}_${cyc}_${envir}

mkdir -p $DATA/run_nemsinterp_3
cd $DATA/run_nemsinterp_3 
####################################
# File To Log Msgs
####################################
#MP export jlogfile=/com/logs/jlogfile

####################################
# Determine Job Output Name on System
####################################
export outid="LL$job"
export jobid="${outid}.o${pid}"
export pgmout="OUTPUT.${pid}"

export cycle=t${cyc}z 

###############################
# Specify NET and RUN name
###############################
export NET=hiresw
export RUN=${NEST}${MODEL}

##################################################
