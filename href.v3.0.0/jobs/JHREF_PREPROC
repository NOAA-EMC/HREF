echo "$USHhref/preprocess_fv3_1h.sh.ecf ${cyc} 26 ${NEST} &" >> poe.eee
echo "$USHhref/preprocess_fv3_1h.sh.ecf ${cyc} 27 ${NEST} &" >> poe.eee
echo "$USHhref/preprocess_fv3_1h.sh.ecf ${cyc} 28 ${NEST} &" >> poe.eee
