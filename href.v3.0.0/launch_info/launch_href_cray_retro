cyc=$1
dom=$2
date=$3


module load prod_util

echo cyc $cyc

dstart=$date$cyc

dend=`$NDATE -24 $dstart`

datem1=`echo $dend | cut -c1-8`

echo $date $datem1

cat runpoe.sh_in_cray_retro | sed s:_CYC_:${cyc}:g | sed s:_DATE_:${date}:g | \
                              sed s:_DATEm1_:${datem1}:g | sed s:_DOM_:${dom}:g   > launch_hrefrunpoe_${cyc}


bsub < launch_hrefrunpoe_${cyc}
# rm launch_hrefrunpoe_${cyc}
