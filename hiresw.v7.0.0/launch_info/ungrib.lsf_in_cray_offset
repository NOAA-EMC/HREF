export COMINgfs=$COMROOTp2/gfs/prod/gfs.${DATEgfs}
export COMINgfs=/gpfs/hps3/emc/meso/noscrub/Matthew.Pyle/gfs_input/gfs.${DATEgfs}


# export COMINnam=$COMROOTp2/nam/prod/nam.${DATE}
# export COMINnamold=$COMROOTp2/nam/prod/nam.${DATEgfs}

export COMINnam=/gpfs/hps3/emc/meso/noscrub/Matthew.Pyle/nam_input/nam.${DATE}
