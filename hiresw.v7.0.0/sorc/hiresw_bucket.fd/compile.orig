INCG2="-I/nwprod/lib/incmod/g2_4 -I/nwprod/lib/incmod/g2tmpl"

OPT="-convert big_endian -assume noold_ldout_format"

echo $LIBDEF

ifort $OPT -o WRFBUCKET.x WRFBUCKET.F $LIBDEF
